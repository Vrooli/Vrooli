      "id": "7829564732190847905",
      "publicId": "learn-plan-v1-0",
      "versionLabel": "1.0.0",
      "versionNotes": "Initial version - Personalized learning curriculum creator",
      "isComplete": true,
      "isPrivate": false,
      "versionIndex": 0,
      "isAutomatable": true,
      "resourceSubType": "RoutineMultiStep",
      "config": {
        "__version": "1.0",
        "graph": {
          "__type": "Sequential",
          "schema": {
            "steps": [
              {
                "id": "step-1",
                "name": "Assess Learning Goals",
                "description": "Clarifies objectives and skill requirements",
                "subroutineId": "7829564732190849608",
                "inputMap": {
                  "learningGoal": "targetSkill",
                  "currentLevel": "existingKnowledge",
                  "motivations": "learningDrivers"
                },
                "outputMap": {
                  "goalAnalysis": "skillBreakdown",
                  "competencyGaps": "learningNeeds",
                  "priorityAreas": "focusPoints"
                }
              },
              {
                "id": "step-2",
                "name": "Design Learning Path",
                "description": "Creates structured progression from basics to advanced",
                "subroutineId": "7829564732190849623",
                "inputMap": {
                  "skillBreakdown": "goalAnalysis",
                  "learningNeeds": "competencyGaps",
                  "pedagogicalApproach": "teachingStrategy"
                },
                "outputMap": {
                  "learningSequence": "progressionPath",
                  "milestoneStructure": "achievementLevels",
                  "dependencyMapping": "prerequisiteChain"
                }
              },
              {
                "id": "step-3",
                "name": "Curate Resources",
                "description": "Selects appropriate learning materials and tools",
                "subroutineId": "7829564732190849626",
                "inputMap": {
                  "progressionPath": "learningSequence",
                  "resourcePreferences": "materialTypes",
                  "accessibilityNeeds": "learningConstraints"
                },
                "outputMap": {
                  "resourceCollection": "learningMaterials",
                  "resourceTypes": "diversifiedContent",
                  "accessibilityMatching": "accommodatedResources"
                }
              },
              {
                "id": "step-4",
                "name": "Create Practice Framework",
                "description": "Designs exercises and application opportunities",
                "subroutineId": "7829564732190849581",
                "inputMap": {
                  "achievementLevels": "milestoneStructure",
                  "practicePreferences": "exerciseStyle",
                  "realWorldApplication": "practicalContext"
                },
                "outputMap": {
                  "practiceActivities": "skillExercises",
                  "applicationProjects": "realWorldTasks",
                  "assessmentMethods": "progressEvaluation"
                }
              },
              {
                "id": "step-5",
                "name": "Build Support System",
                "description": "Establishes community and mentorship connections",
                "subroutineId": "7829564732190849632",
                "inputMap": {
                  "learningCommunity": "socialSupport",
                  "mentorshipNeeds": "guidanceRequirements",
                  "accountabilityPreferences": "motivationalStructure"
                },
                "outputMap": {
                  "communityConnections": "learningNetwork",
                  "mentorshipPlan": "guidanceStrategy",
                  "accountabilityFramework": "motivationSystem"
                }
              },
              {
                "id": "step-6",
                "name": "Generate Complete Plan",
                "description": "Assembles comprehensive learning curriculum",
                "subroutineId": "7829564732190849635",
                "inputMap": {
                  "learningMaterials": "resourceCollection",
                  "skillExercises": "practiceActivities",
                  "timelineStructure": "schedulingFramework"
                },
                "outputMap": {
                  "learningCurriculum": "completePlan",
                  "implementationGuide": "executionInstructions",
                  "adaptationStrategy": "flexibilityFramework"
                }
              }
            ],
            "rootContext": {
              "inputMap": {
                "goal": "targetSkill",
                "background": "existingKnowledge",
                "preferences": "learningStyle",
                "constraints": "timeAndResources"
              },
              "outputMap": {
                "completePlan": "learningOutcome",
                "executionInstructions": "implementationGuide",
                "learningMaterials": "resourceList",
                "motivationSystem": "supportFramework"
              }
            },
            "executionMode": "sequential"
          }
        },
        "formInput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "goal",
                "id": "goal_input",
                "label": "Learning Goal",
                "type": "TextInput",
                "isRequired": true,
                "placeholder": "What skill or subject do you want to learn?"
              },
              {
                "fieldName": "skillLevel",
                "id": "skill_level_input",
                "label": "Skill Category",
                "type": "Selector",
                "isRequired": true,
                "options": [
                  "technical",
                  "creative",
                  "business",
                  "language",
                  "academic",
                  "personal",
                  "professional",
                  "hobby"
                ]
              },
              {
                "fieldName": "background",
                "id": "background_input",
                "label": "Current Knowledge",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "Your existing knowledge and experience in this area"
              },
              {
                "fieldName": "timeCommitment",
                "id": "time_commitment_input",
                "label": "Time Availability",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "How much time can you dedicate to learning?"
              },
              {
                "fieldName": "preferences",
                "id": "preferences_input",
                "label": "Learning Style",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Preferred learning methods and formats"
              },
              {
                "fieldName": "deadline",
                "id": "deadline_input",
                "label": "Target Timeline",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "flexible",
                "options": [
                  "1-month",
                  "3-months",
                  "6-months",
                  "1-year",
                  "flexible"
                ]
              },
              {
                "fieldName": "constraints",
                "id": "constraints_input",
                "label": "Learning Constraints",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Budget, location, technology, or other limitations"
              },
              {
                "fieldName": "practicalApplication",
                "id": "practical_application_input",
                "label": "Practical Application",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "How will you use this skill in real life?"
              }
            ]
          }
        },
        "formOutput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "learningOutcome",
                "id": "learning_outcome_output",
                "label": "Complete Learning Plan",
                "type": "JSON"
              },
              {
                "fieldName": "implementationGuide",
                "id": "implementation_guide_output",
                "label": "Step-by-Step Instructions",
                "type": "JSON"
              },
              {
                "fieldName": "resourceList",
                "id": "resource_list_output",
                "label": "Curated Learning Resources",
                "type": "JSON"
              },
              {
                "fieldName": "supportFramework",
                "id": "support_framework_output",
                "label": "Support & Motivation System",
                "type": "JSON"
              }
            ]
          }
        },
        "executionStrategy": "reasoning",
        "allowStrategyOverride": false,
        "subroutineStrategies": {
          "0": "reasoning",
          "1": "reasoning",
          "2": "reasoning",
          "3": "reasoning",
          "4": "conversational",
          "5": "conversational"
        }
      },
      "translations": [
        {
          "id": "7829564732190847906",
          "language": "en",
          "name": "Learning Plan Generator",
          "description": "Creates personalized learning curriculum for new skills or subjects. Assesses goals and knowledge gaps, designs learning paths, curates resources, creates practice frameworks, builds support systems, and generates comprehensive implementation plans.",
          "instructions": "Describe your learning goal and current knowledge level. Specify time availability and learning preferences. Include any constraints or practical applications. The routine will create a structured learning plan with resources, milestones, and support systems."
        }
      ]
    }
