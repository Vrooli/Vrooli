        "formInput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "destination",
                "id": "destination_input",
                "label": "Travel Destination",
                "type": "TextInput",
                "isRequired": true,
                "placeholder": "Where are you traveling to?"
              },
              {
                "fieldName": "dates",
                "id": "dates_input",
                "label": "Travel Dates",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "Start and end dates of your trip"
              },
              {
                "fieldName": "tripType",
                "id": "trip_type_input",
                "label": "Trip Type",
                "type": "Selector",
                "isRequired": true,
                "options": ["leisure-vacation", "business-travel", "adventure", "cultural-exploration", "family-trip", "romantic-getaway", "backpacking", "luxury-travel", "group-tour"]
              },
              {
                "fieldName": "budget",
                "id": "budget_input",
                "label": "Travel Budget",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "Total budget and spending preferences"
              },
              {
                "fieldName": "groupSize",
                "id": "group_size_input",
                "label": "Number of Travelers",
                "type": "Selector",
                "isRequired": true,
                "options": ["solo", "couple", "small-group", "family", "large-group"]
              },
              {
                "fieldName": "preferences",
                "id": "preferences_input",
                "label": "Travel Preferences",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Activities, pace, accommodation style, dietary needs"
              },
              {
                "fieldName": "activityLevel",
                "id": "activity_level_input",
                "label": "Activity Level",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "moderate",
                "options": ["relaxed", "moderate", "active", "very-active", "extreme"]
              },
              {
                "fieldName": "planningDetail",
                "id": "planning_detail_input",
                "label": "Planning Detail Level",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "balanced",
                "options": ["flexible-outline", "balanced", "detailed-schedule", "minute-by-minute"]
              }
            ]
          }
        },
