      "translations": [
        {
          "id": "7829564732190847822",
          "language": "en",
          "name": "Travel Itinerary Builder",
          "description": "Comprehensive travel planning system that analyzes requirements, researches destinations, plans transportation and accommodation, creates daily itineraries, adds practical details, and compiles complete travel packages with budgets and local insights.",
          "instructions": "Specify your destination and travel dates. Include trip type, budget, and group size. Add any preferences for activities and accommodation. The routine will research destinations, plan logistics, create daily schedules, and compile a complete travel package."
        }
