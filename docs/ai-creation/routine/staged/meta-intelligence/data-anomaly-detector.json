  "versions": [
    {
      "id": "7829564732190847920",
      "publicId": "anomaly-detect-v1-0",
      "versionLabel": "1.0.0",
      "versionNotes": "Initial version - Data anomaly detection and pattern analysis system",
      "isComplete": true,
      "isPrivate": false,
      "versionIndex": 0,
      "isAutomatable": true,
      "resourceSubType": "RoutineMultiStep",
      "config": {
        "__version": "1.0",
        "graph": {
          "__type": "Sequential",
          "schema": {
            "steps": [
              {
                "id": "step-1",
                "name": "Analyze Data Structure",
                "description": "Examines data format, types, and distribution patterns",
                "subroutineId": "7829564732190849206",
                "inputMap": {
                  "inputData": "rawDataset",
                  "dataDescription": "contextualInfo",
                  "analysisScope": "detectionParameters"
                },
                "outputMap": {
                  "dataProfile": "structuralAnalysis",
                  "baselinePatterns": "normalBehavior",
                  "variabilityMetrics": "expectedVariation"
                }
              },
              {
                "id": "step-2",
                "name": "Apply Statistical Detection",
                "description": "Uses statistical methods to identify outliers and unusual values",
                "subroutineId": "7829564732190849209",
                "inputMap": {
                  "rawDataset": "inputData",
                  "normalBehavior": "baselinePatterns",
                  "sensitivityLevel": "detectionThreshold"
                },
                "outputMap": {
                  "statisticalOutliers": "numericalAnomalies",
                  "confidenceScores": "anomalyReliability",
                  "distributionDeviations": "patternBreaks"
                }
              },
              {
                "id": "step-3",
                "name": "Perform Pattern Analysis",
                "description": "Identifies sequential or temporal anomalies in data patterns",
                "subroutineId": "7829564732190849212",
                "inputMap": {
                  "structuralAnalysis": "dataProfile",
                  "temporalSequence": "timeSeriesData",
                  "patternRules": "sequenceExpectations"
                },
                "outputMap": {
                  "sequentialAnomalies": "temporalOutliers",
                  "patternBreaks": "sequenceDeviations",
                  "trendsAnalysis": "behaviorChanges"
                }
              },
              {
                "id": "step-4",
                "name": "Contextual Evaluation",
                "description": "Assesses anomalies within business or domain context",
                "subroutineId": "7829564732190849215",
                "inputMap": {
                  "numericalAnomalies": "statisticalOutliers",
                  "temporalOutliers": "sequentialAnomalies",
                  "domainKnowledge": "businessContext"
                },
                "outputMap": {
                  "contextualAnomalies": "meaningfulOutliers",
                  "falsePositives": "likelyNormalEvents",
                  "severityRatings": "impactAssessment"
                }
              },
              {
                "id": "step-5",
                "name": "Generate Explanations",
                "description": "Provides potential explanations for detected anomalies",
                "subroutineId": "7829564732190849218",
                "inputMap": {
                  "meaningfulOutliers": "contextualAnomalies",
                  "impactAssessment": "severityRatings",
                  "historicalContext": "pastPatterns"
                },
                "outputMap": {
                  "anomalyExplanations": "rootCauseAnalysis",
                  "hypotheticalCauses": "possibleReasons",
                  "investigationSuggestions": "followUpActions"
                }
              },
              {
                "id": "step-6",
                "name": "Create Detection Report",
                "description": "Compiles comprehensive anomaly detection findings",
                "subroutineId": "7829564732190849221",
                "inputMap": {
                  "anomalyReliability": "confidenceScores",
                  "rootCauseAnalysis": "anomalyExplanations",
                  "reportFormat": "outputPreferences"
                },
                "outputMap": {
                  "detectionReport": "anomalyFindings",
                  "prioritizedAlerts": "actionableAnomalies",
                  "monitoringRecommendations": "ongoingWatchPoints"
                }
              }
            ],
            "rootContext": {
              "inputMap": {
                "data": "rawDataset",
                "context": "businessContext",
                "sensitivity": "detectionThreshold",
                "timeframe": "analysisWindow"
              },
              "outputMap": {
                "anomalyFindings": "detectionOutcome",
                "actionableAnomalies": "prioritizedResults",
                "ongoingWatchPoints": "monitoringPlan",
                "rootCauseAnalysis": "explanationReport"
              }
            },
            "executionMode": "sequential"
          }
        },
        "formInput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "data",
                "id": "data_input",
                "label": "Dataset",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "Time-series or categorical data to analyze for anomalies"
              },
              {
                "fieldName": "dataType",
                "id": "data_type_input",
                "label": "Data Type",
                "type": "Selector",
                "isRequired": true,
                "options": [
                  "time-series",
                  "categorical",
                  "numerical",
                  "mixed",
                  "log-data",
                  "financial",
                  "sensor-data",
                  "user-behavior"
                ]
              },
              {
                "fieldName": "context",
                "id": "context_input",
                "label": "Business Context",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Domain knowledge and expected patterns"
              },
              {
                "fieldName": "sensitivity",
                "id": "sensitivity_input",
                "label": "Detection Sensitivity",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "moderate",
                "options": [
                  "low",
                  "moderate",
                  "high",
                  "very-high"
                ]
              },
              {
                "fieldName": "timeframe",
                "id": "timeframe_input",
                "label": "Analysis Window",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "full-dataset",
                "options": [
                  "recent",
                  "full-dataset",
                  "rolling-window",
                  "specific-period"
                ]
              },
              {
                "fieldName": "anomalyTypes",
                "id": "anomaly_types_input",
                "label": "Anomaly Types to Detect",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Point anomalies, pattern breaks, trend changes, etc."
              },
              {
                "fieldName": "exclusions",
                "id": "exclusions_input",
                "label": "Known Exceptions",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Expected outliers or special events to ignore"
              },
              {
                "fieldName": "outputDetail",
                "id": "output_detail_input",
                "label": "Report Detail Level",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "standard",
                "options": [
                  "summary",
                  "standard",
                  "detailed",
                  "comprehensive"
                ]
              }
            ]
          }
        },
        "formOutput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "detectionOutcome",
                "id": "detection_outcome_output",
                "label": "Complete Anomaly Analysis",
                "type": "JSON"
              },
              {
                "fieldName": "prioritizedResults",
                "id": "prioritized_results_output",
                "label": "Actionable Anomalies",
                "type": "JSON"
              },
              {
                "fieldName": "monitoringPlan",
                "id": "monitoring_plan_output",
                "label": "Ongoing Monitoring Strategy",
                "type": "JSON"
              },
              {
                "fieldName": "explanationReport",
                "id": "explanation_report_output",
                "label": "Root Cause Analysis",
                "type": "JSON"
              }
            ]
          }
        },
        "executionStrategy": "deterministic",
        "allowStrategyOverride": false,
        "subroutineStrategies": {
          "0": "deterministic",
          "1": "deterministic",
          "2": "deterministic",
          "3": "reasoning",
          "4": "reasoning",
          "5": "conversational"
        }
      },
      "translations": [
        {
          "id": "7829564732190847921",
          "language": "en",
          "name": "Anomaly Detector",
          "description": "Comprehensive data anomaly detection and pattern analysis system. Analyzes data structure, applies statistical detection methods, performs pattern analysis, evaluates context, generates explanations, and creates detailed reports for unusual patterns or outliers.",
          "instructions": "Provide dataset and specify data type. Include business context and set detection sensitivity. Choose analysis timeframe and anomaly types. The routine will identify outliers, analyze patterns, and provide explanations with actionable recommendations."
        }
      ]
    }
  ]
