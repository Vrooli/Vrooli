                  "requiredCapabilities": "capabilityList",
                  "skillCategories": "categorizedSkills",
                  "prioritySkills": "criticalCapabilities"
                }
              },
              {
                "id": "step-2",
                "name": "Inventory Current Knowledge",
                "description": "Scans knowledge base to assess current capabilities",
                "subroutineId": "7829564732190849080",
                "inputMap": {
                  "knowledgeBase": "currentKnowledge",
                  "teamProfiles": "skillProfiles",
                  "requiredCapabilities": "capabilityList"
                },
                "outputMap": {
                  "existingCapabilities": "availableSkills",
                  "capabilityMatches": "matchingSkills",
                  "coverageMetrics": "skillCoverage"
                }
              },
              {
                "id": "step-3",
                "name": "Identify Gaps",
                "description": "Compares required vs existing capabilities to find gaps",
                "subroutineId": "7829564732190849083",
                "inputMap": {
                  "requiredCapabilities": "capabilityList",
                  "existingCapabilities": "availableSkills",
                  "prioritySkills": "criticalCapabilities"
                },
                "outputMap": {
                  "capabilityGaps": "identifiedGaps",
                  "gapSeverity": "severityScores",
                  "missingDomains": "knowledgeDomains"
                }
              },
              {
                "id": "step-4",
                "name": "Assess Impact and Risk",
                "description": "Evaluates the impact of gaps on task performance",
                "subroutineId": "7829564732190849086",
                "inputMap": {
                  "capabilityGaps": "identifiedGaps",
                  "upcomingTasks": "taskQueue",
                  "gapSeverity": "severityScores"
                },
                "outputMap": {
                  "impactAssessment": "taskImpacts",
                  "riskLevels": "performanceRisks",
                  "criticalGaps": "urgentNeeds"
                }
              },
              {
                "id": "step-5",
                "name": "Generate Remediation Plan",
                "description": "Creates plan to acquire missing capabilities",
                "subroutineId": "7829564732190849089",
                "inputMap": {
                  "capabilityGaps": "identifiedGaps",
                  "criticalGaps": "urgentNeeds",
                  "impactAssessment": "taskImpacts",
                  "missingDomains": "knowledgeDomains"
                },
                "outputMap": {
                  "remediationPlan": "acquisitionPlan",
                  "learningPriorities": "prioritizedLearning",
                  "resourceNeeds": "requiredResources"
                }
              }
            ],
