                "outputMap": {
                  "taskComplexityScores": "complexityAnalysis",
                  "frequencyAssessment": "taskFrequency",
                  "skillRequirements": "requiredSkills",
                  "timeInvestments": "timeAnalysis"
                }
              },
              {
                "id": "step-2",
                "name": "Assess Delegation Potential",
                "description": "Evaluates which tasks are suitable for delegation",
                "subroutineId": "delegation-assessor-v1",
                "inputMap": {
                  "complexityAnalysis": "taskComplexity",
                  "taskFrequency": "frequencyData",
                  "requiredSkills": "skillNeeds",
                  "teamCapabilities": "availableSkills"
                },
                "outputMap": {
                  "delegationCandidates": "delegatableTasks",
                  "skillMatching": "skillAlignment",
                  "trainingNeeds": "developmentRequired",
                  "delegationRisks": "riskAssessment"
                }
              },
              {
                "id": "step-3",
                "name": "Evaluate Automation Opportunities",
                "description": "Identifies tasks suitable for automation",
                "subroutineId": "automation-evaluator-v1",
                "inputMap": {
                  "taskComplexity": "complexityScores",
                  "frequencyData": "taskFrequency",
                  "timeAnalysis": "timeInvestment",
                  "automationOptions": "availableTools"
                },
                "outputMap": {
                  "automationCandidates": "automatableTasks",
                  "toolRecommendations": "suggestedTools",
                  "implementationEffort": "effortEstimates",
                  "automationROI": "returnOnInvestment"
                }
              },
              {
                "id": "step-4",
                "name": "Generate Action Plan",
                "description": "Creates prioritized recommendations for delegation and automation",
                "subroutineId": "action-planner-v1",
                "inputMap": {
                  "delegatableTasks": "delegationOptions",
                  "skillAlignment": "teamMatching",
                  "automatableTasks": "automationOptions",
                  "returnOnInvestment": "roiAnalysis"
                },
                "outputMap": {
                  "prioritizedRecommendations": "actionPlan",
                  "delegationStrategy": "delegationPlan",
                  "automationRoadmap": "automationPlan",
                  "implementationTimeline": "executionSchedule"
                }
              }
            ],
            "rootContext": {
              "inputMap": {
                "taskList": "originalTaskList",
                "teamCapabilities": "originalTeamCapabilities",
                "automationOptions": "originalAutomationOptions"
              },
              "outputMap": {
