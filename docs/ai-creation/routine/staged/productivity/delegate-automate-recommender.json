              {
                "id": "step-1",
                "name": "Analyze Task Characteristics",
                "description": "Evaluates task complexity, frequency, and skill requirements",
                "subroutineId": "task-analyzer-v1",
                "inputMap": {
                  "originalTaskList": "taskList",
                  "originalTeamCapabilities": "teamCapabilities",
                  "originalAutomationOptions": "automationOptions"
