              },
              {
                "id": "step-4",
                "name": "Extract Lessons Learned",
                "description": "Synthesizes insights from the week's experiences",
                "subroutineId": "7829564732190848642",
                "inputMap": {
                  "successPatterns": "whatWorked",
                  "challenges": "identifiedChallenges",
                  "reflectionNotes": "userReflections"
                },
                "outputMap": {
                  "lessonsLearned": "weeklyInsights",
                  "behaviorPatterns": "recurringThemes",
                  "actionableInsights": "improvementIdeas"
                }
              },
              {
                "id": "step-5",
                "name": "Plan Next Week",
                "description": "Uses insights to plan the upcoming week",
                "subroutineId": "7829564732190847574",
                "inputMap": {
                  "taskList": "upcomingTasks",
                  "importance": "taskPriorities",
                  "deadlines": "taskDeadlines"
                },
                "outputMap": {
                  "prioritizedTasks": "nextWeekPriorities",
                  "criticalPath": "focusAreas"
                }
              },
