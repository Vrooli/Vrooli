                "description": "Analyzes what didn't work and why",
                "subroutineId": "7829564732190848372",
                "inputMap": {
                  "pendingItems": "unfinishedTasks",
                  "timeAnalysis": "timeSpentBreakdown",
                  "reflectionNotes": "userReflections"
