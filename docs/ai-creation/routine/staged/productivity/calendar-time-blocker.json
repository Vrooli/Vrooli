  "versions": [
    {
      "id": "7829564732190847938",
      "publicId": "time-block-v1-0",
      "versionLabel": "1.0.0",
      "versionNotes": "Initial version - Time blocking and calendar scheduling system",
      "isComplete": true,
      "isPrivate": false,
      "versionIndex": 0,
      "isAutomatable": true,
      "resourceSubType": "RoutineMultiStep",
      "config": {
        "__version": "1.0",
        "graph": {
          "__type": "Sequential",
          "schema": {
            "steps": [
              {
                "id": "step-1",
                "name": "Analyze Available Time",
                "description": "Examines calendar and identifies available time slots for scheduling",
                "subroutineId": "7829564732190850103",
                "inputMap": {
                  "calendarData": "existingSchedule",
                  "timePreferences": "schedulingConstraints",
                  "workingHours": "availabilityWindow"
                },
                "outputMap": {
                  "availableSlots": "freeTimeBlocks",
                  "timeConstraints": "schedulingLimitations",
                  "optimalPeriods": "productivityWindows"
                }
              },
              {
                "id": "step-2",
                "name": "Prioritize Tasks",
                "description": "Ranks tasks by importance, urgency, and effort requirements",
                "subroutineId": "7829564732190850106",
                "inputMap": {
                  "taskList": "schedulingRequests",
                  "priorityFactors": "importanceCriteria",
                  "deadlineConstraints": "timeRequirements"
                },
                "outputMap": {
                  "prioritizedTasks": "rankedActivities",
                  "effortEstimates": "timeAllocations",
                  "urgencyLevels": "priorityScores"
                }
              },
              {
                "id": "step-3",
                "name": "Match Tasks to Time Blocks",
                "description": "Assigns tasks to optimal time slots based on duration and energy needs",
                "subroutineId": "7829564732190850109",
                "inputMap": {
                  "freeTimeBlocks": "availableSlots",
                  "rankedActivities": "prioritizedTasks",
                  "energyPatterns": "productivityCurves"
                },
                "outputMap": {
                  "taskAssignments": "scheduledBlocks",
                  "conflictResolution": "overlappingItems",
                  "bufferAllocation": "transitionTimes"
                }
              },
              {
                "id": "step-4",
                "name": "Optimize Schedule Flow",
                "description": "Arranges time blocks for maximum efficiency and minimal context switching",
                "subroutineId": "7829564732190850112",
                "inputMap": {
                  "scheduledBlocks": "taskAssignments",
                  "transitionTimes": "bufferAllocation",
                  "efficiencyRules": "optimizationCriteria"
                },
                "outputMap": {
                  "optimizedSchedule": "efficientArrangement",
                  "groupedActivities": "batchedTasks",
                  "flowImprovements": "schedulingEnhancements"
                }
              },
              {
                "id": "step-5",
                "name": "Handle Conflicts",
                "description": "Resolves scheduling conflicts and overlapping commitments",
                "subroutineId": "7829564732190850115",
                "inputMap": {
                  "overlappingItems": "conflictResolution",
                  "efficientArrangement": "optimizedSchedule",
                  "resolutionPreferences": "conflictHandling"
                },
                "outputMap": {
                  "resolvedConflicts": "cleanSchedule",
                  "alternativeOptions": "reschedulingChoices",
                  "warningFlags": "potentialIssues"
                }
              },
              {
                "id": "step-6",
                "name": "Generate Final Schedule",
                "description": "Creates completed time-blocked calendar with all assignments",
                "subroutineId": "7829564732190850118",
                "inputMap": {
                  "cleanSchedule": "resolvedConflicts",
                  "reschedulingChoices": "alternativeOptions",
                  "outputFormat": "calendarFormat"
                },
                "outputMap": {
                  "finalSchedule": "timeBlockedCalendar",
                  "scheduleMetrics": "utilizationStatistics",
                  "implementationTips": "executionGuidance"
                }
              }
            ],
            "rootContext": {
              "inputMap": {
                "tasks": "schedulingRequests",
                "calendar": "existingSchedule",
                "preferences": "schedulingPreferences",
                "timeframe": "schedulingWindow"
              },
              "outputMap": {
                "timeBlockedCalendar": "schedulingOutcome",
                "utilizationStatistics": "efficiencyReport",
                "executionGuidance": "implementationAdvice",
                "efficientArrangement": "optimizedPlan"
              }
            },
            "executionMode": "sequential"
          }
        },
        "formInput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "tasks",
                "id": "tasks_input",
                "label": "Tasks to Schedule",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "List of tasks with durations and priorities"
              },
              {
                "fieldName": "calendar",
                "id": "calendar_input",
                "label": "Existing Calendar",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Current appointments and commitments"
              },
              {
                "fieldName": "timeframe",
                "id": "timeframe_input",
                "label": "Scheduling Timeframe",
                "type": "Selector",
                "isRequired": true,
                "options": [
                  "today",
                  "tomorrow",
                  "this-week",
                  "next-week",
                  "specific-date"
                ]
              },
              {
                "fieldName": "workingHours",
                "id": "working_hours_input",
                "label": "Working Hours",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Start and end times for each day"
              },
              {
                "fieldName": "preferences",
                "id": "preferences_input",
                "label": "Scheduling Preferences",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Break times, buffer preferences, energy patterns"
              },
              {
                "fieldName": "priorityMethod",
                "id": "priority_method_input",
                "label": "Priority Method",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "eisenhower",
                "options": [
                  "importance",
                  "deadline",
                  "eisenhower",
                  "effort",
                  "custom"
                ]
              },
              {
                "fieldName": "blockSize",
                "id": "block_size_input",
                "label": "Minimum Block Size",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "30-minutes",
                "options": [
                  "15-minutes",
                  "30-minutes",
                  "1-hour",
                  "2-hours",
                  "flexible"
                ]
              },
              {
                "fieldName": "bufferTime",
                "id": "buffer_time_input",
                "label": "Buffer Between Tasks",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "15-minutes",
                "options": [
                  "none",
                  "5-minutes",
                  "15-minutes",
                  "30-minutes",
                  "auto"
                ]
              }
            ]
          }
        },
        "formOutput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "schedulingOutcome",
                "id": "scheduling_outcome_output",
                "label": "Complete Time-Blocked Schedule",
                "type": "JSON"
              },
              {
                "fieldName": "efficiencyReport",
                "id": "efficiency_report_output",
                "label": "Schedule Efficiency Analysis",
                "type": "JSON"
              },
              {
                "fieldName": "implementationAdvice",
                "id": "implementation_advice_output",
                "label": "Execution Tips & Guidance",
                "type": "JSON"
              },
              {
                "fieldName": "optimizedPlan",
                "id": "optimized_plan_output",
                "label": "Optimized Schedule Layout",
                "type": "JSON"
              }
            ]
          }
        },
        "executionStrategy": "deterministic",
        "allowStrategyOverride": false,
        "subroutineStrategies": {
          "0": "deterministic",
          "1": "deterministic",
          "2": "deterministic",
          "3": "deterministic",
          "4": "reasoning",
          "5": "conversational"
        }
      },
      "translations": [
        {
          "id": "7829564732190847939",
          "language": "en",
          "name": "Time Block Scheduler",
          "description": "Comprehensive time blocking and calendar scheduling system. Analyzes available time, prioritizes tasks, matches tasks to optimal time blocks, optimizes schedule flow, handles conflicts, and generates efficient time-blocked calendars.",
          "instructions": "Provide tasks to schedule and existing calendar. Set timeframe and working hours. Include scheduling preferences and priority method. The routine will create an optimized time-blocked schedule with efficiency analysis and implementation guidance."
        }
      ]
    }
  ]
