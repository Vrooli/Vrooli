  "versions": [
    {
      "id": "7829564732190847884",
      "publicId": "priority-matrix-v1-0",
      "versionLabel": "1.0.0",
      "versionNotes": "Initial version - Eisenhower Matrix task prioritization system",
      "isComplete": true,
      "isPrivate": false,
      "versionIndex": 0,
      "isAutomatable": true,
      "resourceSubType": "RoutineMultiStep",
      "config": {
        "__version": "1.0",
        "graph": {
          "__type": "Sequential",
          "schema": {
            "steps": [
              {
                "id": "step-1",
                "name": "Assess Task Urgency",
                "description": "Evaluates time sensitivity of each task",
                "subroutineId": "7829564732190850265",
                "inputMap": {
                  "taskList": "inputTasks",
                  "deadlines": "dueDates",
                  "urgencyCriteria": "timeFactors"
                },
                "outputMap": {
                  "urgencyScores": "taskUrgency",
                  "deadlineMap": "timeConstraints",
                  "urgencyRationale": "urgencyExplanation"
                }
              },
              {
                "id": "step-2",
                "name": "Evaluate Task Importance",
                "description": "Determines impact and strategic value",
                "subroutineId": "7829564732190850268",
                "inputMap": {
                  "inputTasks": "taskList",
                  "goals": "strategicObjectives",
                  "impactCriteria": "valueFactors"
                },
                "outputMap": {
                  "importanceScores": "taskImportance",
                  "impactAnalysis": "valueAssessment",
                  "importanceRationale": "importanceExplanation"
                }
              },
              {
                "id": "step-3",
                "name": "Categorize into Quadrants",
                "description": "Places tasks in urgent/important matrix",
                "subroutineId": "7829564732190850271",
                "inputMap": {
                  "taskUrgency": "urgencyScores",
                  "taskImportance": "importanceScores",
                  "thresholds": "quadrantBoundaries"
                },
                "outputMap": {
                  "quadrant1": "doFirst",
                  "quadrant2": "schedule",
                  "quadrant3": "delegate",
                  "quadrant4": "eliminate"
                }
              },
              {
                "id": "step-4",
                "name": "Generate Action Plans",
                "description": "Creates specific recommendations per quadrant",
                "subroutineId": "7829564732190850274",
                "inputMap": {
                  "doFirst": "quadrant1",
                  "schedule": "quadrant2",
                  "delegationOptions": "availableResources"
                },
                "outputMap": {
                  "immediateActions": "urgentImportantPlan",
                  "schedulingPlan": "importantNotUrgentPlan",
                  "delegationPlan": "urgentNotImportantPlan"
                }
              },
              {
                "id": "step-5",
                "name": "Optimize Workload",
                "description": "Balances tasks across time and resources",
                "subroutineId": "7829564732190850277",
                "inputMap": {
                  "urgentImportantPlan": "immediateActions",
                  "availableTime": "timeCapacity",
                  "energyPatterns": "productivityCycles"
                },
                "outputMap": {
                  "optimizedSchedule": "balancedWorkload",
                  "timeAllocation": "taskTimeBlocks",
                  "workloadMetrics": "capacityAnalysis"
                }
              },
              {
                "id": "step-6",
                "name": "Create Implementation Guide",
                "description": "Produces actionable priority report",
                "subroutineId": "7829564732190850280",
                "inputMap": {
                  "balancedWorkload": "optimizedSchedule",
                  "quadrantExplanations": "categorizationRationale",
                  "visualPreferences": "displayFormat"
                },
                "outputMap": {
                  "priorityReport": "completeMatrix",
                  "actionSteps": "implementationPlan",
                  "visualMatrix": "matrixDiagram"
                }
              }
            ],
            "rootContext": {
              "inputMap": {
                "tasks": "inputTasks",
                "criteria": "prioritizationFactors",
                "goals": "objectiveAlignment",
                "resources": "availableCapacity"
              },
              "outputMap": {
                "completeMatrix": "priorityOutcome",
                "implementationPlan": "actionGuidance",
                "matrixDiagram": "visualPriorities",
                "capacityAnalysis": "workloadInsights"
              }
            },
            "executionMode": "sequential"
          }
        },
        "formInput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "tasks",
                "id": "tasks_input",
                "label": "Task List",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "List of tasks to prioritize"
              },
              {
                "fieldName": "taskContext",
                "id": "task_context_input",
                "label": "Task Context",
                "type": "Selector",
                "isRequired": true,
                "options": [
                  "work",
                  "personal",
                  "academic",
                  "project",
                  "mixed"
                ]
              },
              {
                "fieldName": "criteria",
                "id": "criteria_input",
                "label": "Prioritization Criteria",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Custom urgency and importance factors"
              },
              {
                "fieldName": "goals",
                "id": "goals_input",
                "label": "Strategic Goals",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Objectives for importance alignment"
              },
              {
                "fieldName": "timeHorizon",
                "id": "time_horizon_input",
                "label": "Planning Horizon",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "week",
                "options": [
                  "day",
                  "week",
                  "month",
                  "quarter",
                  "year"
                ]
              },
              {
                "fieldName": "resources",
                "id": "resources_input",
                "label": "Available Resources",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Time, people, or tools available"
              },
              {
                "fieldName": "strictness",
                "id": "strictness_input",
                "label": "Matrix Strictness",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "balanced",
                "options": [
                  "flexible",
                  "balanced",
                  "strict",
                  "ruthless"
                ]
              },
              {
                "fieldName": "includeRationale",
                "id": "include_rationale_input",
                "label": "Include Explanations",
                "type": "Switch",
                "isRequired": false,
                "defaultValue": true
              }
            ]
          }
        },
        "formOutput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "priorityOutcome",
                "id": "priority_outcome_output",
                "label": "Complete Priority Matrix",
                "type": "JSON"
              },
              {
                "fieldName": "actionGuidance",
                "id": "action_guidance_output",
                "label": "Implementation Actions",
                "type": "JSON"
              },
              {
                "fieldName": "visualPriorities",
                "id": "visual_priorities_output",
                "label": "Visual Matrix Diagram",
                "type": "JSON"
              },
              {
                "fieldName": "workloadInsights",
                "id": "workload_insights_output",
                "label": "Capacity Analysis",
                "type": "JSON"
              }
            ]
          }
        },
        "executionStrategy": "deterministic",
        "allowStrategyOverride": false,
        "subroutineStrategies": {
          "0": "deterministic",
          "1": "reasoning",
          "2": "deterministic",
          "3": "reasoning",
          "4": "deterministic",
          "5": "conversational"
        }
      },
      "translations": [
        {
          "id": "7829564732190847885",
          "language": "en",
          "name": "Priority Matrix (Eisenhower Assistant)",
          "description": "Sorts tasks using the urgent/important matrix for better prioritization. Assesses urgency and importance, categorizes into quadrants (Do First, Schedule, Delegate, Eliminate), and creates actionable implementation plans.",
          "instructions": "Provide your task list with any deadlines. Include strategic goals for importance assessment. Specify available resources and time horizon. The routine will categorize tasks by priority and provide specific action recommendations."
        }
      ]
    }
  ]
