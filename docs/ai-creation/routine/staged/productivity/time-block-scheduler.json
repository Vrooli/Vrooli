      "translations": [
        {
          "id": "7829564732190847864",
          "language": "en",
          "name": "Time Block Scheduler",
          "description": "Allocates time slots for tasks in calendar format. Analyzes available time, prioritizes tasks, estimates durations, optimizes allocation, adds breaks and transitions, and generates calendar-ready schedules with realistic time management.",
          "instructions": "Provide task list with priorities and existing calendar. Specify working hours and scheduling preferences. Choose optimization goal and break duration. The routine will create an optimized time-blocked schedule respecting all constraints."
        }
      ]
