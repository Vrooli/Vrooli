      "publicId": "time-block-v1-0",
      "versionLabel": "1.0.0",
      "versionNotes": "Initial version - Time slot allocation for calendar scheduling",
      "isComplete": true,
      "isPrivate": false,
      "versionIndex": 0,
      "isAutomatable": true,
      "resourceSubType": "RoutineMultiStep",
      "config": {
        "__version": "1.0",
        "graph": {
          "__type": "Sequential",
          "schema": {
            "steps": [
              {
                "id": "step-1",
                "name": "Analyze Available Time",
                "description": "Identifies open time slots in calendar",
                "subroutineId": "TODO: Need time analysis subroutine",
                "inputMap": {
                  "calendarData": "existingSchedule",
                  "workingHours": "availabilityWindow",
                  "blockedTime": "unavailablePeriods"
                },
                "outputMap": {
                  "availableSlots": "openTimeBlocks",
                  "totalAvailableTime": "schedulableHours",
                  "constraintMap": "timeRestrictions"
                }
              },
              {
                "id": "step-2",
                "name": "Prioritize Tasks",
                "description": "Orders tasks by importance and urgency",
                "subroutineId": "TODO: Need task prioritization subroutine",
                "inputMap": {
                  "taskList": "pendingTasks",
                  "deadlines": "dueDates",
                  "priorities": "importanceScores"
                },
                "outputMap": {
                  "prioritizedTasks": "orderedTaskList",
                  "criticalPath": "mustDoTasks",
                  "flexibleTasks": "optionalWork"
                }
              },
              {
                "id": "step-3",
                "name": "Estimate Task Durations",
                "description": "Calculates time needed for each task",
                "subroutineId": "TODO: Need duration estimation subroutine",
                "inputMap": {
                  "orderedTaskList": "prioritizedTasks",
                  "historicalData": "pastDurations",
                  "complexityFactors": "taskDifficulty"
                },
                "outputMap": {
                  "taskDurations": "timeEstimates",
                  "bufferTime": "paddingRequirements",
                  "totalTimeNeeded": "aggregateDuration"
                }
              },
              {
                "id": "step-4",
                "name": "Optimize Time Allocation",
                "description": "Assigns tasks to optimal time slots",
                "subroutineId": "TODO: Need allocation optimization subroutine",
                "inputMap": {
                  "openTimeBlocks": "availableSlots",
                  "timeEstimates": "taskDurations",
                  "energyPatterns": "productivityCurve"
                },
                "outputMap": {
                  "optimalSchedule": "timeAllocations",
                  "efficiencyScore": "utilizationRate",
                  "conflictResolutions": "scheduleAdjustments"
                }
              },
              {
                "id": "step-5",
                "name": "Add Breaks & Transitions",
                "description": "Inserts rest periods and transition time",
                "subroutineId": "TODO: Need break scheduling subroutine",
                "inputMap": {
                  "timeAllocations": "optimalSchedule",
                  "breakPreferences": "restRequirements",
                  "transitionNeeds": "switchingTime"
                },
                "outputMap": {
                  "finalSchedule": "completeTimeBlocks",
                  "breakSchedule": "restPeriods",
                  "transitionBuffers": "switchTime"
                }
              },
              {
                "id": "step-6",
                "name": "Generate Calendar Format",
                "description": "Creates calendar-ready schedule with details",
                "subroutineId": "TODO: Need calendar formatting subroutine",
                "inputMap": {
                  "completeTimeBlocks": "finalSchedule",
                  "taskDetails": "workDescriptions",
                  "formatPreferences": "calendarStyle"
                },
                "outputMap": {
                  "calendarSchedule": "formattedCalendar",
                  "dailyAgenda": "structuredPlan",
                  "reminders": "notificationSchedule"
                }
              }
            ],
            "rootContext": {
              "inputMap": {
                "tasks": "pendingTasks",
                "calendar": "existingSchedule",
                "preferences": "schedulingPreferences",
                "constraints": "timeConstraints"
              },
              "outputMap": {
                "formattedCalendar": "scheduleOutcome",
                "structuredPlan": "timeBlockPlan",
                "utilizationRate": "efficiencyMetrics",
                "notificationSchedule": "reminderSystem"
              }
            },
            "executionMode": "sequential"
          }
        },
        "formInput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "tasks",
                "id": "tasks_input",
                "label": "Task List",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "Tasks with durations, priorities, and deadlines"
              },
              {
                "fieldName": "calendar",
                "id": "calendar_input",
                "label": "Existing Calendar",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "Current appointments and blocked time"
              },
              {
                "fieldName": "scheduleType",
                "id": "schedule_type_input",
                "label": "Schedule Type",
                "type": "Selector",
                "isRequired": true,
                "options": [
                  "daily",
                  "weekly",
                  "sprint",
                  "project",
                  "exam-prep",
                  "custom"
                ]
              },
              {
                "fieldName": "workingHours",
                "id": "working_hours_input",
                "label": "Working Hours",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Start and end times for work periods"
              },
              {
                "fieldName": "preferences",
                "id": "preferences_input",
                "label": "Scheduling Preferences",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Break frequency, focus time, energy patterns"
              },
              {
                "fieldName": "constraints",
                "id": "constraints_input",
                "label": "Time Constraints",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Fixed commitments, no-work periods"
              },
              {
                "fieldName": "optimizeFor",
                "id": "optimize_for_input",
                "label": "Optimization Goal",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "balance",
                "options": [
                  "productivity",
                  "balance",
                  "deadlines",
                  "energy",
                  "minimal-switching"
                ]
              },
              {
                "fieldName": "breakDuration",
                "id": "break_duration_input",
                "label": "Break Duration",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "15-min",
                "options": [
                  "5-min",
                  "10-min",
                  "15-min",
                  "30-min",
                  "flexible"
                ]
              }
            ]
          }
        },
        "formOutput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "scheduleOutcome",
                "id": "schedule_outcome_output",
                "label": "Complete Time Block Schedule",
                "type": "JSON"
              },
              {
                "fieldName": "timeBlockPlan",
                "id": "time_block_plan_output",
                "label": "Structured Daily Plan",
                "type": "JSON"
              },
              {
                "fieldName": "efficiencyMetrics",
                "id": "efficiency_metrics_output",
                "label": "Schedule Efficiency Analysis",
                "type": "JSON"
              },
              {
                "fieldName": "reminderSystem",
                "id": "reminder_system_output",
                "label": "Task Reminders & Notifications",
                "type": "JSON"
              }
            ]
          }
        },
        "executionStrategy": "deterministic",
        "allowStrategyOverride": false,
        "subroutineStrategies": {
