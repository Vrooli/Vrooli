      "config": {
        "__version": "1.0",
        "graph": {
          "__type": "Sequential",
          "schema": {
            "steps": [
              {
                "id": "step-1",
                "name": "Define Presentation Goals",
                "description": "Establishes objectives, audience, and key messages",
                "subroutineId": "TODO: Need goal definition subroutine",
                "inputMap": {
                  "presentationPurpose": "objectiveDefinition",
                  "audienceProfile": "targetViewers",
                  "desiredOutcomes": "successMetrics"
                },
                "outputMap": {
                  "presentationStrategy": "communicationPlan",
                  "coreMessages": "keyTakeaways",
                  "audienceAnalysis": "viewerInsights"
                }
              },
              {
                "id": "step-2",
                "name": "Structure Content Flow",
                "description": "Creates logical narrative and slide organization",
                "subroutineId": "TODO: Need content structuring subroutine",
                "inputMap": {
                  "keyTakeaways": "coreMessages",
                  "storyArc": "narrativeStructure",
                  "timingConstraints": "presentationLength"
                },
                "outputMap": {
                  "slideOutline": "presentationStructure",
                  "contentFlow": "narrativeSequence",
                  "sectionBreakdown": "topicOrganization"
                }
              },
              {
                "id": "step-3",
                "name": "Design Visual Theme",
                "description": "Develops consistent visual style and branding",
                "subroutineId": "TODO: Need design theme subroutine",
                "inputMap": {
                  "brandGuidelines": "visualIdentity",
                  "audiencePreferences": "designExpectations",
                  "presentationContext": "environmentFactors"
                },
                "outputMap": {
                  "visualTheme": "designSystem",
                  "colorPalette": "brandColors",
                  "typographyGuide": "fontChoices"
                }
              },
              {
                "id": "step-4",
                "name": "Create Slide Content",
                "description": "Develops text, visuals, and data for each slide",
                "subroutineId": "TODO: Need slide creation subroutine",
                "inputMap": {
                  "presentationStructure": "slideOutline",
                  "designSystem": "visualTheme",
                  "contentDetails": "slideInformation"
                },
                "outputMap": {
                  "slideContent": "completedSlides",
                  "visualElements": "graphicsCharts",
                  "speakerNotes": "presentationScript"
                }
              },
              {
                "id": "step-5",
                "name": "Add Engagement Elements",
                "description": "Incorporates interactive and memorable components",
                "subroutineId": "TODO: Need engagement design subroutine",
                "inputMap": {
                  "viewerInsights": "audienceAnalysis",
                  "interactionPoints": "engagementOpportunities",
                  "memorabilityFactors": "retentionStrategies"
                },
                "outputMap": {
                  "interactiveElements": "audienceEngagement",
                  "visualHooks": "attentionGrabbers",
                  "callToAction": "responseDrivers"
                }
              },
              {
                "id": "step-6",
                "name": "Polish & Prepare Delivery",
                "description": "Finalizes presentation and prepares speaker materials",
                "subroutineId": "TODO: Need delivery preparation subroutine",
                "inputMap": {
                  "completedSlides": "slideContent",
                  "presentationScript": "speakerNotes",
                  "rehearsalNeeds": "practiceRequirements"
                },
                "outputMap": {
                  "finalPresentation": "polishedDeck",
                  "deliveryGuide": "speakerPreparation",
                  "backupMaterials": "contingencyAssets"
                }
              }
            ],
            "rootContext": {
              "inputMap": {
                "topic": "presentationSubject",
                "audience": "targetViewers",
                "purpose": "objectiveDefinition",
                "constraints": "presentationRequirements"
