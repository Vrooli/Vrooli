              "outputMap": {
                "polishedDeck": "presentationOutcome",
                "speakerPreparation": "deliveryMaterials",
                "completedSlides": "slidePresentation",
                "audienceEngagement": "interactiveComponents"
              }
            },
            "executionMode": "sequential"
          }
        },
        "formInput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "topic",
                "id": "topic_input",
                "label": "Presentation Topic",
                "type": "TextInput",
                "isRequired": true,
                "placeholder": "What is your presentation about?"
              },
              {
                "fieldName": "audience",
                "id": "audience_input",
                "label": "Target Audience",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "Who will be viewing this presentation? Their background and expectations"
              },
              {
                "fieldName": "presentationType",
                "id": "presentation_type_input",
                "label": "Presentation Type",
                "type": "Selector",
                "isRequired": true,
                "options": [
                  "sales-pitch",
                  "educational",
                  "conference-talk",
                  "board-meeting",
                  "investor-pitch",
                  "team-update",
                  "workshop",
                  "keynote",
                  "webinar"
                ]
              },
              {
                "fieldName": "purpose",
                "id": "purpose_input",
                "label": "Presentation Goals",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "What do you want to achieve with this presentation?"
              },
              {
                "fieldName": "duration",
                "id": "duration_input",
                "label": "Presentation Length",
                "type": "Selector",
                "isRequired": true,
                "options": [
                  "5-minutes",
                  "10-minutes",
                  "20-minutes",
                  "30-minutes",
                  "45-minutes",
                  "60-minutes",
                  "90-minutes"
                ]
              },
              {
                "fieldName": "constraints",
                "id": "constraints_input",
                "label": "Requirements & Constraints",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Brand guidelines, technical requirements, content restrictions"
              },
              {
                "fieldName": "visualStyle",
                "id": "visual_style_input",
                "label": "Visual Style Preference",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "professional",
                "options": [
                  "minimalist",
                  "professional",
                  "creative",
                  "data-heavy",
                  "visual-storytelling",
                  "corporate",
                  "modern",
                  "playful"
                ]
              },
              {
                "fieldName": "interactivity",
                "id": "interactivity_input",
                "label": "Interactivity Level",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "moderate",
                "options": [
                  "none",
                  "minimal",
                  "moderate",
                  "high",
                  "workshop-style"
                ]
              }
            ]
          }
        },
        "formOutput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
