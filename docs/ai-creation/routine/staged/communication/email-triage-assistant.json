                "outputMap": {
                  "priorityRating": "importanceLevel",
                  "urgencyScore": "timelineRequirement",
                  "stakeholderImpact": "audienceSignificance"
                }
              },
              {
                "id": "step-3",
                "name": "Categorize by Type",
                "description": "Classifies emails into actionable categories",
                "subroutineId": "TODO: Need email categorization subroutine",
                "inputMap": {
                  "communicationPurpose": "intentClassification",
                  "importanceLevel": "priorityRating",
                  "categorizationRules": "classificationLogic"
                },
                "outputMap": {
                  "emailCategory": "messageType",
                  "actionRequired": "responseNeeds",
                  "handlingApproach": "processingMethod"
                }
              },
              {
                "id": "step-4",
                "name": "Generate Response Strategy",
                "description": "Creates appropriate response approach for each email",
                "subroutineId": "TODO: Need response strategy subroutine",
                "inputMap": {
                  "messageType": "emailCategory",
                  "responseNeeds": "actionRequired",
                  "communicationStyle": "responsePreferences"
                },
                "outputMap": {
                  "responseStrategy": "replyApproach",
                  "responseTemplate": "draftStructure",
                  "timingRecommendation": "schedulingSuggestion"
                }
              },
              {
                "id": "step-5",
                "name": "Create Action Plan",
                "description": "Develops specific steps for handling each email",
                "subroutineId": "TODO: Need action planning subroutine",
                "inputMap": {
                  "replyApproach": "responseStrategy",
                  "timelineRequirement": "urgencyScore",
                  "workflowIntegration": "systemConnections"
                },
                "outputMap": {
                  "actionItems": "taskList",
                  "delegationOptions": "assignmentPossibilities",
                  "followUpSchedule": "trackingPlan"
                }
              },
              {
                "id": "step-6",
                "name": "Generate Triage Summary",
                "description": "Compiles comprehensive email management recommendations",
                "subroutineId": "TODO: Need summary generation subroutine",
                "inputMap": {
                  "taskList": "actionItems",
                  "draftStructure": "responseTemplate",
                  "outputFormat": "presentationStyle"
                },
                "outputMap": {
                  "triageSummary": "managementPlan",
                  "prioritizedQueue": "organizedEmails",
                  "efficiencyTips": "productivityGuidance"
                }
              }
