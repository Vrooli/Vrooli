{
  "id": "7829564732190847583",
  "publicId": "web-research-v1",
  "resourceType": "Routine",
  "isPrivate": false,
  "permissions": "{}",
  "isInternal": false,
  "tags": [
    {
      "id": "7829564732190847091",
      "tag": "automation",
      "translations": [
        {
          "id": "7829564732190847191",
          "language": "en",
          "description": "Process automation"
        }
      ]
    }
  ],
  "versions": [
    {
      "id": "7829564732190847584",
      "publicId": "web-research-v1-0",
      "versionLabel": "1.0.0",
      "versionNotes": "Initial version - Comprehensive web research with source compilation",
      "isComplete": true,
      "isPrivate": false,
      "versionIndex": 0,
      "isAutomatable": true,
      "resourceSubType": "RoutineWeb",
      "config": {
        "__version": "1.0",
        "callDataWeb": {
          "__version": "1.0",
          "schema": {
            "searchQueries": [
              "{{input.primaryQuery}}",
              "{{input.secondaryQuery}}",
              "{{input.tertiaryQuery}}"
            ],
            "maxResultsPerQuery": 5,
            "searchDepth": "{{input.searchDepth}}",
            "sourceFilters": {
              "excludeDomains": "{{input.excludedSources}}",
              "preferredDomains": "{{input.preferredSources}}",
              "dateRange": "{{input.dateFilter}}"
            },
            "extractionRules": {
              "summaryLength": "{{input.summaryPreference}}",
              "includeMetadata": true,
              "extractQuotes": true
            }
          }
        },
        "formInput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "primaryQuery",
                "id": "primary_query_input",
                "label": "Primary Search Query",
                "type": "TextInput",
                "isRequired": true,
                "placeholder": "Main research question or topic"
              },
              {
                "fieldName": "secondaryQuery",
                "id": "secondary_query_input",
                "label": "Secondary Query",
                "type": "TextInput",
                "isRequired": false,
                "placeholder": "Alternative search angle (optional)"
              },
              {
                "fieldName": "tertiaryQuery",
                "id": "tertiary_query_input",
                "label": "Tertiary Query",
                "type": "TextInput",
                "isRequired": false,
                "placeholder": "Additional search perspective (optional)"
              },
              {
                "fieldName": "searchDepth",
                "id": "search_depth_input",
                "label": "Search Depth",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "moderate",
                "options": [
                  "quick",
                  "moderate",
                  "thorough"
                ]
              },
              {
                "fieldName": "preferredSources",
                "id": "preferred_sources_input",
                "label": "Preferred Sources",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "List of trusted domains (optional)"
              },
              {
                "fieldName": "excludedSources",
                "id": "excluded_sources_input",
                "label": "Excluded Sources",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Domains to exclude (optional)"
              },
              {
                "fieldName": "dateFilter",
                "id": "date_filter_input",
                "label": "Date Range",
                "type": "TextInput",
                "isRequired": false,
                "placeholder": "e.g., 'past year', 'last 6 months'"
              },
              {
                "fieldName": "summaryPreference",
                "id": "summary_preference_input",
                "label": "Summary Length",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "medium",
                "options": [
                  "brief",
                  "medium",
                  "detailed"
                ]
              }
            ]
          }
        },
        "formOutput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "researchBrief",
                "id": "research_brief_output",
                "label": "Research Brief",
                "type": "TextInput"
              },
              {
                "fieldName": "keyFindings",
                "id": "key_findings_output",
                "label": "Key Findings",
                "type": "JSON"
              },
              {
                "fieldName": "sourceLinks",
                "id": "source_links_output",
                "label": "Source Links",
                "type": "JSON"
              },
              {
                "fieldName": "credibilityNotes",
                "id": "credibility_notes_output",
                "label": "Source Credibility Notes",
                "type": "JSON"
              }
            ]
          }
        },
        "executionStrategy": "reasoning"
      },
      "translations": [
        {
          "id": "7829564732190847585",
          "language": "en",
          "name": "Web Research Agent",
          "description": "Conducts thorough web searches and compiles findings into comprehensive research briefs. Uses multiple search angles, evaluates source credibility, and properly cites all sources.",
          "instructions": "Provide your research query and any additional search angles. You can specify search depth, preferred/excluded sources, date filters, and summary preferences. The routine will search multiple angles, compile findings, and produce a well-cited research brief."
        }
      ]
    }
  ]
}
