      "isComplete": true,
      "isPrivate": false,
      "versionIndex": 0,
      "isAutomatable": true,
      "resourceSubType": "RoutineMultiStep",
      "config": {
        "__version": "1.0",
        "graph": {
          "__type": "Sequential",
          "schema": {
            "steps": [
              {
                "id": "step-1",
                "name": "Define Research Scope",
                "description": "Establishes research questions, boundaries, and objectives",
                "subroutineId": "7829564732190849749",
                "inputMap": {
                  "researchQuestion": "primaryInquiry",
                  "contextualBackground": "domainContext",
                  "intendedOutcomes": "researchGoals"
                },
                "outputMap": {
                  "researchFramework": "scopeDefinition",
                  "searchStrategy": "investigationPlan",
                  "inclusionCriteria": "relevanceFilters"
                }
              },
              {
                "id": "step-2",
                "name": "Gather Source Materials",
                "description": "Collects diverse sources from multiple channels",
                "subroutineId": "7829564732190849752",
                "inputMap": {
                  "investigationPlan": "searchStrategy",
                  "sourceChannels": "informationSources",
                  "qualityThresholds": "credibilityStandards"
                },
                "outputMap": {
                  "sourceCollection": "researchMaterials",
                  "metadataRecords": "sourceAttributes",
                  "coverageAnalysis": "topicSpanning"
                }
              },
              {
                "id": "step-3",
                "name": "Extract Key Information",
                "description": "Identifies and extracts relevant data points and insights",
                "subroutineId": "TODO: Need information extraction subroutine",
                "inputMap": {
                  "researchMaterials": "sourceCollection",
                  "relevanceFilters": "inclusionCriteria",
                  "extractionMethods": "analysisApproaches"
                },
                "outputMap": {
                  "extractedData": "keyFindings",
                  "conceptMapping": "ideaConnections",
                  "evidenceChain": "supportingData"
                }
              },
              {
                "id": "step-4",
                "name": "Analyze & Compare Findings",
                "description": "Performs comparative analysis and identifies patterns",
                "subroutineId": "TODO: Need comparative analysis subroutine",
                "inputMap": {
                  "keyFindings": "extractedData",
                  "analysisFramework": "comparisonMethods",
                  "conflictResolution": "disagreementHandling"
                },
                "outputMap": {
                  "patternAnalysis": "emergentThemes",
                  "consensusPoints": "agreementAreas",
                  "divergenceMapping": "conflictingViews"
                }
              },
              {
                "id": "step-5",
                "name": "Synthesize Coherent Narrative",
                "description": "Integrates findings into unified understanding",
                "subroutineId": "TODO: Need synthesis subroutine",
                "inputMap": {
                  "emergentThemes": "patternAnalysis",
                  "supportingData": "evidenceChain",
                  "narrativeStructure": "storyFramework"
                },
                "outputMap": {
                  "synthesisNarrative": "integratedFindings",
                  "insightHierarchy": "layeredUnderstanding",
                  "knowledgeGaps": "identifiedLimitations"
                }
              },
              {
                "id": "step-6",
                "name": "Generate Research Output",
                "description": "Creates comprehensive research deliverables",
                "subroutineId": "TODO: Need output generation subroutine",
                "inputMap": {
                  "integratedFindings": "synthesisNarrative",
                  "outputRequirements": "deliverableSpecs",
                  "audienceNeeds": "communicationStyle"
                },
                "outputMap": {
                  "researchReport": "comprehensiveDocument",
                  "executiveSummary": "condensedInsights",
                  "recommendationSet": "actionableGuidance"
                }
              }
            ],
            "rootContext": {
              "inputMap": {
                "research": "primaryInquiry",
                "context": "domainContext",
                "sources": "informationSources",
                "requirements": "deliverableSpecs"
              },
              "outputMap": {
                "comprehensiveDocument": "researchOutcome",
                "condensedInsights": "keySummary",
                "integratedFindings": "synthesizedKnowledge",
                "actionableGuidance": "recommendations"
              }
            },
            "executionMode": "sequential"
          }
        },
        "formInput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "research",
                "id": "research_input",
                "label": "Research Question",
                "type": "TextInput",
                "isRequired": true,
                "placeholder": "What are you trying to research or understand?"
              },
              {
                "fieldName": "context",
                "id": "context_input",
                "label": "Research Context",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "Background, domain, and scope of the research"
              },
              {
                "fieldName": "researchType",
                "id": "research_type_input",
                "label": "Research Type",
                "type": "Selector",
                "isRequired": true,
                "options": [
                  "literature-review",
                  "market-research",
                  "scientific-inquiry",
                  "policy-analysis",
                  "competitive-intelligence",
                  "trend-analysis",
                  "feasibility-study",
                  "meta-analysis",
                  "exploratory-research"
                ]
              },
              {
                "fieldName": "sources",
                "id": "sources_input",
                "label": "Information Sources",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "Available sources: databases, journals, reports, websites, experts"
              },
              {
                "fieldName": "depth",
                "id": "depth_input",
                "label": "Research Depth",
                "type": "Selector",
                "isRequired": true,
                "options": [
                  "surface-overview",
                  "moderate-depth",
                  "comprehensive",
                  "exhaustive",
                  "systematic-review"
                ]
              },
              {
                "fieldName": "requirements",
                "id": "requirements_input",
                "label": "Output Requirements",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Format, length, style, and audience for deliverables"
              },
              {
                "fieldName": "timeline",
                "id": "timeline_input",
                "label": "Research Timeline",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "1-week",
                "options": [
                  "1-day",
                  "3-days",
                  "1-week",
                  "2-weeks",
                  "1-month",
                  "3-months"
                ]
              },
              {
                "fieldName": "biasHandling",
                "id": "bias_handling_input",
                "label": "Bias Management",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "acknowledge",
                "options": [
                  "minimize",
                  "acknowledge",
                  "contrast-perspectives",
                  "systematic-review",
                  "triangulation"
                ]
              }
            ]
          }
        },
        "formOutput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "researchOutcome",
                "id": "research_outcome_output",
                "label": "Complete Research Report",
                "type": "JSON"
              },
              {
                "fieldName": "keySummary",
                "id": "key_summary_output",
                "label": "Executive Summary",
                "type": "JSON"
              },
              {
                "fieldName": "synthesizedKnowledge",
                "id": "synthesized_knowledge_output",
                "label": "Integrated Research Findings",
                "type": "JSON"
              },
              {
                "fieldName": "recommendations",
                "id": "recommendations_output",
                "label": "Actionable Recommendations",
                "type": "JSON"
              }
            ]
          }
        },
        "executionStrategy": "reasoning",
        "allowStrategyOverride": false,
