        "callDataWeb": {
          "__version": "1.0",
          "schema": {
            "searchQuery": "{{input.companyName}} {{input.searchFocus}}",
            "processingInstructions": "Extract key information about the company focusing on: recent news, company culture, products/services, leadership team, recent achievements, challenges, and any information relevant to {{input.interviewRole}}. Summarize findings in a structured format.",
