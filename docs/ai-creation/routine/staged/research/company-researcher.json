            "resultFormat": "structured"
          }
        },
        "formInput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "companyName",
                "id": "company_name_input",
                "label": "Company Name",
                "type": "Text",
                "isRequired": true,
                "props": {
                  "placeholder": "Enter the company name..."
                }
              },
              {
                "fieldName": "searchFocus",
                "id": "search_focus_input",
                "label": "Research Focus",
                "type": "Text",
                "isRequired": false,
                "props": {
                  "placeholder": "e.g., 'recent news', 'company culture', 'product launches'"
                }
              },
              {
                "fieldName": "interviewRole",
                "id": "role_input",
                "label": "Interview Role",
                "type": "Text",
                "isRequired": false,
                "props": {
                  "placeholder": "Position you're interviewing for (optional)"
                }
              }
            ]
          }
        },
        "formOutput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
