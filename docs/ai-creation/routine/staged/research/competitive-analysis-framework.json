                "fieldName": "geographicScope",
                "id": "geographic_scope_input",
                "label": "Geographic Scope",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "national",
                "options": [
                  "local",
                  "regional",
                  "national",
                  "international",
                  "global"
                ]
              },
              {
                "fieldName": "industryStage",
                "id": "industry_stage_input",
                "label": "Industry Stage",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "mature",
                "options": [
                  "emerging",
                  "growth",
                  "mature",
                  "declining",
                  "transforming"
                ]
              },
              {
                "fieldName": "analysisDepth",
                "id": "analysis_depth_input",
                "label": "Analysis Depth",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "standard",
                "options": [
                  "high-level",
                  "standard",
                  "detailed",
                  "comprehensive"
                ]
              }
            ]
