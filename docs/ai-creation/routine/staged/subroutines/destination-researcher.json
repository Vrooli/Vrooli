          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "locationOverview",
                "id": "location_overview_input",
                "label": "Destination Insights",
                "type": "JSON",
                "isRequired": true
              },
              {
                "fieldName": "interestCategories",
                "id": "interest_categories_input",
                "label": "Activity Preferences",
                "type": "JSON",
                "isRequired": true
              },
              {
                "fieldName": "researchDepth",
                "id": "research_depth_input",
                "label": "Information Detail Level",
                "type": "Text",
                "isRequired": false
              }
            ]
          }
        },
        "formOutput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "attractionsList",
                "id": "attractions_output",
                "label": "Points of Interest",
                "type": "JSON"
              },
              {
                "fieldName": "culturalInsights",
                "id": "cultural_output",
                "label": "Local Knowledge",
                "type": "JSON"
              },
              {
                "fieldName": "hiddenGems",
                "id": "hidden_gems_output",
