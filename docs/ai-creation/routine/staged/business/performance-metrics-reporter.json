  "isInternal": false,
  "tags": [],
  "versions": [
    {
      "id": "7829564732190847929",
      "publicId": "metrics-report-v1",
      "versionLabel": "1.0.0",
      "versionNotes": "Initial version",
      "isComplete": true,
      "isPrivate": false,
      "versionIndex": 0,
      "isAutomatable": true,
      "resourceSubType": "RoutineMultiStep",
      "config": {
        "__version": "1.0",
        "graph": {
          "__type": "Sequential",
          "schema": {
            "steps": [
              {
                "id": "step-1",
                "name": "Parse Performance Data",
                "description": "Extracts and structures raw performance metrics",
                "subroutineId": "data-parser-v1",
                "inputMap": {
                  "originalPerformanceData": "performanceData",
                  "originalTargets": "performanceTargets",
                  "originalTimeperiod": "reportingPeriod",
                  "originalAudience": "targetAudience"
                },
                "outputMap": {
                  "structuredMetrics": "parsedMetrics",
                  "keyPerformanceIndicators": "mainKPIs",
                  "benchmarkComparisons": "targetComparisons",
                  "timeSeriesData": "trendData"
                }
              },
              {
                "id": "step-2",
                "name": "Analyze Trends and Patterns",
                "description": "Identifies performance trends, patterns, and anomalies",
                "subroutineId": "trend-analyzer-v1",
                "inputMap": {
                  "parsedMetrics": "metricsData",
                  "mainKPIs": "kpiValues",
                  "targetComparisons": "benchmarkData",
                  "trendData": "historicalData"
                },
                "outputMap": {
                  "trendAnalysis": "identifiedTrends",
                  "performanceGaps": "varianceAnalysis",
                  "standoutMetrics": "notablePerformance",
                  "concernAreas": "problemSpots"
                }
              },
              {
                "id": "step-3",
                "name": "Generate Narrative Insights",
                "description": "Creates story-driven explanations of the performance data",
                "subroutineId": "narrative-builder-v1",
                "inputMap": {
                  "identifiedTrends": "trendInsights",
                  "varianceAnalysis": "performanceGaps",
                  "notablePerformance": "highlights",
                  "problemSpots": "challenges"
                },
                "outputMap": {
                  "executiveNarrative": "storyOverview",
                  "detailedFindings": "analyticalNarrative",
                  "contextualExplanations": "backgroundContext",
                  "performanceStory": "dataStoryline"
                }
              },
              {
                "id": "step-4",
                "name": "Compile Final Report",
                "description": "Assembles comprehensive performance report with recommendations",
                "subroutineId": "report-compiler-v1",
                "inputMap": {
                  "storyOverview": "executiveSummary",
                  "analyticalNarrative": "detailedAnalysis",
                  "backgroundContext": "contextualInfo",
                  "dataStoryline": "performanceNarrative"
                },
                "outputMap": {
                  "finalReport": "completeReport",
                  "keyRecommendations": "actionableAdvice",
                  "futureProjections": "outlookAnalysis",
                  "stakeholderSummary": "executiveBrief"
                }
              }
            ],
            "rootContext": {
              "inputMap": {
                "performanceData": "originalPerformanceData",
                "performanceTargets": "originalTargets",
                "reportingPeriod": "originalTimeperiod",
                "targetAudience": "originalAudience"
              },
              "outputMap": {
                "completeReport": "finalReport",
                "actionableAdvice": "keyRecommendations",
                "outlookAnalysis": "futureProjections",
                "executiveBrief": "stakeholderSummary"
              }
            },
            "executionMode": "sequential"
          }
        },
        "formInput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "performanceData",
                "id": "performance_data_input",
                "label": "Performance Data",
                "type": "Text",
                "isRequired": true,
                "props": {
                  "placeholder": "Paste your raw performance metrics, KPIs, and data...",
                  "rows": 6
                }
              },
              {
                "fieldName": "performanceTargets",
                "id": "targets_input",
                "label": "Performance Targets/Goals",
                "type": "Text",
                "isRequired": false,
                "props": {
                  "placeholder": "List your performance targets, goals, or benchmarks...",
                  "rows": 3
                }
              },
              {
                "fieldName": "reportingPeriod",
                "id": "period_input",
                "label": "Reporting Period",
                "type": "Text",
                "isRequired": false,
                "props": {
                  "placeholder": "Specify the time period (e.g., Q1 2024, January 2024, etc.)...",
                  "rows": 1
                }
              },
              {
                "fieldName": "targetAudience",
                "id": "audience_input",
                "label": "Target Audience",
                "type": "Text",
                "isRequired": false,
                "props": {
                  "placeholder": "Who is this report for? (executives, team, stakeholders, etc.)...",
                  "rows": 1
                }
              }
            ]
          }
        },
        "formOutput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "finalReport",
                "id": "complete_report_output",
                "label": "Complete Performance Report",
                "type": "Text"
              },
              {
                "fieldName": "keyRecommendations",
                "id": "recommendations_output",
                "label": "Key Recommendations",
                "type": "Text"
              },
              {
                "fieldName": "futureProjections",
                "id": "projections_output",
                "label": "Future Outlook and Projections",
                "type": "Text"
              },
              {
                "fieldName": "stakeholderSummary",
                "id": "executive_summary_output",
                "label": "Executive Summary for Stakeholders",
                "type": "Text"
              }
            ]
          }
        },
        "executionStrategy": "deterministic",
        "allowStrategyOverride": true,
        "subroutineStrategies": {
          "0": "deterministic",
          "1": "reasoning",
          "2": "conversational",
          "3": "conversational"
        }
      },
      "translations": [
        {
          "id": "7829564732190847930",
          "language": "en",
          "name": "Performance Metrics Reporter",
          "description": "Transforms raw performance data into compelling narrative reports. Analyzes trends, identifies key insights, and creates comprehensive reports with executive summaries, detailed findings, and actionable recommendations for stakeholders.",
          "instructions": "Provide your performance data, targets, and reporting context. The routine will analyze trends, create narrative explanations of the data, and generate a complete report with executive summary and actionable recommendations tailored to your audience.",
          "details": "This routine converts numbers and metrics into meaningful stories that stakeholders can understand and act upon. It identifies trends, explains variances, highlights achievements, and provides context-rich narratives that make performance data accessible and actionable."
        }
      ]
    }
  ]
