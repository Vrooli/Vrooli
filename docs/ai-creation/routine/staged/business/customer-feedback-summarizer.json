                "subroutineId": "sentiment-analyzer-v1",
                "inputMap": {
                  "parsedFeedback": "feedbackText",
                  "categoryBreakdown": "feedbackTypes",
                  "segmentAnalysis": "customerData",
                  "volumeMetrics": "dataVolume"
