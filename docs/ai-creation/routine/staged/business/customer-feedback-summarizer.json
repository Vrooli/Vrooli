              },
              {
                "id": "step-3",
                "name": "Identify Key Insights",
                "description": "Extracts actionable insights and priority areas",
                "subroutineId": "insight-extractor-v1",
                "inputMap": {
                  "overallSentiment": "sentimentData",
                  "recurringThemes": "themeData",
                  "emotionalPatterns": "emotionalAnalysis",
                  "commonIssues": "frequentProblems"
                },
                "outputMap": {
