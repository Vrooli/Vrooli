                "outputMap": {
                  "customerPersonas": "detailedProfiles",
                  "segmentMapping": "audienceSegments",
                  "needsAnalysis": "customerMotivations"
                }
              },
              {
                "id": "step-2",
                "name": "Map Journey Stages",
                "description": "Identifies all touchpoints and interaction phases",
                "subroutineId": "7829564732190849704",
                "inputMap": {
                  "detailedProfiles": "customerPersonas",
                  "businessProcess": "serviceFlow",
                  "channelInventory": "touchpointList"
                },
                "outputMap": {
                  "journeyStages": "customerPhases",
                  "touchpointMap": "interactionPoints",
                  "channelFlow": "omnichanneMapping"
                }
              },
              {
                "id": "step-3",
                "name": "Analyze Pain Points",
                "description": "Identifies friction and frustration in the journey",
                "subroutineId": "TODO: Need pain point analysis subroutine",
                "inputMap": {
                  "customerPhases": "journeyStages",
                  "feedbackData": "customerComplaints",
                  "performanceMetrics": "experienceData"
                },
                "outputMap": {
                  "painPoints": "frictionAreas",
                  "emotionalMapping": "sentimentFlow",
                  "dropoffAnalysis": "abandonmentPoints"
                }
              },
              {
                "id": "step-4",
                "name": "Identify Opportunities",
                "description": "Discovers improvement and delight opportunities",
                "subroutineId": "TODO: Need opportunity identification subroutine",
                "inputMap": {
                  "frictionAreas": "painPoints",
                  "competitorAnalysis": "marketBenchmarks",
                  "innovationPotential": "enhancementAreas"
                },
                "outputMap": {
                  "opportunities": "improvementOptions",
                  "delightMoments": "wowFactors",
                  "competitiveAdvantages": "differentiators"
                }
              },
              {
                "id": "step-5",
                "name": "Design Ideal Experience",
                "description": "Creates optimized journey with enhanced touchpoints",
                "subroutineId": "TODO: Need experience design subroutine",
                "inputMap": {
                  "improvementOptions": "opportunities",
                  "customerMotivations": "needsAnalysis",
                  "designPrinciples": "experienceStrategy"
                },
                "outputMap": {
                  "idealJourney": "optimizedExperience",
                  "touchpointDesign": "enhancedInteractions",
                  "emotionalDesign": "feelingsTrajectory"
                }
              },
              {
                "id": "step-6",
                "name": "Create Implementation Plan",
                "description": "Develops roadmap for journey improvements",
                "subroutineId": "TODO: Need implementation planning subroutine",
                "inputMap": {
                  "optimizedExperience": "idealJourney",
                  "resourceRequirements": "implementationNeeds",
                  "priorityFramework": "executionSequence"
                },
                "outputMap": {
                  "implementationRoadmap": "actionPlan",
                  "metricsDashboard": "measurementFramework",
                  "changeManagement": "rolloutStrategy"
                }
              }
            ],
            "rootContext": {
              "inputMap": {
                "business": "companyProfile",
                "customers": "audienceInsights",
                "objectives": "journeyGoals",
                "resources": "availableCapabilities"
              },
              "outputMap": {
                "actionPlan": "journeyOutcome",
                "optimizedExperience": "idealJourneyMap",
                "measurementFramework": "successMetrics",
                "enhancedInteractions": "improvedTouchpoints"
              }
            },
            "executionMode": "sequential"
          }
        },
        "formInput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "business",
                "id": "business_input",
                "label": "Business Profile",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "Your business type, industry, and value proposition"
              },
              {
                "fieldName": "customers",
                "id": "customers_input",
                "label": "Customer Information",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "Target audience demographics, behaviors, and needs"
