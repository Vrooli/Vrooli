              {
                "fieldName": "journeyType",
                "id": "journey_type_input",
                "label": "Journey Type",
                "type": "Selector",
                "isRequired": true,
                "options": [
                  "purchase-journey",
                  "onboarding",
                  "service-experience",
                  "subscription-lifecycle",
                  "b2b-sales",
                  "support-journey",
                  "renewal-retention",
                  "advocacy-journey"
                ]
              },
              {
                "fieldName": "objectives",
                "id": "objectives_input",
                "label": "Mapping Objectives",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "What do you want to achieve with this journey mapping?"
              },
              {
                "fieldName": "currentChallenges",
                "id": "current_challenges_input",
                "label": "Current Challenges",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Known pain points or areas of concern"
              },
              {
                "fieldName": "touchpoints",
                "id": "touchpoints_input",
                "label": "Current Touchpoints",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Existing customer interaction channels and points"
              },
              {
                "fieldName": "maturityLevel",
                "id": "maturity_level_input",
                "label": "CX Maturity",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "developing",
                "options": [
                  "basic",
                  "developing",
                  "established",
                  "advanced",
                  "leading"
                ]
              },
              {
                "fieldName": "resources",
                "id": "resources_input",
                "label": "Available Resources",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Budget, team, and technology capabilities"
              }
            ]
          }
        },
        "formOutput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "journeyOutcome",
