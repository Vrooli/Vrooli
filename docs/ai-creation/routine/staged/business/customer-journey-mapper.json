        "graph": {
          "__type": "Sequential",
          "schema": {
            "steps": [
              {
                "id": "step-1",
                "name": "Define Customer Personas",
                "description": "Creates detailed profiles of target customer segments",
                "subroutineId": "7829564732190849701",
                "inputMap": {
                  "customerData": "audienceInsights",
                  "segmentationCriteria": "demographicFactors",
                  "behavioralPatterns": "customerBehaviors"
