      "publicId": "invest-analyze-v1-0",
      "versionLabel": "1.0.0",
      "versionNotes": "Initial version - Comprehensive investment portfolio analysis and optimization",
      "isComplete": true,
      "isPrivate": false,
      "versionIndex": 0,
      "isAutomatable": true,
      "resourceSubType": "RoutineMultiStep",
      "config": {
        "__version": "1.0",
        "graph": {
          "__type": "Sequential",
          "schema": {
            "steps": [
              {
                "id": "step-1",
                "name": "Assess Current Holdings",
                "description": "Analyzes existing portfolio composition and performance",
                "subroutineId": "7829564732190849425",
                "inputMap": {
                  "portfolioData": "currentHoldings",
                  "timeFrame": "analysisWindow",
                  "benchmarks": "comparisonIndices"
                },
                "outputMap": {
                  "portfolioBreakdown": "compositionAnalysis",
                  "performanceMetrics": "returnAnalysis",
                  "riskAssessment": "volatilityMeasures"
                }
              },
              {
                "id": "step-2",
                "name": "Evaluate Risk Profile",
                "description": "Assesses investor risk tolerance and capacity",
                "subroutineId": "7829564732190849428",
                "inputMap": {
                  "investorProfile": "personalSituation",
                  "goals": "investmentObjectives",
                  "timeHorizon": "investmentTimeframe"
                },
                "outputMap": {
                  "riskTolerance": "riskPreferences",
                  "riskCapacity": "riskAbility",
                  "targetAllocation": "recommendedMix"
                }
              },
              {
                "id": "step-3",
                "name": "Analyze Asset Allocation",
                "description": "Reviews diversification and alignment with goals",
                "subroutineId": "7829564732190849431",
                "inputMap": {
                  "compositionAnalysis": "portfolioBreakdown",
                  "recommendedMix": "targetAllocation",
                  "marketConditions": "economicContext"
                },
                "outputMap": {
                  "allocationGaps": "diversionAnalysis",
                  "diversificationScore": "riskDistribution",
                  "rebalancingNeeds": "adjustmentRequirements"
                }
              },
              {
                "id": "step-4",
                "name": "Evaluate Individual Investments",
                "description": "Assesses quality and suitability of specific holdings",
                "subroutineId": "7829564732190849434",
                "inputMap": {
                  "currentHoldings": "portfolioData",
                  "fundamentalData": "investmentMetrics",
                  "marketTrends": "sectorAnalysis"
                },
                "outputMap": {
                  "investmentQuality": "holdingAssessment",
                  "underperformers": "weakPositions",
                  "opportunityFlags": "improvementAreas"
                }
              },
              {
                "id": "step-5",
                "name": "Generate Optimization Recommendations",
                "description": "Creates specific actionable improvement strategies",
                "subroutineId": "7829564732190849455",
                "inputMap": {
                  "allAnalysis": "comprehensiveAssessment",
                  "adjustmentRequirements": "rebalancingNeeds",
                  "improvementAreas": "opportunityFlags"
                },
                "outputMap": {
                  "optimizationPlan": "improvementStrategy",
                  "tradingActions": "specificRecommendations",
                  "timingGuidance": "implementationPlan"
                }
              },
              {
                "id": "step-6",
                "name": "Create Monitoring Framework",
                "description": "Establishes ongoing portfolio tracking system",
                "subroutineId": "7829564732190849458",
                "inputMap": {
                  "optimizationPlan": "improvementStrategy",
                  "keyMetrics": "trackingIndicators",
                  "reviewSchedule": "monitoringFrequency"
                },
                "outputMap": {
                  "monitoringDashboard": "trackingSystem",
                  "alertTriggers": "warningSignals",
                  "reviewSchedule": "maintenancePlan"
                }
              }
            ],
            "rootContext": {
              "inputMap": {
                "portfolio": "currentHoldings",
                "investor": "personalSituation",
                "objectives": "investmentObjectives",
                "timeframe": "investmentTimeframe"
              },
              "outputMap": {
                "improvementStrategy": "portfolioOptimization",
                "specificRecommendations": "actionPlan",
                "trackingSystem": "monitoringFramework",
                "comprehensiveAssessment": "portfolioReport"
              }
            },
            "executionMode": "sequential"
          }
        },
        "formInput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "portfolio",
                "id": "portfolio_input",
                "label": "Current Portfolio Holdings",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "List of investments with quantities, values, and dates"
              },
              {
                "fieldName": "investor",
                "id": "investor_input",
                "label": "Investor Profile",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "Age, income, net worth, financial situation"
              },
              {
                "fieldName": "objectives",
                "id": "objectives_input",
                "label": "Investment Objectives",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "Goals like retirement, growth, income, preservation"
              },
              {
                "fieldName": "timeframe",
                "id": "timeframe_input",
                "label": "Investment Time Horizon",
                "type": "Selector",
                "isRequired": true,
                "options": [
                  "short-term-1-3-years",
                  "medium-term-3-10-years",
                  "long-term-10+-years",
                  "mixed-timeframes"
                ]
              },
              {
                "fieldName": "riskPreference",
                "id": "risk_preference_input",
                "label": "Risk Preference",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "moderate",
                "options": [
                  "conservative",
                  "moderate",
                  "aggressive",
                  "very-aggressive",
                  "unknown"
                ]
              },
              {
                "fieldName": "analysisDepth",
                "id": "analysis_depth_input",
                "label": "Analysis Depth",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "comprehensive",
                "options": [
                  "basic",
                  "standard",
                  "comprehensive",
                  "detailed"
                ]
              },
              {
                "fieldName": "marketContext",
                "id": "market_context_input",
                "label": "Current Market Context",
                "type": "TextInput",
                "isRequired": false,
                "placeholder": "Any specific market conditions or economic factors to consider"
              }
            ]
          }
        },
        "formOutput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "portfolioOptimization",
                "id": "portfolio_optimization_output",
                "label": "Portfolio Optimization Strategy",
                "type": "JSON"
              },
              {
                "fieldName": "actionPlan",
                "id": "action_plan_output",
                "label": "Specific Action Plan",
                "type": "JSON"
              },
              {
                "fieldName": "monitoringFramework",
                "id": "monitoring_framework_output",
                "label": "Portfolio Monitoring Framework",
                "type": "JSON"
              },
              {
                "fieldName": "portfolioReport",
                "id": "portfolio_report_output",
                "label": "Comprehensive Portfolio Report",
                "type": "JSON"
              }
            ]
          }
        },
        "executionStrategy": "reasoning",
        "allowStrategyOverride": false,
        "subroutineStrategies": {
          "0": "reasoning",
          "1": "reasoning",
          "2": "reasoning",
          "3": "reasoning",
          "4": "reasoning",
          "5": "reasoning"
        }
      },
      "translations": [
        {
          "id": "7829564732190847693",
          "language": "en",
          "name": "Investment Portfolio Analyzer",
          "description": "Comprehensive investment portfolio analysis system that evaluates current holdings, assesses risk profiles, analyzes asset allocation, evaluates individual investments, generates optimization recommendations, and creates monitoring frameworks for informed investment decisions.",
          "instructions": "Provide your current portfolio holdings, investor profile, and investment objectives. Specify your time horizon and risk preferences. The routine will analyze your portfolio composition, evaluate performance, identify optimization opportunities, and create a monitoring framework with specific action recommendations."
        }
      ]
    }
