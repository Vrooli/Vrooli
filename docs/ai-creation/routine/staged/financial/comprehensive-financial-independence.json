              },
              {
                "id": "step-4",
                "name": "Develop Investment Strategy",
                "description": "Creates diversified portfolio aligned with risk tolerance and timeline",
                "subroutineId": "7829564732190849482",
                "inputMap": {
                  "investmentCapacity": "savingsRate",
                  "riskProfile": "riskTolerance",
                  "investmentKnowledge": "experienceLevel"
                },
                "outputMap": {
                  "portfolioStrategy": "investmentPlan",
                  "assetAllocation": "diversificationFramework",
