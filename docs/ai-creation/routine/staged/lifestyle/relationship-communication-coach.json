              },
              {
                "fieldName": "communicationStyle",
                "id": "communication_style_input",
                "label": "Communication Style",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "mixed",
                "options": [
                  "direct",
                  "indirect",
                  "emotional",
                  "logical",
                  "expressive",
                  "reserved",
                  "mixed"
