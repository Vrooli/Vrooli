        "graph": {
          "__type": "Sequential",
          "schema": {
            "steps": [
              {
                "id": "step-1",
                "name": "Assess Communication Patterns",
                "description": "Analyzes current communication styles and relationship dynamics",
                "subroutineId": "7829564732190849725",
                "inputMap": {
                  "relationshipType": "contextType",
                  "communicationChallenges": "currentIssues",
                  "interactionHistory": "relationshipDynamics"
                },
                "outputMap": {
                  "communicationStyle": "currentPatterns",
                  "conflictTriggers": "tensionPoints",
                  "strengthAreas": "positivePatterns"
                }
              },
              {
                "id": "step-2",
                "name": "Identify Communication Goals",
                "description": "Defines specific objectives for relationship improvement",
                "subroutineId": "7829564732190849728",
                "inputMap": {
                  "relationshipGoals": "desiredOutcomes",
                  "currentPatterns": "communicationStyle",
                  "urgencyLevel": "improvementPriority"
                },
                "outputMap": {
                  "communicationObjectives": "targetGoals",
                  "prioritySkills": "focusAreas",
                  "successIndicators": "progressMeasures"
                }
              },
              {
                "id": "step-3",
                "name": "Develop Listening Skills",
                "description": "Enhances active listening and empathy capabilities",
                "subroutineId": "7829564732190849755",
                "inputMap": {
                  "listeningChallenges": "hearingBarriers",
                  "empathyLevel": "understandingCapacity",
                  "attentionFactors": "focusAbility"
                },
                "outputMap": {
                  "listeningTechniques": "hearingStrategies",
                  "empathyPractices": "understandingExercises",
                  "presenceTechniques": "attentionMethods"
                }
              },
              {
                "id": "step-4",
                "name": "Practice Expression Skills",
                "description": "Improves clear, honest, and respectful communication",
                "subroutineId": "7829564732190849758",
                "inputMap": {
                  "expressionChallenges": "communicationBarriers",
                  "emotionalRegulation": "feelingsManagement",
                  "clarityNeeds": "messageEffectiveness"
                },
                "outputMap": {
                  "expressionTechniques": "communicationMethods",
                  "emotionalSkills": "feelingsExpression",
                  "clarityStrategies": "messageClarity"
                }
              },
              {
                "id": "step-5",
                "name": "Build Conflict Resolution Skills",
                "description": "Develops healthy disagreement and problem-solving approaches",
                "subroutineId": "7829564732190849761",
                "inputMap": {
                  "conflictPatterns": "disagreementStyles",
                  "tensionPoints": "conflictTriggers",
                  "resolutionGoals": "harmonyObjectives"
                },
                "outputMap": {
                  "conflictStrategies": "resolutionMethods",
                  "deescalationTechniques": "calmingApproaches",
                  "compromiseMethods": "solutionStrategies"
                }
              },
              {
                "id": "step-6",
                "name": "Create Practice Plan",
                "description": "Develops implementation strategy for ongoing improvement",
                "subroutineId": "7829564732190849764",
                "inputMap": {
                  "allSkills": "communicationToolkit",
                  "relationshipContext": "practiceEnvironment",
                  "improvementTimeline": "developmentSchedule"
                },
                "outputMap": {
                  "practicePlan": "implementationStrategy",
                  "skillReinforcement": "continuousImprovement",
                  "progressTracking": "developmentMonitoring"
                }
              }
            ],
            "rootContext": {
              "inputMap": {
                "relationship": "contextType",
                "challenges": "currentIssues",
                "goals": "desiredOutcomes",
                "timeline": "developmentSchedule"
              },
              "outputMap": {
                "implementationStrategy": "communicationPlan",
                "hearingStrategies": "listeningSkills",
                "communicationMethods": "expressionSkills",
                "resolutionMethods": "conflictManagement"
              }
            },
            "executionMode": "sequential"
          }
        },
