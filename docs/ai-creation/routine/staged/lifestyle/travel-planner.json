      "translations": [
        {
          "id": "7829564732190847660",
          "language": "en",
          "name": "Travel Planner",
          "description": "Comprehensive travel planning assistant that creates complete trip itineraries. Researches transportation and accommodation options, curates activities based on interests, optimizes daily schedules, and provides essential travel resources and tips.",
          "instructions": "Specify your destination, travel dates, budget, and interests. Choose your travel style and group size. The routine will create a complete travel plan with optimized itinerary, booking recommendations, activity suggestions, and practical travel resources tailored to your preferences."
        }
      ]
