      "resourceSubType": "RoutineMultiStep",
      "config": {
        "__version": "1.0",
        "graph": {
          "__type": "Sequential",
          "schema": {
            "steps": [
              {
                "id": "step-1",
                "name": "Define Travel Requirements",
                "description": "Clarifies destination, dates, budget, and travel preferences",
                "subroutineId": "7829564732190848954",
                "inputMap": {
                  "destination": "travelDestination",
                  "travelDates": "tripDates",
                  "budget": "travelBudget",
                  "preferences": "travelStyle"
                },
                "outputMap": {
                  "travelSpecs": "tripRequirements",
                  "constraints": "travelLimitations",
                  "priorities": "travelerPreferences"
                }
              },
              {
                "id": "step-2",
                "name": "Research Transportation Options",
                "description": "Finds optimal flights, trains, or other transportation methods",
                "subroutineId": "7829564732190849638",
                "inputMap": {
                  "travelSpecs": "tripRequirements",
                  "departure": "originLocation",
                  "destination": "travelDestination",
                  "flexibleDates": "dateFlexibility"
                },
                "outputMap": {
                  "transportOptions": "travelChoices",
                  "recommendedRoutes": "optimalPaths",
                  "pricingInfo": "transportCosts"
                }
              },
              {
                "id": "step-3",
                "name": "Find Accommodation",
                "description": "Searches for suitable hotels, rentals, or lodging options",
                "subroutineId": "7829564732190849641",
                "inputMap": {
                  "destination": "travelDestination",
                  "travelDates": "tripDates",
                  "budget": "lodgingBudget",
                  "accommodationType": "lodgingPreferences"
                },
                "outputMap": {
                  "accommodationOptions": "lodgingChoices",
                  "locationAnalysis": "neighborhoodInfo",
                  "pricingComparison": "accommodationCosts"
                }
              },
              {
                "id": "step-4",
                "name": "Plan Activities & Attractions",
                "description": "Curates activities, attractions, and experiences based on interests",
                "subroutineId": "7829564732190849644",
                "inputMap": {
                  "destination": "travelDestination",
                  "interests": "activityPreferences",
                  "duration": "tripLength",
                  "season": "travelSeason"
                },
                "outputMap": {
                  "attractions": "mustSeeList",
                  "activities": "experienceOptions",
                  "localRecommendations": "insiderTips"
                }
              },
              {
                "id": "step-5",
                "name": "Create Daily Itinerary",
                "description": "Organizes activities into logical daily schedules with timing",
                "subroutineId": "7829564732190849647",
                "inputMap": {
                  "attractions": "mustSeeList",
                  "activities": "experienceOptions",
                  "accommodation": "lodgingLocation",
                  "transportation": "localTransport"
                },
                "outputMap": {
                  "dailySchedule": "itineraryPlan",
                  "routeOptimization": "efficientRoutes",
                  "timeAllocation": "activityTiming"
                }
              },
              {
                "id": "step-6",
                "name": "Compile Travel Resources",
                "description": "Gathers essential travel information, contacts, and documentation",
                "subroutineId": "7829564732190849650",
                "inputMap": {
                  "destination": "travelDestination",
                  "itinerary": "travelPlan",
                  "requirements": "documentationNeeds"
                },
                "outputMap": {
                  "travelGuide": "comprehensiveGuide",
                  "emergencyInfo": "safetyResources",
                  "practicalTips": "travelAdvice"
                }
              }
            ],
            "rootContext": {
              "inputMap": {
                "destination": "travelDestination",
                "dates": "tripDates",
                "budget": "travelBudget",
                "interests": "activityPreferences"
              },
              "outputMap": {
                "travelPlan": "completeTravelPlan",
                "itineraryPlan": "dailyItinerary",
                "travelChoices": "bookingRecommendations",
                "comprehensiveGuide": "travelResources"
              }
            },
            "executionMode": "sequential"
          }
        },
        "formInput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "destination",
                "id": "destination_input",
                "label": "Travel Destination",
                "type": "TextInput",
                "isRequired": true,
                "placeholder": "Where would you like to travel?"
              },
              {
                "fieldName": "dates",
                "id": "dates_input",
                "label": "Travel Dates",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "Departure and return dates (flexible dates if applicable)"
              },
              {
                "fieldName": "budget",
                "id": "budget_input",
                "label": "Travel Budget",
                "type": "TextInput",
                "isRequired": true,
                "placeholder": "Total budget or budget range for the trip"
              },
              {
                "fieldName": "interests",
                "id": "interests_input",
                "label": "Travel Interests",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "What you like to do: culture, food, adventure, relaxation, etc."
              },
              {
                "fieldName": "travelStyle",
                "id": "travel_style_input",
                "label": "Travel Style",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "balanced",
                "options": [
                  "budget",
                  "mid-range",
                  "luxury",
                  "backpacking",
                  "business",
                  "family",
                  "romantic",
                  "solo",
                  "group",
                  "balanced"
                ]
              },
              {
                "fieldName": "groupSize",
                "id": "group_size_input",
                "label": "Group Size",
                "type": "IntegerInput",
                "isRequired": false,
                "defaultValue": 1
              },
              {
                "fieldName": "accommodationType",
                "id": "accommodation_type_input",
                "label": "Preferred Accommodation",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "any",
                "options": [
                  "any",
                  "hotel",
                  "vacation-rental",
                  "hostel",
                  "bed-breakfast",
                  "resort",
                  "apartment"
                ]
              }
            ]
          }
        },
        "formOutput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "completeTravelPlan",
                "id": "complete_travel_plan_output",
                "label": "Complete Travel Plan",
                "type": "JSON"
              },
              {
                "fieldName": "dailyItinerary",
                "id": "daily_itinerary_output",
                "label": "Daily Itinerary",
                "type": "JSON"
              },
              {
                "fieldName": "bookingRecommendations",
                "id": "booking_recommendations_output",
                "label": "Booking Recommendations",
                "type": "JSON"
              },
              {
                "fieldName": "travelResources",
                "id": "travel_resources_output",
                "label": "Travel Guide & Resources",
                "type": "JSON"
              }
            ]
          }
        },
        "executionStrategy": "reasoning",
        "allowStrategyOverride": false,
