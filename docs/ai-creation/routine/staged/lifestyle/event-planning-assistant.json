  "versions": [
    {
      "id": "7829564732190847698",
      "publicId": "event-plan-v1-0",
      "versionLabel": "1.0.0",
      "versionNotes": "Initial version - Comprehensive event planning and coordination system",
      "isComplete": true,
      "isPrivate": false,
      "versionIndex": 0,
      "isAutomatable": true,
      "resourceSubType": "RoutineMultiStep",
      "config": {
        "__version": "1.0",
        "graph": {
          "__type": "Sequential",
          "schema": {
            "steps": [
              {
                "id": "step-1",
                "name": "Define Event Vision & Goals",
                "description": "Clarifies event purpose, objectives, and success criteria",
                "subroutineId": "7829564732190849707",
                "inputMap": {
                  "eventPurpose": "objectives",
                  "targetAudience": "attendeeProfile",
                  "eventType": "occasionCategory"
                },
                "outputMap": {
                  "eventVision": "clarifiedPurpose",
                  "successMetrics": "measurementCriteria",
                  "audienceExpectations": "attendeeNeeds"
                }
              },
              {
                "id": "step-2",
                "name": "Establish Budget & Timeline",
                "description": "Creates financial framework and project schedule",
                "subroutineId": "7829564732190849710",
                "inputMap": {
                  "budgetConstraints": "availableResources",
                  "eventDate": "targetDate",
                  "planningTime": "preparationWindow"
                },
                "outputMap": {
                  "budgetAllocation": "spendingPlan",
                  "projectTimeline": "planningSchedule",
                  "criticalMilestones": "keyDeadlines"
                }
              },
              {
                "id": "step-3",
                "name": "Select Venue & Logistics",
                "description": "Identifies optimal location and basic infrastructure needs",
                "subroutineId": "7829564732190849713",
                "inputMap": {
                  "attendeeCount": "expectedGuests",
                  "eventVision": "clarifiedPurpose",
                  "budgetAllocation": "spendingPlan"
                },
                "outputMap": {
                  "venueOptions": "locationChoices",
                  "logisticsRequirements": "infrastructureNeeds",
                  "accessibilityPlan": "inclusivityMeasures"
                }
              },
              {
                "id": "step-4",
                "name": "Plan Program & Activities",
                "description": "Designs event agenda and engagement activities",
                "subroutineId": "7829564732190849716",
                "inputMap": {
                  "eventVision": "clarifiedPurpose",
                  "audienceExpectations": "attendeeNeeds",
                  "eventDuration": "timeAllocation"
                },
                "outputMap": {
                  "eventProgram": "activitySchedule",
                  "engagementActivities": "interactionPlans",
                  "contingencyPrograms": "backupActivities"
                }
              },
              {
                "id": "step-5",
                "name": "Coordinate Vendors & Services",
                "description": "Manages external providers and service coordination",
                "subroutineId": "7829564732190849719",
                "inputMap": {
                  "serviceNeeds": "requiredServices",
                  "budgetAllocation": "spendingPlan",
                  "qualityStandards": "serviceExpectations"
                },
                "outputMap": {
                  "vendorList": "serviceProviders",
                  "contractGuidance": "agreementFramework",
                  "coordinationPlan": "serviceManagement"
                }
              },
              {
                "id": "step-6",
                "name": "Create Execution Checklist",
                "description": "Develops comprehensive day-of-event management plan",
                "subroutineId": "7829564732190849722",
                "inputMap": {
                  "allPlanning": "comprehensivePlan",
                  "teamRoles": "staffAssignments",
                  "riskFactors": "potentialIssues"
                },
                "outputMap": {
                  "executionChecklist": "dayOfEventPlan",
                  "emergencyProtocols": "contingencyProcedures",
                  "successTracking": "evaluationMethods"
                }
              }
            ],
            "rootContext": {
              "inputMap": {
                "purpose": "objectives",
                "budget": "availableResources",
                "date": "targetDate",
                "guests": "expectedGuests"
              },
              "outputMap": {
                "comprehensivePlan": "eventMasterPlan",
                "planningSchedule": "projectTimeline",
                "dayOfEventPlan": "executionGuide",
                "serviceProviders": "vendorCoordination"
              }
            },
            "executionMode": "sequential"
          }
        },
        "formInput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "purpose",
                "id": "purpose_input",
                "label": "Event Purpose",
                "type": "TextInput",
                "isRequired": true,
                "placeholder": "What is the purpose and goal of this event?"
              },
              {
                "fieldName": "budget",
                "id": "budget_input",
                "label": "Available Budget",
                "type": "TextInput",
                "isRequired": true,
                "placeholder": "What is your total budget for this event?"
              },
              {
                "fieldName": "date",
                "id": "date_input",
                "label": "Target Event Date",
                "type": "TextInput",
                "isRequired": true,
                "placeholder": "When do you want to hold the event?"
              },
              {
                "fieldName": "guests",
                "id": "guests_input",
                "label": "Expected Guest Count",
                "type": "IntegerInput",
                "isRequired": true
              },
              {
                "fieldName": "eventType",
                "id": "event_type_input",
                "label": "Event Type",
                "type": "Selector",
                "isRequired": true,
                "options": [
                  "wedding",
                  "corporate-meeting",
                  "conference",
                  "birthday-party",
                  "anniversary",
                  "graduation",
                  "networking-event",
                  "fundraiser",
                  "product-launch",
                  "workshop",
                  "seminar",
                  "celebration",
                  "other"
                ]
              },
              {
                "fieldName": "formality",
                "id": "formality_input",
                "label": "Formality Level",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "semi-formal",
                "options": [
                  "casual",
                  "semi-formal",
                  "formal",
                  "black-tie",
                  "white-tie"
                ]
              },
              {
                "fieldName": "duration",
                "id": "duration_input",
                "label": "Event Duration",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "half-day",
                "options": [
                  "2-hours",
                  "half-day",
                  "full-day",
                  "multi-day",
                  "week-long"
                ]
              },
              {
                "fieldName": "specialRequirements",
                "id": "special_requirements_input",
                "label": "Special Requirements",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Any special needs, dietary restrictions, accessibility requirements, etc."
              }
            ]
          }
        },
        "formOutput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "eventMasterPlan",
                "id": "event_master_plan_output",
                "label": "Complete Event Master Plan",
                "type": "JSON"
              },
              {
                "fieldName": "projectTimeline",
                "id": "project_timeline_output",
                "label": "Planning Timeline & Milestones",
                "type": "JSON"
              },
              {
                "fieldName": "executionGuide",
                "id": "execution_guide_output",
                "label": "Day-of-Event Execution Guide",
                "type": "JSON"
              },
              {
                "fieldName": "vendorCoordination",
                "id": "vendor_coordination_output",
                "label": "Vendor Coordination Plan",
                "type": "JSON"
              }
            ]
          }
        },
        "executionStrategy": "reasoning",
        "allowStrategyOverride": false,
        "subroutineStrategies": {
          "0": "reasoning",
          "1": "reasoning",
          "2": "reasoning",
          "3": "reasoning",
          "4": "reasoning",
          "5": "reasoning"
        }
      },
      "translations": [
        {
          "id": "7829564732190847699",
          "language": "en",
          "name": "Event Planning Assistant",
          "description": "Comprehensive event planning system that guides through vision setting, budget planning, venue selection, program design, vendor coordination, and execution planning for successful events of any scale and type.",
          "instructions": "Specify your event purpose, budget, target date, and expected guest count. Choose the event type and formality level. The routine will create a complete event plan with timeline, vendor coordination, program design, and day-of-event execution guide."
        }
      ]
    }
  ]
