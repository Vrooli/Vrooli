  "versions": [
    {
      "id": "7829564732190847034",
      "publicId": "morn-pln-v1",
      "versionLabel": "1.0.0",
      "versionNotes": "Initial version",
      "isComplete": true,
      "isPrivate": false,
      "versionIndex": 0,
      "isAutomatable": true,
      "resourceSubType": "RoutineMultiStep",
      "config": {
        "__version": "1.0",
        "executionStrategy": "conversational",
        "callDataMultiStep": {
          "__version": "1.0",
          "schema": {
            "__type": "Sequential",
            "schema": {
              "steps": [
                {
                  "id": "step_wellness_check",
                  "name": "Morning Wellness Check",
                  "description": "Quick check-in on physical and mental state",
                  "subroutineId": "7829564732190848555",
                  "inputMap": {
                    "currentMood": "morningMood",
                    "energyLevel": "energyLevel",
                    "stressLevel": "anticipatedStress",
                    "physicalState": "sleepQuality"
                  },
                  "outputMap": {
                    "copingStrategies": "wellnessStrategies",
                    "affirmations": "morningAffirmations"
                  }
                },
                {
                  "id": "step_task_prioritization",
                  "name": "Prioritize Daily Tasks",
                  "description": "Analyze and prioritize tasks for the day",
                  "subroutineId": "7829564732190848558",
                  "inputMap": {
                    "taskList": "todoList",
                    "energyLevel": "energyLevel",
                    "availableTime": "availableHours",
                    "priorities": "topPriorities"
                  },
                  "outputMap": {
                    "prioritizedTasks": "todaysTasks",
                    "focusItems": "mustDoToday"
                  }
                },
                {
                  "id": "step_schedule_optimization",
                  "name": "Optimize Daily Schedule",
                  "description": "Create time blocks for prioritized tasks",
                  "subroutineId": "7829564732190848561",
                  "inputMap": {
                    "prioritizedTasks": "todaysTasks",
                    "calendarEvents": "calendarData",
                    "energyLevel": "energyLevel",
                    "preferences": "schedulingPreferences"
                  },
                  "outputMap": {
                    "timeBlocks": "dailySchedule",
                    "bufferTime": "breakSchedule"
                  }
                },
                {
                  "id": "step_daily_briefing",
                  "name": "Generate Daily Briefing",
                  "description": "Compile comprehensive morning briefing",
                  "subroutineId": "7829564732190848564",
                  "inputMap": {
                    "wellnessData": "wellnessStrategies",
                    "prioritizedTasks": "todaysTasks",
                    "schedule": "dailySchedule",
                    "affirmations": "morningAffirmations",
                    "weatherData": "weatherInfo",
                    "newsHighlights": "newsTopics"
                  },
                  "outputMap": {
                    "briefingReport": "morningBriefing",
                    "quickReference": "dayAtAGlance"
                  }
                },
                {
                  "id": "step_motivation_boost",
                  "name": "Add Motivational Elements",
                  "description": "Include motivational quote and intention setting",
                  "subroutineId": "7829564732190848567",
                  "inputMap": {
                    "mood": "morningMood",
                    "goals": "topPriorities",
                    "challenges": "anticipatedChallenges"
                  },
                  "outputMap": {
                    "motivationalQuote": "dailyQuote",
                    "intention": "dailyIntention"
                  }
                }
              ]
            }
          }
        },
        "formInput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "morningMood",
                "id": "morning_mood_input",
                "label": "How are you feeling this morning?",
                "type": "Selector",
                "isRequired": true,
                "props": {
                  "options": ["Energized", "Calm", "Anxious", "Tired", "Motivated", "Overwhelmed", "Neutral"],
                  "defaultValue": "Neutral"
                }
              },
              {
                "fieldName": "sleepQuality",
                "id": "sleep_quality_input",
                "label": "How was your sleep?",
                "type": "Text",
                "isRequired": false,
                "props": {
                  "placeholder": "Hours slept, quality, any issues..."
                }
              },
              {
                "fieldName": "energyLevel",
                "id": "energy_level_input",
                "label": "Energy Level (1-10)",
                "type": "IntegerInput",
                "isRequired": true,
                "props": {
                  "min": 1,
                  "max": 10,
                  "defaultValue": 5
                }
              },
              {
                "fieldName": "anticipatedStress",
                "id": "anticipated_stress_input",
                "label": "Anticipated Stress Level Today (1-10)",
                "type": "IntegerInput",
                "isRequired": true,
                "props": {
                  "min": 1,
                  "max": 10,
                  "defaultValue": 5
                }
              },
              {
                "fieldName": "todoList",
                "id": "todo_list_input",
                "label": "Today's To-Do List",
                "type": "Text",
                "isRequired": true,
                "props": {
                  "placeholder": "List all tasks you need to accomplish today...",
                  "minRows": 4
                }
              },
              {
                "fieldName": "topPriorities",
                "id": "top_priorities_input",
                "label": "Top 3 Priorities",
                "type": "Text",
                "isRequired": true,
                "props": {
                  "placeholder": "What absolutely must get done today?"
                }
              },
              {
                "fieldName": "calendarData",
                "id": "calendar_data_input",
                "label": "Scheduled Events/Meetings",
                "type": "Text",
                "isRequired": false,
                "props": {
                  "placeholder": "Any fixed appointments or meetings today?",
                  "minRows": 2
                }
              },
              {
                "fieldName": "availableHours",
                "id": "available_hours_input",
                "label": "Available Work Hours",
                "type": "IntegerInput",
                "isRequired": true,
                "props": {
                  "min": 1,
                  "max": 24,
                  "defaultValue": 8
                }
              },
              {
                "fieldName": "anticipatedChallenges",
                "id": "anticipated_challenges_input",
                "label": "Anticipated Challenges",
                "type": "Text",
                "isRequired": false,
                "props": {
                  "placeholder": "Any challenges or concerns for today?"
                }
              },
              {
                "fieldName": "schedulingPreferences",
                "id": "scheduling_preferences_input",
                "label": "Scheduling Preferences",
                "type": "Text",
                "isRequired": false,
                "props": {
                  "placeholder": "Deep work in morning? Meetings in afternoon? Break preferences?"
                }
              }
            ]
          }
        },
        "formOutput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "morningBriefing",
                "id": "morning_briefing_output",
                "label": "Complete Morning Briefing",
                "type": "Text"
              },
              {
                "fieldName": "dayAtAGlance",
                "id": "day_at_glance_output",
                "label": "Day at a Glance",
                "type": "Text"
              },
              {
                "fieldName": "todaysTasks",
                "id": "todays_tasks_output",
                "label": "Prioritized Task List",
                "type": "Text"
              },
              {
                "fieldName": "dailySchedule",
                "id": "daily_schedule_output",
                "label": "Time-Blocked Schedule",
                "type": "Text"
              },
              {
                "fieldName": "wellnessStrategies",
                "id": "wellness_strategies_output",
                "label": "Wellness Tips for Today",
                "type": "Text"
              },
              {
                "fieldName": "dailyQuote",
                "id": "daily_quote_output",
                "label": "Motivational Quote",
                "type": "Text"
              }
            ]
          }
        }
      },
      "translations": [
        {
          "id": "7829564732190847035",
          "language": "en",
          "name": "Integrated Morning Briefing & Planner",
          "description": "Comprehensive morning routine combining wellness check, task planning, and daily briefing. Creates a complete morning report with prioritized schedule, wellness tips, and motivational elements. Uses BPMN workflow to orchestrate multiple planning components.",
          "instructions": "1. Start with a quick wellness check-in\n2. List all tasks for today and your top 3 priorities\n3. Add any scheduled events or meetings\n4. Specify how many hours you have available\n5. Share any anticipated challenges\n\nThe system will create:\n- A wellness-aware daily plan\n- Prioritized and time-blocked schedule\n- Morning briefing with all key information\n- Personalized wellness strategies\n- Motivational elements for the day\n\nBest used first thing in the morning for optimal daily planning."
        }
      ]
    }
  ]
