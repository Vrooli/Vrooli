      "translations": [
        {
          "id": "7829564732190847828",
          "language": "en",
          "name": "Health Symptom Tracker",
          "description": "Comprehensive health monitoring system that logs symptoms and vitals, analyzes historical patterns, identifies triggers, generates personalized insights, creates medical summaries, and develops actionable health improvement plans.",
          "instructions": "Log your current symptoms and any vital measurements. Specify tracking focus and health history. Include lifestyle factors and wellness goals. The routine will analyze patterns, identify triggers, generate insights, and create health action plans."
        }
      ]
    }
