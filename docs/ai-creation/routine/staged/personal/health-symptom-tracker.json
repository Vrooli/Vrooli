  "versions": [
    {
      "id": "7829564732190847827",
      "publicId": "health-track-v1-0",
      "versionLabel": "1.0.0",
      "versionNotes": "Initial version - Health symptom tracking and pattern analysis system",
      "isComplete": true,
      "isPrivate": false,
      "versionIndex": 0,
      "isAutomatable": true,
      "resourceSubType": "RoutineMultiStep",
      "config": {
        "__version": "1.0",
        "graph": {
          "__type": "Sequential",
          "schema": {
            "steps": [
              {
                "id": "step-1",
                "name": "Log Symptoms & Vitals",
                "description": "Records current symptoms, measurements, and health data",
                "subroutineId": "7829564732190848570",
                "inputMap": {
                  "symptomReport": "currentSymptoms",
                  "vitalMeasurements": "healthMetrics",
                  "contextualFactors": "environmentalData"
                },
                "outputMap": {
                  "symptomRecord": "healthEntry",
                  "severityAssessment": "intensityRating",
                  "timestampedData": "chronologicalRecord"
                }
              },
              {
                "id": "step-2",
                "name": "Analyze Historical Patterns",
                "description": "Examines past entries for trends and correlations",
                "subroutineId": "7829564732190848573",
                "inputMap": {
                  "healthEntry": "symptomRecord",
                  "historicalData": "pastRecords",
                  "analysisWindow": "timeframeScope"
                },
                "outputMap": {
                  "trendAnalysis": "patternIdentification",
                  "correlationMapping": "symptomRelationships",
                  "frequencyPatterns": "occurrenceAnalysis"
                }
              },
              {
                "id": "step-3",
                "name": "Identify Triggers & Factors",
                "description": "Discovers potential causes and contributing factors",
                "subroutineId": "7829564732190848576",
                "inputMap": {
                  "symptomRelationships": "correlationMapping",
                  "lifestyleData": "behaviorPatterns",
                  "environmentalFactors": "externalInfluences"
                },
                "outputMap": {
                  "triggerIdentification": "causativeFactors",
                  "riskFactorAnalysis": "contributingElements",
                  "preventionInsights": "avoidanceStrategies"
                }
              },
              {
                "id": "step-4",
                "name": "Generate Health Insights",
                "description": "Creates personalized health observations and advice",
                "subroutineId": "7829564732190848579",
                "inputMap": {
                  "patternIdentification": "trendAnalysis",
                  "causativeFactors": "triggerIdentification",
                  "healthGoals": "wellnessObjectives"
                },
                "outputMap": {
                  "healthInsights": "personalizedFindings",
                  "improvementSuggestions": "wellnessRecommendations",
                  "warningIndicators": "healthAlerts"
                }
              },
              {
                "id": "step-5",
                "name": "Create Medical Summary",
                "description": "Prepares documentation for healthcare providers",
                "subroutineId": "7829564732190848582",
                "inputMap": {
                  "chronologicalRecord": "timestampedData",
                  "personalizedFindings": "healthInsights",
                  "medicalFormat": "providerRequirements"
                },
                "outputMap": {
                  "medicalSummary": "providerReport",
                  "symptomTimeline": "chronologicalView",
                  "keyObservations": "clinicalHighlights"
                }
              },
              {
                "id": "step-6",
                "name": "Plan Health Actions",
                "description": "Develops actionable health improvement strategies",
                "subroutineId": "7829564732190848585",
                "inputMap": {
                  "wellnessRecommendations": "improvementSuggestions",
                  "avoidanceStrategies": "preventionInsights",
                  "personalPreferences": "lifestyleConstraints"
                },
                "outputMap": {
                  "actionPlan": "healthStrategy",
                  "habitModifications": "behaviorChanges",
                  "monitoringSchedule": "trackingPlan"
                }
              }
            ],
            "rootContext": {
              "inputMap": {
                "symptoms": "currentSymptoms",
                "history": "pastRecords",
                "lifestyle": "behaviorPatterns",
                "goals": "wellnessObjectives"
              },
              "outputMap": {
                "providerReport": "healthSummary",
                "healthStrategy": "actionPlan",
                "personalizedFindings": "healthInsights",
                "trackingPlan": "monitoringSystem"
              }
            },
            "executionMode": "sequential"
          }
        },
        "formInput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "symptoms",
                "id": "symptoms_input",
                "label": "Current Symptoms",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "Describe symptoms, severity, duration, and location"
              },
              {
                "fieldName": "trackingType",
                "id": "tracking_type_input",
                "label": "Tracking Focus",
                "type": "Selector",
                "isRequired": true,
                "options": [
                  "general-wellness",
                  "chronic-condition",
                  "pain-management",
                  "mental-health",
                  "allergies",
                  "medication-effects",
                  "fitness-recovery",
                  "sleep-quality",
                  "digestive-health"
                ]
              },
              {
                "fieldName": "history",
                "id": "history_input",
                "label": "Health History",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Previous symptoms, diagnoses, or health events"
              },
              {
                "fieldName": "vitals",
                "id": "vitals_input",
                "label": "Vital Measurements",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Blood pressure, heart rate, temperature, etc."
              },
              {
                "fieldName": "lifestyle",
                "id": "lifestyle_input",
                "label": "Lifestyle Factors",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Diet, exercise, sleep, stress levels"
              },
              {
                "fieldName": "goals",
                "id": "goals_input",
                "label": "Health Goals",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "What health improvements are you seeking?"
              },
              {
                "fieldName": "trackingPeriod",
                "id": "tracking_period_input",
                "label": "Analysis Period",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "last-month",
                "options": [
                  "last-week",
                  "last-month",
                  "last-3-months",
                  "last-6-months",
                  "last-year",
                  "all-time"
                ]
              },
              {
                "fieldName": "sensitivity",
                "id": "sensitivity_input",
                "label": "Alert Sensitivity",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "balanced",
                "options": [
                  "low",
                  "balanced",
                  "high",
                  "maximum"
                ]
              }
            ]
          }
        },
        "formOutput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "healthSummary",
                "id": "health_summary_output",
                "label": "Medical Provider Summary",
                "type": "JSON"
              },
              {
                "fieldName": "actionPlan",
                "id": "action_plan_output",
                "label": "Health Action Plan",
                "type": "JSON"
              },
              {
                "fieldName": "healthInsights",
                "id": "health_insights_output",
                "label": "Personalized Health Insights",
                "type": "JSON"
              },
              {
                "fieldName": "monitoringSystem",
                "id": "monitoring_system_output",
                "label": "Ongoing Tracking Plan",
                "type": "JSON"
              }
            ]
          }
        },
        "executionStrategy": "reasoning",
        "allowStrategyOverride": false,
        "subroutineStrategies": {
          "0": "deterministic",
          "1": "reasoning",
          "2": "reasoning",
          "3": "reasoning",
          "4": "conversational",
          "5": "reasoning"
        }
