              },
              {
                "id": "step-2",
                "name": "Track Daily Progress",
                "description": "Records habit completion and patterns",
                "subroutineId": "7829564732190848486",
                "inputMap": {
                  "trackingStructure": "habitFramework",
                  "dailyCheckIn": "completionStatus",
                  "contextFactors": "environmentalInfluences"
