              },
              {
                "id": "step-3",
                "name": "Analyze Patterns",
                "description": "Identifies trends and success factors",
                "subroutineId": "7829564732190848303",
                "inputMap": {
                  "completionHistory": "progressLog",
                  "behaviorTrends": "patternIdentification",
                  "successFactors": "enablerConditions"
                },
                "outputMap": {
                  "trendAnalysis": "habitPatterns",
                  "successTriggers": "positiveFactors",
                  "strugglingAreas": "challengeZones"
                }
              },
              {
                "id": "step-4",
                "name": "Provide Personalized Feedback",
                "description": "Generates coaching insights and encouragement",
                "subroutineId": "7829564732190848492",
                "inputMap": {
                  "habitPatterns": "trendAnalysis",
                  "positiveFactors": "successTriggers",
                  "coachingStyle": "feedbackPreferences"
                },
                "outputMap": {
                  "personalizedFeedback": "coachingInsights",
                  "motivationalMessages": "encouragement",
                  "improvementSuggestions": "optimizationTips"
                }
              },
              {
                "id": "step-5",
                "name": "Adapt Strategies",
                "description": "Adjusts approach based on performance",
                "subroutineId": "7829564732190848495",
                "inputMap": {
                  "challengeZones": "strugglingAreas",
                  "optimizationTips": "improvementSuggestions",
                  "adaptationStrategy": "adjustmentApproach"
                },
                "outputMap": {
                  "modifiedApproach": "adaptedStrategy",
                  "newTechniques": "enhancedMethods",
                  "supportSystems": "reinforcementMechanisms"
                }
              },
              {
                "id": "step-6",
                "name": "Generate Progress Report",
                "description": "Creates comprehensive habit summary and goals",
                "subroutineId": "7829564732190848498",
                "inputMap": {
                  "coachingInsights": "personalizedFeedback",
                  "adaptedStrategy": "modifiedApproach",
                  "reportingPeriod": "timeframe"
                },
                "outputMap": {
                  "progressReport": "habitSummary",
                  "achievementHighlights": "successCelebration",
                  "futureGoals": "nextSteps"
                }
              }
            ],
            "rootContext": {
              "inputMap": {
                "habits": "targetHabits",
                "checkins": "dailyUpdates",
                "preferences": "coachingStyle",
                "period": "trackingTimeframe"
