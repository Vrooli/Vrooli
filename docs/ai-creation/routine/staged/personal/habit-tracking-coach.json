                "placeholder": "What you want to achieve with these habits"
              },
              {
                "fieldName": "preferences",
                "id": "preferences_input",
                "label": "Coaching Style",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "encouraging",
                "options": [
                  "gentle",
                  "encouraging",
                  "direct",
                  "analytical",
                  "motivational"
                ]
              },
              {
                "fieldName": "trackingFrequency",
                "id": "tracking_frequency_input",
                "label": "Check-in Frequency",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "daily",
                "options": [
                  "daily",
                  "every-few-days",
                  "weekly",
                  "as-needed"
                ]
              },
              {
                "fieldName": "strugglingWith",
                "id": "struggling_with_input",
                "label": "Current Struggles",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Challenges or obstacles you're facing"
              },
              {
                "fieldName": "period",
                "id": "period_input",
                "label": "Tracking Period",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "monthly",
                "options": [
                  "weekly",
                  "monthly",
                  "quarterly",
                  "ongoing"
                ]
              }
            ]
          }
        },
        "formOutput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
