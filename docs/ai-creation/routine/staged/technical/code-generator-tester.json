  "versions": [
    {
      "id": "7829564732190847812",
      "publicId": "code-gen-test-v1-0",
      "versionLabel": "1.0.0",
      "versionNotes": "Initial version - Code generation, testing, and self-debugging system",
      "isComplete": true,
      "isPrivate": false,
      "versionIndex": 0,
      "isAutomatable": true,
      "resourceSubType": "RoutineMultiStep",
      "config": {
        "__version": "1.0",
        "graph": {
          "__type": "Sequential",
          "schema": {
            "steps": [
              {
                "id": "step-1",
                "name": "Analyze Requirements",
                "description": "Parses specifications and identifies technical requirements",
                "subroutineId": "7829564732190849302",
                "inputMap": {
                  "functionSpec": "requirementDescription",
                  "constraints": "technicalConstraints",
                  "examples": "inputOutputExamples"
                },
                "outputMap": {
                  "parsedRequirements": "technicalSpecs",
                  "functionSignature": "interfaceDefinition",
                  "edgeCases": "specialScenarios"
                }
              },
              {
                "id": "step-2",
                "name": "Design Code Architecture",
                "description": "Plans code structure and implementation approach",
                "subroutineId": "7829564732190849305",
                "inputMap": {
                  "technicalSpecs": "parsedRequirements",
                  "codePatterns": "designPrinciples",
                  "languageIdioms": "bestPractices"
                },
                "outputMap": {
                  "codeStructure": "architectureDesign",
                  "algorithmChoice": "implementationStrategy",
                  "dependencyList": "requiredLibraries"
                }
              },
              {
                "id": "step-3",
                "name": "Generate Implementation",
                "description": "Creates the actual code implementation",
                "subroutineId": "7829564732190849308",
                "inputMap": {
                  "architectureDesign": "codeStructure",
                  "interfaceDefinition": "functionSignature",
                  "codingStandards": "styleGuidelines"
                },
                "outputMap": {
                  "generatedCode": "implementation",
                  "documentationComments": "codeDocumentation",
                  "complexityMetrics": "codeAnalysis"
                }
              },
              {
                "id": "step-4",
                "name": "Create Test Suite",
                "description": "Generates comprehensive tests for the code",
                "subroutineId": "7829564732190849311",
                "inputMap": {
                  "implementation": "generatedCode",
                  "specialScenarios": "edgeCases",
                  "testFramework": "testingLibrary"
                },
                "outputMap": {
                  "testSuite": "generatedTests",
                  "testCoverage": "coverageReport",
                  "testScenarios": "testCaseDescriptions"
                }
              },
              {
                "id": "step-5",
                "name": "Execute & Debug Tests",
                "description": "Runs tests and attempts to fix failures automatically",
                "subroutineId": "7829564732190849332",
                "inputMap": {
                  "generatedTests": "testSuite",
                  "implementation": "generatedCode",
                  "debuggingStrategies": "fixApproaches"
                },
                "outputMap": {
                  "testResults": "executionReport",
                  "debuggingAttempts": "fixHistory",
                  "finalCode": "debuggedImplementation"
                }
              },
              {
                "id": "step-6",
                "name": "Optimize & Document",
                "description": "Refines code performance and completes documentation",
                "subroutineId": "7829564732190847219",
                "inputMap": {
                  "debuggedImplementation": "finalCode",
                  "performanceGoals": "optimizationTargets",
                  "documentationStyle": "docRequirements"
                },
                "outputMap": {
                  "optimizedCode": "finalImplementation",
                  "performanceReport": "optimizationResults",
                  "completeDocumentation": "finalDocumentation"
                }
              }
            ],
            "rootContext": {
              "inputMap": {
                "specification": "requirementDescription",
                "language": "programmingLanguage",
                "testCases": "inputOutputExamples",
                "style": "codingStandards"
              },
              "outputMap": {
                "finalImplementation": "generatedCode",
                "executionReport": "testResults",
                "finalDocumentation": "codeDocumentation",
                "optimizationResults": "performanceMetrics"
              }
            },
            "executionMode": "sequential"
          }
        },
        "formInput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "specification",
                "id": "specification_input",
                "label": "Function Specification",
                "type": "TextInput",
                "isRequired": true,
                "placeholder": "Describe what the function should do"
              },
              {
                "fieldName": "language",
                "id": "language_input",
                "label": "Programming Language",
                "type": "Selector",
                "isRequired": true,
                "options": [
                  "python",
                  "javascript",
                  "typescript",
                  "java",
                  "csharp",
                  "go",
                  "rust",
                  "cpp",
                  "ruby",
                  "php"
                ]
              },
              {
                "fieldName": "testCases",
                "id": "test_cases_input",
                "label": "Test Cases",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "Input/output examples and edge cases"
              },
              {
                "fieldName": "codeType",
                "id": "code_type_input",
                "label": "Code Type",
                "type": "Selector",
                "isRequired": true,
                "options": [
                  "function",
                  "class",
                  "module",
                  "api-endpoint",
                  "algorithm",
                  "data-structure",
                  "utility",
                  "full-application"
                ]
              },
              {
                "fieldName": "style",
                "id": "style_input",
                "label": "Coding Style",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Specific coding standards or style preferences"
              },
              {
                "fieldName": "complexity",
                "id": "complexity_input",
                "label": "Expected Complexity",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "moderate",
                "options": [
                  "simple",
                  "moderate",
                  "complex",
                  "highly-complex"
                ]
              },
              {
                "fieldName": "optimization",
                "id": "optimization_input",
                "label": "Optimization Priority",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "balanced",
                "options": [
                  "readability",
                  "performance",
                  "memory",
                  "balanced",
                  "maintainability"
                ]
              },
              {
                "fieldName": "testingDepth",
                "id": "testing_depth_input",
                "label": "Testing Depth",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "comprehensive",
                "options": [
                  "basic",
                  "standard",
                  "comprehensive",
                  "exhaustive"
                ]
              }
            ]
          }
        },
        "formOutput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "generatedCode",
                "id": "generated_code_output",
                "label": "Final Generated Code",
                "type": "JSON"
              },
              {
                "fieldName": "testResults",
                "id": "test_results_output",
                "label": "Test Execution Report",
                "type": "JSON"
              },
              {
                "fieldName": "codeDocumentation",
                "id": "code_documentation_output",
                "label": "Complete Documentation",
                "type": "JSON"
              },
              {
                "fieldName": "performanceMetrics",
                "id": "performance_metrics_output",
                "label": "Performance Analysis",
                "type": "JSON"
              }
            ]
          }
        },
        "executionStrategy": "reasoning",
        "allowStrategyOverride": false,
        "subroutineStrategies": {
          "0": "reasoning",
          "1": "reasoning",
          "2": "reasoning",
          "3": "reasoning",
          "4": "deterministic",
          "5": "reasoning"
        }
      },
      "translations": [
        {
          "id": "7829564732190847813",
          "language": "en",
          "name": "Code Generator & Tester",
          "description": "Advanced code generation system that analyzes requirements, designs architecture, generates implementation, creates test suites, executes debugging cycles, and optimizes performance with comprehensive documentation.",
          "instructions": "Describe the function you need and provide test cases. Select programming language and code type. Specify any style preferences and optimization priorities. The routine will generate code, create tests, debug failures, and optimize the implementation."
        }
      ]
    }
  ]
