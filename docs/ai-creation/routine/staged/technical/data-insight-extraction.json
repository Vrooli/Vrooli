      "publicId": "data-insight-v1-0",
      "versionLabel": "1.0.0",
      "versionNotes": "Initial version - Pattern and insight extraction from datasets",
      "isComplete": true,
      "isPrivate": false,
      "versionIndex": 0,
      "isAutomatable": true,
      "resourceSubType": "RoutineMultiStep",
      "config": {
        "__version": "1.0",
        "graph": {
          "__type": "Sequential",
          "schema": {
            "steps": [
              {
                "id": "step-1",
                "name": "Profile Dataset",
                "description": "Analyzes data structure and characteristics",
                "subroutineId": "TODO: Need data profiling subroutine",
                "inputMap": {
                  "dataset": "inputData",
                  "dataTypes": "columnTypes",
                  "profilingDepth": "analysisLevel"
                },
                "outputMap": {
                  "dataProfile": "structuralAnalysis",
                  "statisticalSummary": "descriptiveStats",
                  "qualityReport": "dataQuality"
                }
              },
              {
                "id": "step-2",
                "name": "Detect Patterns",
                "description": "Identifies trends, cycles, and correlations",
                "subroutineId": "TODO: Need pattern detection subroutine",
                "inputMap": {
                  "structuralAnalysis": "dataProfile",
                  "patternTypes": "searchTargets",
                  "sensitivityLevel": "detectionThreshold"
                },
                "outputMap": {
                  "identifiedPatterns": "trendAnalysis",
                  "correlationMatrix": "variableRelationships",
                  "seasonality": "cyclicalPatterns"
                }
              },
              {
                "id": "step-3",
                "name": "Find Anomalies",
                "description": "Locates outliers and unusual data points",
                "subroutineId": "TODO: Need anomaly detection subroutine",
                "inputMap": {
                  "descriptiveStats": "statisticalSummary",
                  "anomalyMethods": "detectionAlgorithms",
                  "contextualFactors": "businessRules"
                },
                "outputMap": {
                  "anomalies": "outlierList",
                  "anomalyScores": "deviationMetrics",
                  "anomalyContext": "outlierExplanations"
                }
              },
              {
                "id": "step-4",
                "name": "Generate Insights",
                "description": "Translates patterns into business insights",
                "subroutineId": "TODO: Need insight generation subroutine",
                "inputMap": {
                  "trendAnalysis": "identifiedPatterns",
                  "businessContext": "domainKnowledge",
                  "insightPriorities": "focusAreas"
                },
                "outputMap": {
                  "keyInsights": "actionableFindings",
                  "insightRationale": "evidenceBase",
                  "confidenceLevels": "insightCertainty"
                }
              },
              {
                "id": "step-5",
                "name": "Create Visualizations",
                "description": "Recommends charts and graphs for insights",
                "subroutineId": "TODO: Need visualization recommendation subroutine",
                "inputMap": {
                  "actionableFindings": "keyInsights",
                  "dataCharacteristics": "visualizationContext",
                  "audienceLevel": "presentationStyle"
                },
                "outputMap": {
                  "visualizationSpecs": "chartRecommendations",
                  "dashboardLayout": "visualArrangement",
                  "narrativeFlow": "storyStructure"
                }
              },
              {
                "id": "step-6",
                "name": "Compile Report",
                "description": "Assembles comprehensive insight report",
                "subroutineId": "TODO: Need report compilation subroutine",
                "inputMap": {
                  "chartRecommendations": "visualizationSpecs",
                  "evidenceBase": "insightRationale",
                  "reportTemplate": "documentFormat"
                },
                "outputMap": {
                  "insightReport": "completeAnalysis",
                  "executiveSummary": "keyTakeaways",
                  "recommendations": "nextSteps"
                }
              }
            ],
            "rootContext": {
              "inputMap": {
                "data": "inputData",
                "objectives": "analysisGoals",
                "context": "businessContext",
                "format": "outputPreferences"
              },
              "outputMap": {
                "completeAnalysis": "insightOutcome",
                "keyTakeaways": "executiveSummary",
                "chartRecommendations": "visualizations",
                "nextSteps": "recommendations"
              }
            },
            "executionMode": "sequential"
          }
        },
        "formInput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "data",
                "id": "data_input",
                "label": "Dataset",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "Structured or unstructured data for analysis"
              },
              {
                "fieldName": "dataFormat",
                "id": "data_format_input",
                "label": "Data Format",
                "type": "Selector",
                "isRequired": true,
                "options": [
                  "tabular",
                  "time-series",
                  "hierarchical",
                  "graph",
                  "mixed",
                  "unstructured"
                ]
              },
              {
                "fieldName": "objectives",
                "id": "objectives_input",
                "label": "Analysis Objectives",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "What insights are you looking for?"
              },
              {
                "fieldName": "analysisDepth",
                "id": "analysis_depth_input",
                "label": "Analysis Depth",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "comprehensive",
                "options": [
                  "quick",
                  "standard",
                  "comprehensive",
                  "exhaustive"
                ]
              },
              {
                "fieldName": "context",
                "id": "context_input",
                "label": "Business Context",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Industry, domain knowledge, business rules"
              },
              {
                "fieldName": "focusAreas",
                "id": "focus_areas_input",
                "label": "Focus Areas",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "all",
                "options": [
                  "trends",
                  "anomalies",
                  "correlations",
                  "predictions",
                  "segments",
                  "all"
                ]
              },
              {
                "fieldName": "confidenceThreshold",
                "id": "confidence_threshold_input",
                "label": "Confidence Threshold",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "medium",
                "options": [
                  "low",
                  "medium",
                  "high",
                  "very-high"
                ]
              },
              {
                "fieldName": "format",
                "id": "format_input",
                "label": "Output Format",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "report",
                "options": [
                  "summary",
                  "report",
                  "dashboard",
                  "presentation"
                ]
              }
            ]
          }
        },
        "formOutput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "insightOutcome",
                "id": "insight_outcome_output",
                "label": "Complete Insight Analysis",
                "type": "JSON"
              },
              {
                "fieldName": "executiveSummary",
                "id": "executive_summary_output",
                "label": "Key Takeaways",
                "type": "JSON"
              },
              {
                "fieldName": "visualizations",
                "id": "visualizations_output",
                "label": "Visualization Recommendations",
                "type": "JSON"
              },
              {
                "fieldName": "recommendations",
                "id": "recommendations_output",
                "label": "Next Steps & Actions",
                "type": "JSON"
              }
            ]
          }
        },
        "executionStrategy": "deterministic",
        "allowStrategyOverride": false,
        "subroutineStrategies": {
          "0": "deterministic",
          "1": "deterministic",
          "2": "deterministic",
