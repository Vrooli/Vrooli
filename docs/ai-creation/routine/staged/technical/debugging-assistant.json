      "translations": [
        {
          "id": "7829564732190847852",
          "language": "en",
          "name": "Debugging Assistant",
          "description": "Intelligent debugging system that analyzes error context, identifies root causes, generates solution hypotheses, tests fixes, implements optimal solutions, and documents resolutions with prevention guidelines.",
          "instructions": "Provide error description and affected code. Specify error type and programming language. Include environment details and previous attempts. The routine will analyze the issue, find root causes, test solutions, and provide working fixes with prevention strategies."
        }
      ]
