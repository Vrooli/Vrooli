  "versions": [
    {
      "id": "7829564732190847926",
      "publicId": "debug-assist-v1-0",
      "versionLabel": "1.0.0",
      "versionNotes": "Initial version - Code debugging and bug fixing assistance system",
      "isComplete": true,
      "isPrivate": false,
      "versionIndex": 0,
      "isAutomatable": true,
      "resourceSubType": "RoutineMultiStep",
      "config": {
        "__version": "1.0",
        "graph": {
          "__type": "Sequential",
          "schema": {
            "steps": [
              {
                "id": "step-1",
                "name": "Analyze Error Context",
                "description": "Examines error messages, code, and execution environment",
                "subroutineId": "7829564732190849353",
                "inputMap": {
                  "sourceCode": "codeToDebug",
                  "errorMessages": "bugReports",
                  "executionContext": "runtimeEnvironment"
                },
                "outputMap": {
                  "errorAnalysis": "bugBreakdown",
                  "codeStructure": "programFlow",
                  "environmentFactors": "contextualIssues"
                }
              },
              {
                "id": "step-2",
                "name": "Identify Root Causes",
                "description": "Traces through code to find the source of the problem",
                "subroutineId": "7829564732190849356",
                "inputMap": {
                  "bugBreakdown": "errorAnalysis",
                  "programFlow": "codeStructure",
                  "debuggingHints": "investigationClues"
                },
                "outputMap": {
                  "rootCauses": "problemSources",
                  "executionPath": "bugLocation",
                  "contributingFactors": "relatedIssues"
                }
              },
              {
                "id": "step-3",
                "name": "Generate Fix Strategies",
                "description": "Develops multiple approaches to resolve the identified issues",
                "subroutineId": "7829564732190849359",
                "inputMap": {
                  "problemSources": "rootCauses",
                  "bugLocation": "executionPath",
                  "codeConstraints": "fixingLimitations"
                },
                "outputMap": {
                  "fixStrategies": "solutionApproaches",
                  "riskAssessment": "implementationRisks",
                  "priorityOrder": "recommendedSequence"
                }
              },
              {
                "id": "step-4",
                "name": "Implement Code Fixes",
                "description": "Creates specific code changes to address the problems",
                "subroutineId": "7829564732190849362",
                "inputMap": {
                  "solutionApproaches": "fixStrategies",
                  "codeToDebug": "sourceCode",
                  "implementationStyle": "codingPreferences"
                },
                "outputMap": {
                  "codeChanges": "fixedCode",
                  "modificationExplanations": "changeJustifications",
                  "testingSuggestions": "validationSteps"
                }
              },
              {
                "id": "step-5",
                "name": "Validate Solutions",
                "description": "Tests fixes and checks for potential side effects",
                "subroutineId": "7829564732190849383",
                "inputMap": {
                  "fixedCode": "codeChanges",
                  "validationSteps": "testingSuggestions",
                  "qualityChecks": "validationCriteria"
                },
                "outputMap": {
                  "testResults": "validationOutcome",
                  "sideEffects": "unintendedConsequences",
                  "performanceImpact": "optimizationEffects"
                }
              },
              {
                "id": "step-6",
                "name": "Create Debug Report",
                "description": "Compiles comprehensive debugging documentation",
                "subroutineId": "7829564732190849338",
                "inputMap": {
                  "changeJustifications": "modificationExplanations",
                  "validationOutcome": "testResults",
                  "preventionTips": "futureAvoidanceStrategies"
                },
                "outputMap": {
                  "debugReport": "troubleshootingSummary",
                  "implementationGuide": "stepByStepInstructions",
                  "preventionAdvice": "bestPractices"
                }
              }
            ],
            "rootContext": {
              "inputMap": {
                "code": "codeToDebug",
                "errors": "bugReports",
                "context": "runtimeEnvironment",
                "preferences": "debuggingPreferences"
              },
              "outputMap": {
                "troubleshootingSummary": "debuggingOutcome",
                "stepByStepInstructions": "implementationPlan",
                "bestPractices": "preventionGuidance",
                "fixedCode": "resolvedCode"
              }
            },
            "executionMode": "sequential"
          }
        },
        "formInput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "code",
                "id": "code_input",
                "label": "Code with Bug",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "Source code that contains the bug or error"
              },
              {
                "fieldName": "errors",
                "id": "errors_input",
                "label": "Error Messages",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "Error messages, stack traces, or bug descriptions"
              },
              {
                "fieldName": "language",
                "id": "language_input",
                "label": "Programming Language",
                "type": "Selector",
                "isRequired": true,
                "options": [
                  "javascript",
                  "python",
                  "java",
                  "csharp",
                  "cpp",
                  "rust",
                  "go",
                  "typescript",
                  "php",
                  "ruby",
                  "swift",
                  "kotlin"
                ]
              },
              {
                "fieldName": "context",
                "id": "context_input",
                "label": "Runtime Context",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Environment details, dependencies, runtime conditions"
              },
              {
                "fieldName": "expectedBehavior",
                "id": "expected_behavior_input",
                "label": "Expected Behavior",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "What the code should do when working correctly"
              },
              {
                "fieldName": "debuggingLevel",
                "id": "debugging_level_input",
                "label": "Debugging Depth",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "thorough",
                "options": [
                  "quick-fix",
                  "standard",
                  "thorough",
                  "comprehensive"
                ]
              },
              {
                "fieldName": "fixPreferences",
                "id": "fix_preferences_input",
                "label": "Fix Preferences",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Coding style, performance priorities, compatibility requirements"
              },
              {
                "fieldName": "testingCapability",
                "id": "testing_capability_input",
                "label": "Testing Capability",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "manual",
                "options": [
                  "none",
                  "manual",
                  "unit-tests",
                  "full-suite",
                  "automated"
                ]
              }
            ]
          }
        },
        "formOutput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "debuggingOutcome",
                "id": "debugging_outcome_output",
                "label": "Complete Debug Analysis",
                "type": "JSON"
              },
              {
                "fieldName": "implementationPlan",
                "id": "implementation_plan_output",
                "label": "Step-by-Step Fix Instructions",
                "type": "JSON"
              },
              {
                "fieldName": "preventionGuidance",
                "id": "prevention_guidance_output",
                "label": "Best Practices & Prevention",
                "type": "JSON"
              },
              {
                "fieldName": "resolvedCode",
                "id": "resolved_code_output",
                "label": "Fixed Code Implementation",
                "type": "JSON"
              }
            ]
          }
        },
        "executionStrategy": "reasoning",
        "allowStrategyOverride": false,
        "subroutineStrategies": {
          "0": "reasoning",
          "1": "reasoning",
          "2": "reasoning",
          "3": "reasoning",
          "4": "deterministic",
          "5": "conversational"
        }
      },
      "translations": [
        {
          "id": "7829564732190847927",
          "language": "en",
          "name": "Debugging Assistant",
          "description": "Comprehensive code debugging and bug fixing assistance system. Analyzes error context, identifies root causes, generates fix strategies, implements code solutions, validates fixes, and creates detailed debugging reports.",
          "instructions": "Provide code with bugs and error messages. Specify programming language and runtime context. Include expected behavior and fix preferences. The routine will analyze the problem, identify causes, and provide tested solutions with implementation guidance."
        }
      ]
    }
  ]
