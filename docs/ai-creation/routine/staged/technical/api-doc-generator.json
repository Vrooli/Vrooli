        }
      ]
    }
  ],
  "versions": [
    {
      "id": "7829564732190847923",
      "publicId": "api-docs-gen-v1-0",
      "versionLabel": "1.0.0",
      "versionNotes": "Initial version - API documentation generation from code and specifications",
      "isComplete": true,
      "isPrivate": false,
      "versionIndex": 0,
      "isAutomatable": true,
      "resourceSubType": "RoutineMultiStep",
      "config": {
        "__version": "1.0",
        "graph": {
          "__type": "Sequential",
          "schema": {
            "steps": [
              {
                "id": "step-1",
                "name": "Parse API Structure",
                "description": "Analyzes code or specification files to extract API endpoints",
                "subroutineId": "7829564732190849371",
                "inputMap": {
                  "sourceCode": "apiDefinition",
                  "specificationFiles": "configurationData",
                  "parsingMethod": "extractionApproach"
                },
                "outputMap": {
                  "endpointCatalog": "apiStructure",
                  "methodDetails": "operationSpecs",
                  "dataModels": "schemaDefinitions"
                }
              },
              {
                "id": "step-2",
                "name": "Extract Documentation Elements",
                "description": "Identifies existing comments, annotations, and documentation",
                "subroutineId": "7829564732190849374",
                "inputMap": {
                  "apiStructure": "endpointCatalog",
                  "codeComments": "inlineDocumentation",
                  "existingDocs": "currentDocumentation"
                },
                "outputMap": {
                  "extractedDescriptions": "foundDocumentation",
                  "missingElements": "documentationGaps",
                  "qualityAssessment": "docCompleteness"
                }
              },
              {
                "id": "step-3",
                "name": "Generate Missing Documentation",
                "description": "Creates descriptions for undocumented endpoints and parameters",
                "subroutineId": "7829564732190849377",
                "inputMap": {
                  "documentationGaps": "missingElements",
                  "operationSpecs": "methodDetails",
                  "documentationStyle": "writingGuidelines"
                },
                "outputMap": {
                  "generatedDescriptions": "newDocumentation",
                  "parameterExplanations": "fieldDescriptions",
                  "usageExamples": "codeSnippets"
                }
              },
              {
                "id": "step-4",
                "name": "Create Interactive Examples",
                "description": "Develops request/response examples and usage demonstrations",
                "subroutineId": "7829564732190849341",
                "inputMap": {
                  "schemaDefinitions": "dataModels",
                  "fieldDescriptions": "parameterExplanations",
                  "exampleComplexity": "demonstrationLevel"
                },
                "outputMap": {
                  "requestExamples": "sampleRequests",
                  "responseExamples": "sampleResponses",
                  "useCaseScenarios": "practicalExamples"
                }
              },
              {
                "id": "step-5",
                "name": "Format Documentation",
                "description": "Structures content according to documentation standards",
                "subroutineId": "7829564732190849380",
                "inputMap": {
                  "foundDocumentation": "extractedDescriptions",
                  "newDocumentation": "generatedDescriptions",
                  "outputFormat": "documentationFormat"
                },
                "outputMap": {
                  "formattedSections": "structuredContent",
                  "navigationStructure": "documentationIndex",
                  "crossReferences": "linkingStructure"
                }
              },
              {
                "id": "step-6",
                "name": "Generate Final Documentation",
                "description": "Compiles complete API documentation package",
                "subroutineId": "7829564732190849392",
                "inputMap": {
                  "structuredContent": "formattedSections",
                  "sampleRequests": "requestExamples",
                  "practicalExamples": "useCaseScenarios"
                },
                "outputMap": {
                  "completeDocs": "apiDocumentation",
                  "quickStartGuide": "gettingStarted",
                  "validationReport": "qualityChecklist"
                }
              }
            ],
            "rootContext": {
              "inputMap": {
                "source": "apiDefinition",
                "format": "documentationFormat",
                "style": "writingGuidelines",
                "audience": "targetUsers"
              },
              "outputMap": {
                "apiDocumentation": "documentationOutcome",
                "gettingStarted": "quickReference",
                "qualityChecklist": "completenessReport",
                "practicalExamples": "usageGuide"
              }
            },
            "executionMode": "sequential"
          }
        },
        "formInput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "source",
                "id": "source_input",
                "label": "API Source",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "Source code, OpenAPI spec, or existing documentation"
              },
              {
                "fieldName": "apiType",
                "id": "api_type_input",
                "label": "API Type",
                "type": "Selector",
                "isRequired": true,
                "options": [
                  "rest",
                  "graphql",
                  "grpc",
                  "soap",
                  "websocket",
                  "rpc",
                  "mixed"
                ]
              },
              {
                "fieldName": "format",
                "id": "format_input",
                "label": "Documentation Format",
                "type": "Selector",
                "isRequired": true,
                "options": [
                  "markdown",
                  "html",
                  "openapi",
                  "postman",
                  "insomnia",
                  "readme",
                  "confluence"
                ]
              },
              {
                "fieldName": "audience",
                "id": "audience_input",
                "label": "Target Audience",
                "type": "Selector",
                "isRequired": true,
                "options": [
                  "developers",
                  "technical-writers",
                  "product-managers",
                  "qa-testers",
                  "mixed-audience"
                ]
              },
              {
                "fieldName": "style",
                "id": "style_input",
                "label": "Documentation Style",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Writing guidelines, tone, and formatting preferences"
              },
              {
                "fieldName": "includeExamples",
                "id": "include_examples_input",
                "label": "Include Code Examples",
                "type": "Switch",
                "isRequired": false,
                "defaultValue": true
              },
              {
                "fieldName": "exampleLanguages",
                "id": "example_languages_input",
                "label": "Example Languages",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Programming languages for code examples (curl, python, javascript, etc.)"
              },
              {
                "fieldName": "completenessLevel",
                "id": "completeness_level_input",
                "label": "Documentation Depth",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "comprehensive",
                "options": [
                  "basic",
                  "standard",
                  "comprehensive",
                  "enterprise"
                ]
              }
            ]
          }
        },
        "formOutput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "documentationOutcome",
                "id": "documentation_outcome_output",
                "label": "Complete API Documentation",
                "type": "JSON"
              },
              {
                "fieldName": "quickReference",
                "id": "quick_reference_output",
                "label": "Quick Start Guide",
                "type": "JSON"
              },
              {
                "fieldName": "completenessReport",
                "id": "completeness_report_output",
                "label": "Documentation Quality Report",
                "type": "JSON"
              },
              {
                "fieldName": "usageGuide",
                "id": "usage_guide_output",
                "label": "Usage Examples & Scenarios",
                "type": "JSON"
              }
            ]
          }
        },
        "executionStrategy": "reasoning",
        "allowStrategyOverride": false,
        "subroutineStrategies": {
          "0": "deterministic",
          "1": "reasoning",
          "2": "conversational",
          "3": "reasoning",
          "4": "deterministic",
          "5": "conversational"
        }
      },
      "translations": [
        {
          "id": "7829564732190847924",
          "language": "en",
          "name": "API Documentation Generator",
          "description": "Comprehensive API documentation generation system. Parses API structure, extracts existing documentation, generates missing descriptions, creates interactive examples, formats content, and produces complete documentation packages.",
          "instructions": "Provide API source code or specifications. Choose documentation format and target audience. Set style preferences and example languages. The routine will analyze your API and generate comprehensive documentation with examples and usage guides."
        }
      ]
    }
  ]
}
