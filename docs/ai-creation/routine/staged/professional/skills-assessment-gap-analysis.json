  "versions": [
    {
      "id": "7829564732190847740",
      "publicId": "skills-assess-v1-0",
      "versionLabel": "1.0.0",
      "versionNotes": "Initial version - Comprehensive skills evaluation and development planning",
      "isComplete": true,
      "isPrivate": false,
      "versionIndex": 0,
      "isAutomatable": true,
      "resourceSubType": "RoutineCode",
      "config": {
        "__version": "1.0",
        "callDataCode": {
          "__version": "1.0",
          "schema": {
            "inputTemplate": {
              "currentSkills": "{{input.skillInventory}}",
              "targetRole": "{{input.desiredPosition}}",
              "careerGoals": "{{input.professionalObjectives}}",
              "timeframe": "{{input.developmentTimeline}}",
              "learningStyle": "{{input.preferredLearning}}",
              "constraints": "{{input.developmentConstraints}}"
            },
            "outputMapping": {
              "skillsMatrix": "result.matrix",
              "gapAnalysis": "result.gaps",
              "developmentPlan": "result.plan",
              "learningPath": "result.learning",
              "prioritySkills": "result.priorities",
              "progressFramework": "result.tracking"
            }
          }
        },
        "formInput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "skillInventory",
                "id": "skill_inventory_input",
                "label": "Current Skills Inventory",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "List your current skills with proficiency levels"
              },
              {
                "fieldName": "desiredPosition",
                "id": "desired_position_input",
                "label": "Target Role/Position",
                "type": "TextInput",
                "isRequired": true,
                "placeholder": "What role or position are you targeting?"
              },
              {
                "fieldName": "professionalObjectives",
                "id": "professional_objectives_input",
                "label": "Career Goals",
                "type": "JSON",
                "isRequired": true,
                "placeholder": "What are your professional development goals?"
              },
              {
                "fieldName": "developmentTimeline",
                "id": "development_timeline_input",
                "label": "Development Timeline",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "6-months",
                "options": [
                  "3-months",
                  "6-months",
                  "1-year",
                  "2-years",
                  "3-years",
                  "ongoing"
                ]
              },
              {
                "fieldName": "preferredLearning",
                "id": "preferred_learning_input",
                "label": "Learning Preferences",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "How do you prefer to learn new skills?"
              },
              {
                "fieldName": "developmentConstraints",
                "id": "development_constraints_input",
                "label": "Development Constraints",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Time, budget, or other limitations"
              },
              {
                "fieldName": "industryContext",
                "id": "industry_context_input",
                "label": "Industry/Field",
                "type": "TextInput",
                "isRequired": false,
                "placeholder": "What industry or field are you in?"
              }
            ]
          }
        },
        "formOutput": {
          "__version": "1.0",
          "schema": {
            "containers": [],
            "elements": [
              {
                "fieldName": "skillsMatrix",
                "id": "skills_matrix_output",
                "label": "Skills Competency Matrix",
                "type": "JSON"
              },
              {
                "fieldName": "gapAnalysis",
                "id": "gap_analysis_output",
                "label": "Skills Gap Analysis",
                "type": "JSON"
              },
              {
                "fieldName": "developmentPlan",
                "id": "development_plan_output",
                "label": "Skills Development Plan",
                "type": "JSON"
              },
              {
                "fieldName": "learningPath",
                "id": "learning_path_output",
                "label": "Personalized Learning Path",
                "type": "JSON"
              },
              {
                "fieldName": "prioritySkills",
                "id": "priority_skills_output",
                "label": "Priority Skills Focus",
                "type": "JSON"
              },
              {
                "fieldName": "progressFramework",
                "id": "progress_framework_output",
                "label": "Progress Tracking Framework",
                "type": "JSON"
              }
            ]
          }
        },
        "executionStrategy": "reasoning"
      },
      "translations": [
        {
          "id": "7829564732190847741",
          "language": "en",
          "name": "Skills Assessment & Gap Analysis",
          "description": "Comprehensive skills evaluation system that creates competency matrices, identifies skill gaps, develops personalized learning paths, prioritizes development areas, and establishes progress tracking for professional growth.",
          "instructions": "Document your current skills and target position. Specify career goals and development timeline. The routine will analyze skill gaps, create development plans, recommend learning paths, and establish tracking frameworks for systematic skill advancement."
        }
      ]
    }
  ]
}
