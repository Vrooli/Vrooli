              {
                "fieldName": "conflictType",
                "id": "conflict_type_input",
                "label": "Conflict Type",
                "type": "Selector",
                "isRequired": true,
                "options": [
                  "interpersonal",
                  "workplace",
                  "family",
                  "business",
                  "community",
                  "organizational",
                  "legal-dispute",
                  "resource-allocation"
                ]
              },
              {
                "fieldName": "urgency",
                "id": "urgency_input",
                "label": "Urgency Level",
                "type": "Selector",
                "isRequired": true,
                "options": [
                  "low",
                  "moderate",
                  "high",
                  "critical"
                ]
              },
              {
                "fieldName": "preferences",
                "id": "preferences_input",
                "label": "Mediation Preferences",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Any specific requirements for the mediation process?"
              },
              {
                "fieldName": "previousAttempts",
                "id": "previous_attempts_input",
                "label": "Previous Resolution Attempts",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "What has been tried before?"
              },
              {
                "fieldName": "constraints",
                "id": "constraints_input",
                "label": "Limitations & Constraints",
                "type": "JSON",
                "isRequired": false,
                "placeholder": "Any legal, financial, or other constraints to consider?"
              },
              {
                "fieldName": "timeline",
                "id": "timeline_input",
                "label": "Resolution Timeline",
                "type": "Selector",
                "isRequired": false,
                "defaultValue": "flexible",
                "options": [
                  "immediate",
                  "1-week",
                  "1-month",
                  "3-months",
                  "flexible"
                ]
              }
            ]
          }
        },
