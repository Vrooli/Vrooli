{
  "id": "1234567890123456804",
  "publicId": "csv-json-convert-template",
  "resourceType": "Routine",
  "isPrivate": false,
  "permissions": "{}",
  "isInternal": false,
  "tags": [],
  "versions": [
    {
      "id": "1234567890123456805",
      "publicId": "csv-json-convert-v1",
      "versionLabel": "1.0.0",
      "versionNotes": "Template for CSV to JSON data conversion",
      "isComplete": true,
      "isPrivate": false,
      "versionIndex": 0,
      "isAutomatable": true,
      "resourceSubType": "RoutineData",
      "config": {
        "__version": "1.0",
        "callDataData": {
          "__version": "1.0",
          "schema": {
            "operation": "transform",
            "inputFormat": "csv",
            "outputFormat": "json",
            "transformationRules": {
              "mappings": [
                {
                  "source": "csv_headers",
                  "target": "json_fields",
                  "transform": "fieldMapping"
                }
              ],
              "csvOptions": {
                "delimiter": "{{input.delimiter}}",
                "hasHeaders": "{{input.hasHeaders}}",
                "encoding": "{{input.encoding}}"
              }
            },
            "validationRules": {
              "required": "{{input.requiredColumns}}",
              "types": "{{input.columnTypes}}",
              "constraints": {
                "maxRows": "{{input.maxRows}}",
                "skipEmpty": "{{input.skipEmptyRows}}"
              }
            },
            "options": {
              "preserveMetadata": true,
              "errorHandling": "{{input.errorHandling}}",
              "batchSize": "{{input.batchSize}}"
            }
          }
        },
        "formInput": {
          "__version": "1.0",
          "schema": {
            "elements": [
              {
                "fieldName": "csvData",
                "id": "csv_data_input",
                "label": "CSV Data",
                "type": "TextArea",
                "isRequired": true,
                "placeholder": "Paste CSV data here"
              },
              {
                "fieldName": "delimiter",
                "id": "delimiter_input",
                "label": "CSV Delimiter",
                "type": "Selector",
                "options": [",", ";", "\t", "|"],
                "defaultValue": ","
              },
              {
                "fieldName": "hasHeaders",
                "id": "has_headers_input",
                "label": "Has Header Row",
                "type": "Checkbox",
                "defaultValue": true
              },
              {
                "fieldName": "encoding",
                "id": "encoding_input",
                "label": "Text Encoding",
                "type": "Selector",
                "options": ["utf-8", "ascii", "latin1"],
                "defaultValue": "utf-8"
              },
              {
                "fieldName": "errorHandling",
                "id": "error_handling_input",
                "label": "Error Handling",
                "type": "Selector",
                "options": ["strict", "lenient", "skip"],
                "defaultValue": "lenient"
              },
              {
                "fieldName": "maxRows",
                "id": "max_rows_input",
                "label": "Maximum Rows",
                "type": "IntegerInput",
                "defaultValue": 10000,
                "min": 1,
                "max": 100000
              }
            ]
          }
        },
        "formOutput": {
          "__version": "1.0",
          "schema": {
            "elements": [
              {
                "fieldName": "jsonData",
                "id": "json_data_output",
                "label": "Converted JSON Data",
                "type": "JSON"
              },
              {
                "fieldName": "recordCount",
                "id": "record_count_output",
                "label": "Records Converted",
                "type": "IntegerInput"
              },
              {
                "fieldName": "conversionMetadata",
                "id": "conversion_metadata_output",
                "label": "Conversion Metadata",
                "type": "JSON"
              },
              {
                "fieldName": "errors",
                "id": "errors_output",
                "label": "Conversion Errors",
                "type": "JSON"
              }
            ]
          }
        },
        "executionStrategy": "deterministic"
      },
      "translations": [
        {
          "id": "1234567890123456806",
          "language": "en",
          "name": "CSV to JSON Converter",
          "description": "Converts CSV data to JSON format with configurable delimiters, encoding, and error handling. Provides validation and conversion metrics for data quality assurance.",
          "instructions": "Paste CSV data and configure parsing options. Specify delimiter, encoding, and error handling preferences. The converter will process the data and provide structured JSON output."
        }
      ]
    }
  ]
}