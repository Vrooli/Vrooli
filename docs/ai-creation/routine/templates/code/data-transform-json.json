      "config": {
        "__version": "1.0",
        "callDataCode": {
          "__version": "1.0",
          "schema": {
            "inputTemplate": {
              "sourceData": "{{input.jsonData}}",
              "transformationRules": "{{input.transformRules}}",
              "options": {
                "preserveKeys": "{{input.preserveKeys}}",
                "validateOutput": true
              }
            },
            "outputMappings": [
              {
                "schemaIndex": 0,
                "mapping": {
                  "transformedData": "result",
                  "recordCount": "metadata.count",
                  "processingTime": "metadata.timeMs",
                  "errors": "metadata.errors"
                }
              }
            ],
            "language": "javascript",
            "timeoutMs": 30000,
            "memoryLimitMb": 128
          }
        },
        "formInput": {
          "__version": "1.0",
          "schema": {
            "elements": [
              {
                "fieldName": "jsonData",
                "id": "json_data_input",
