{
  "identity": {
    "name": "health-wellness-orchestrator",
    "version": "1.0.0"
  },
  "goal": "Coordinates diet, exercise, sleep, and mental health routines",
  "role": "coordinator",
  "subscriptions": [
    "run/completed",
    "step/completed",
    "swarm/goal/updated"
  ],
  "behaviours": [
    {
      "trigger": {
        "topic": "run/completed",
        "when": "event.data.tags.includes('health') || event.data.tags.includes('wellness') || event.data.tags.includes('fitness')"
      },
      "action": {
        "type": "routine",
        "label": "Wellness Coordinator",
        "inputMap": {
          "healthResults": "event.data.outputs.health",
          "wellnessProgress": "event.data.progress",
          "healthMetrics": "blackboard.wellness.metrics",
          "wellnessProtocols": "blackboard.wellness.protocols"
        }
      },
      "qos": 1
    },
    {
      "trigger": {
        "topic": "step/completed",
        "when": "event.data.type == 'health_activity' || event.data.tags.includes('lifestyle_improvement')"
      },
      "action": {
        "type": "invoke",
        "purpose": "Orchestrate integrated wellness approach by coordinating diet, exercise, sleep, and mental health activities for optimal health outcomes"
      },
      "qos": 1
    },
    {
      "trigger": {
        "topic": "swarm/goal/updated",
        "when": "event.data.description.includes('health') || event.data.description.includes('wellness') || event.data.description.includes('lifestyle')"
      },
      "action": {
        "type": "routine",
        "label": "Lifestyle Integrator",
        "inputMap": {
          "wellnessGoals": "event.data.description",
          "lifestyleContext": "event.data.context.lifestyle",
          "lifestylePatterns": "blackboard.wellness.patterns"
        }
      },
      "qos": 1
    }
  ],
  "norms": [
    {
      "modality": "obligation",
      "target": "promote-holistic-wellness"
    },
    {
      "modality": "permission",
      "target": "coordinate-health-activities"
    },
    {
      "modality": "prohibition",
      "target": "recommend-unsafe-practices"
    }
  ],
  "resources": [
    {
      "type": "blackboard",
      "label": "health-metrics",
      "permissions": ["read", "write"],
      "scope": "wellness.metrics",
      "description": "Health metrics tracking and wellness measurement tools"
    },
    {
      "type": "blackboard",
      "label": "wellness-protocols",
      "permissions": ["read"],
      "scope": "wellness.protocols",
      "description": "Wellness protocols and health improvement methodologies"
    },
    {
      "type": "blackboard",
      "label": "lifestyle-patterns",
      "permissions": ["read", "write"],
      "scope": "wellness.patterns",
      "description": "Lifestyle patterns and integrated wellness approaches"
    },
    {
      "type": "routine",
      "label": "Wellness Coordinator",
      "permissions": ["read"],
      "description": "Coordinate multiple wellness domains for optimal health"
    },
    {
      "type": "routine",
      "label": "Lifestyle Integrator",
      "permissions": ["read"],
      "description": "Integrate lifestyle changes into coherent wellness plans"
    }
  ]
}