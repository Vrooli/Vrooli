{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "VerticalPackageConfiguration",
  "description": "Industry vertical package configurations for specialized swarm deployments",
  "type": "object",
  "properties": {
    "dental": { "$ref": "#/definitions/DentalConfig" },
    "landscaping": { "$ref": "#/definitions/LandscapingConfig" },
    "nursery": { "$ref": "#/definitions/NurseryConfig" },
    "onlineRetail": { "$ref": "#/definitions/OnlineRetailConfig" },
    "consulting": { "$ref": "#/definitions/ConsultingConfig" },
    "saasTools": { "$ref": "#/definitions/SaaSToolsConfig" },
    "healthcare": { "$ref": "#/definitions/HealthcareConfig" },
    "legal": { "$ref": "#/definitions/LegalConfig" },
    "education": { "$ref": "#/definitions/EducationConfig" },
    "manufacturing": { "$ref": "#/definitions/ManufacturingConfig" }
  },
  "definitions": {
    "DentalConfig": {
      "type": "object",
      "description": "Dental office automation and management configuration",
      "properties": {
        "compliance": {
          "type": "object",
          "properties": {
            "hipaaRequired": { "type": "boolean", "default": true },
            "dataRetentionYears": { "type": "number", "default": 7 },
            "encryptionRequired": { "type": "boolean", "default": true },
            "auditLogging": { "type": "boolean", "default": true }
          }
        },
        "workflows": {
          "type": "array",
          "description": "Standard dental office workflows",
          "items": {
            "type": "object",
            "properties": {
              "name": { "type": "string" },
              "description": { "type": "string" },
              "automationLevel": {
                "type": "string",
                "enum": ["manual", "assisted", "automated"]
              },
              "routineIds": {
                "type": "array",
                "items": { "type": "string" }
              }
            },
            "required": ["name", "automationLevel"]
          },
          "default": [
            {
              "name": "appointment-scheduling",
              "description": "Automated appointment booking and reminders",
              "automationLevel": "automated"
            },
            {
              "name": "insurance-verification",
              "description": "Insurance eligibility and benefits verification",
              "automationLevel": "assisted"
            },
            {
              "name": "treatment-planning",
              "description": "Treatment plan generation and cost estimation",
              "automationLevel": "assisted"
            },
            {
              "name": "follow-up-care",
              "description": "Post-treatment follow-up and care instructions",
              "automationLevel": "automated"
            }
          ]
        },
        "integrations": {
          "type": "object",
          "properties": {
            "practiceManagement": {
              "type": "array",
              "items": { "type": "string" },
              "default": ["Dentrix", "Eaglesoft", "OpenDental", "Curve"]
            },
            "imaging": {
              "type": "array",
              "items": { "type": "string" },
              "default": ["Dexis", "Schick", "Kodak"]
            },
            "billing": {
              "type": "array",
              "items": { "type": "string" },
              "default": ["Henry Schein", "Patterson", "Benco"]
            }
          }
        },
        "kpis": {
          "type": "object",
          "properties": {
            "patientSatisfaction": { "type": "number", "minimum": 4.0, "default": 4.5 },
            "appointmentUtilization": { "type": "number", "minimum": 80, "default": 90 },
            "insuranceApprovalRate": { "type": "number", "minimum": 85, "default": 95 },
            "followUpCompliance": { "type": "number", "minimum": 80, "default": 85 }
          }
        }
      },
      "required": ["compliance", "workflows"],
      "additionalProperties": false
    },
    "LandscapingConfig": {
      "type": "object",
      "description": "Landscaping business automation and management",
      "properties": {
        "seasonalOperations": {
          "type": "object",
          "properties": {
            "springServices": {
              "type": "array",
              "items": { "type": "string" },
              "default": ["cleanup", "planting", "mulching", "fertilizing"]
            },
            "summerServices": {
              "type": "array",
              "items": { "type": "string" },
              "default": ["mowing", "watering", "pest-control", "pruning"]
            },
            "fallServices": {
              "type": "array",
              "items": { "type": "string" },
              "default": ["leaf-removal", "winterization", "tree-care"]
            },
            "winterServices": {
              "type": "array",
              "items": { "type": "string" },
              "default": ["snow-removal", "ice-management", "equipment-maintenance"]
            }
          }
        },
        "workflows": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": { "type": "string" },
              "description": { "type": "string" },
              "seasonality": {
                "type": "array",
                "items": { "type": "string", "enum": ["spring", "summer", "fall", "winter"] }
              },
              "automationLevel": {
                "type": "string",
                "enum": ["manual", "assisted", "automated"]
              }
            },
            "required": ["name", "automationLevel"]
          },
          "default": [
            {
              "name": "route-optimization",
              "description": "Optimize daily service routes for efficiency",
              "automationLevel": "automated"
            },
            {
              "name": "weather-scheduling",
              "description": "Adjust schedules based on weather conditions",
              "automationLevel": "automated"
            },
            {
              "name": "equipment-tracking",
              "description": "Track equipment usage and maintenance needs",
              "automationLevel": "assisted"
            },
            {
              "name": "customer-communication",
              "description": "Service updates and appointment confirmations",
              "automationLevel": "automated"
            }
          ]
        },
        "kpis": {
          "type": "object",
          "properties": {
            "customerRetention": { "type": "number", "minimum": 85, "default": 90 },
            "routeEfficiency": { "type": "number", "minimum": 80, "default": 85 },
            "weatherAdaptability": { "type": "number", "minimum": 90, "default": 95 },
            "equipmentUtilization": { "type": "number", "minimum": 75, "default": 80 }
          }
        }
      },
      "required": ["seasonalOperations", "workflows"],
      "additionalProperties": false
    },
    "NurseryConfig": {
      "type": "object",
      "description": "Plant nursery management and automation",
      "properties": {
        "plantCategories": {
          "type": "array",
          "items": { "type": "string" },
          "default": ["annuals", "perennials", "shrubs", "trees", "houseplants", "herbs", "vegetables"]
        },
        "careAutomation": {
          "type": "object",
          "properties": {
            "wateringSchedules": { "type": "boolean", "default": true },
            "fertilizationTracking": { "type": "boolean", "default": true },
            "pestMonitoring": { "type": "boolean", "default": true },
            "growthTracking": { "type": "boolean", "default": true },
            "inventoryManagement": { "type": "boolean", "default": true }
          }
        },
        "workflows": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": { "type": "string" },
              "description": { "type": "string" },
              "frequency": { "type": "string", "enum": ["daily", "weekly", "monthly", "seasonal"] },
              "automationLevel": { "type": "string", "enum": ["manual", "assisted", "automated"] }
            },
            "required": ["name", "frequency", "automationLevel"]
          },
          "default": [
            {
              "name": "watering-management",
              "description": "Automated watering based on plant needs and weather",
              "frequency": "daily",
              "automationLevel": "automated"
            },
            {
              "name": "inventory-tracking",
              "description": "Track plant inventory and reorder triggers",
              "frequency": "daily",
              "automationLevel": "automated"
            },
            {
              "name": "growth-monitoring",
              "description": "Monitor plant growth and health indicators",
              "frequency": "weekly",
              "automationLevel": "assisted"
            },
            {
              "name": "customer-recommendations",
              "description": "Recommend plants based on customer needs",
              "frequency": "daily",
              "automationLevel": "automated"
            }
          ]
        },
        "kpis": {
          "type": "object",
          "properties": {
            "plantSurvivalRate": { "type": "number", "minimum": 85, "default": 92 },
            "inventoryAccuracy": { "type": "number", "minimum": 95, "default": 98 },
            "customerSatisfaction": { "type": "number", "minimum": 4.0, "default": 4.3 },
            "wasteReduction": { "type": "number", "minimum": 15, "default": 20 }
          }
        }
      },
      "required": ["plantCategories", "careAutomation", "workflows"],
      "additionalProperties": false
    },
    "OnlineRetailConfig": {
      "type": "object",
      "description": "E-commerce and online retail automation",
      "properties": {
        "platforms": {
          "type": "array",
          "items": { "type": "string" },
          "default": ["shopify", "woocommerce", "magento", "bigcommerce", "amazon", "ebay"]
        },
        "workflows": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": { "type": "string" },
              "description": { "type": "string" },
              "priority": { "type": "string", "enum": ["high", "medium", "low"] },
              "automationLevel": { "type": "string", "enum": ["manual", "assisted", "automated"] }
            },
            "required": ["name", "priority", "automationLevel"]
          },
          "default": [
            {
              "name": "order-processing",
              "description": "Automated order fulfillment and tracking",
              "priority": "high",
              "automationLevel": "automated"
            },
            {
              "name": "inventory-management",
              "description": "Real-time inventory tracking and reorder automation",
              "priority": "high",
              "automationLevel": "automated"
            },
            {
              "name": "customer-service",
              "description": "Automated customer support and inquiry handling",
              "priority": "medium",
              "automationLevel": "assisted"
            },
            {
              "name": "pricing-optimization",
              "description": "Dynamic pricing based on competition and demand",
              "priority": "medium",
              "automationLevel": "assisted"
            }
          ]
        },
        "kpis": {
          "type": "object",
          "properties": {
            "orderFulfillmentTime": { "type": "number", "maximum": 24, "default": 12 },
            "inventoryAccuracy": { "type": "number", "minimum": 95, "default": 99 },
            "customerResponseTime": { "type": "number", "maximum": 2, "default": 1 },
            "conversionRate": { "type": "number", "minimum": 2, "default": 3.5 }
          }
        }
      },
      "required": ["platforms", "workflows"],
      "additionalProperties": false
    },
    "ConsultingConfig": {
      "type": "object",
      "description": "Consulting services automation and project management",
      "properties": {
        "serviceTypes": {
          "type": "array",
          "items": { "type": "string" },
          "default": ["strategy", "implementation", "optimization", "training", "auditing"]
        },
        "projectPhases": {
          "type": "array",
          "items": { "type": "string" },
          "default": ["discovery", "analysis", "planning", "execution", "delivery", "support"]
        },
        "workflows": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": { "type": "string" },
              "description": { "type": "string" },
              "phase": { "type": "string" },
              "automationLevel": { "type": "string", "enum": ["manual", "assisted", "automated"] }
            },
            "required": ["name", "phase", "automationLevel"]
          },
          "default": [
            {
              "name": "project-scoping",
              "description": "Automated project scope and timeline estimation",
              "phase": "discovery",
              "automationLevel": "assisted"
            },
            {
              "name": "deliverable-tracking",
              "description": "Track deliverable completion and quality",
              "phase": "execution",
              "automationLevel": "automated"
            },
            {
              "name": "client-communication",
              "description": "Regular client updates and milestone reports",
              "phase": "execution",
              "automationLevel": "automated"
            },
            {
              "name": "knowledge-capture",
              "description": "Capture and document project learnings",
              "phase": "delivery",
              "automationLevel": "assisted"
            }
          ]
        },
        "kpis": {
          "type": "object",
          "properties": {
            "projectOnTime": { "type": "number", "minimum": 85, "default": 90 },
            "clientSatisfaction": { "type": "number", "minimum": 4.0, "default": 4.5 },
            "utilizationRate": { "type": "number", "minimum": 75, "default": 80 },
            "repeatClientRate": { "type": "number", "minimum": 60, "default": 70 }
          }
        }
      },
      "required": ["serviceTypes", "projectPhases", "workflows"],
      "additionalProperties": false
    },
    "SaaSToolsConfig": {
      "type": "object",
      "description": "SaaS tool development and optimization",
      "properties": {
        "developmentStages": {
          "type": "array",
          "items": { "type": "string" },
          "default": ["ideation", "mvp", "beta", "launch", "growth", "scale"]
        },
        "workflows": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": { "type": "string" },
              "description": { "type": "string" },
              "stage": { "type": "string" },
              "automationLevel": { "type": "string", "enum": ["manual", "assisted", "automated"] }
            },
            "required": ["name", "stage", "automationLevel"]
          },
          "default": [
            {
              "name": "feature-development",
              "description": "Automated feature planning and development tracking",
              "stage": "growth",
              "automationLevel": "assisted"
            },
            {
              "name": "user-onboarding",
              "description": "Automated user onboarding and tutorial sequences",
              "stage": "launch",
              "automationLevel": "automated"
            },
            {
              "name": "performance-monitoring",
              "description": "Real-time performance and usage analytics",
              "stage": "scale",
              "automationLevel": "automated"
            },
            {
              "name": "customer-feedback",
              "description": "Collect and analyze customer feedback for improvements",
              "stage": "growth",
              "automationLevel": "assisted"
            }
          ]
        },
        "kpis": {
          "type": "object",
          "properties": {
            "featureAdoption": { "type": "number", "minimum": 40, "default": 60 },
            "userOnboardingCompletion": { "type": "number", "minimum": 70, "default": 85 },
            "systemUptime": { "type": "number", "minimum": 99, "default": 99.9 },
            "customerLifetimeValue": { "type": "number", "minimum": 100, "default": 500 }
          }
        }
      },
      "required": ["developmentStages", "workflows"],
      "additionalProperties": false
    },
    "HealthcareConfig": {
      "type": "object",
      "description": "Healthcare practice automation (beyond dental)",
      "properties": {
        "compliance": {
          "type": "object",
          "properties": {
            "hipaaRequired": { "type": "boolean", "default": true },
            "hitech": { "type": "boolean", "default": true },
            "dataRetentionYears": { "type": "number", "default": 10 },
            "encryptionRequired": { "type": "boolean", "default": true },
            "auditLogging": { "type": "boolean", "default": true }
          }
        },
        "specialties": {
          "type": "array",
          "items": { "type": "string" },
          "default": ["family-medicine", "cardiology", "dermatology", "orthopedics", "pediatrics"]
        },
        "workflows": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": { "type": "string" },
              "description": { "type": "string" },
              "priority": { "type": "string", "enum": ["critical", "high", "medium", "low"] },
              "automationLevel": { "type": "string", "enum": ["manual", "assisted", "automated"] }
            },
            "required": ["name", "priority", "automationLevel"]
          },
          "default": [
            {
              "name": "appointment-scheduling",
              "description": "Intelligent appointment scheduling with provider matching",
              "priority": "high",
              "automationLevel": "automated"
            },
            {
              "name": "clinical-documentation",
              "description": "Assisted clinical note generation and coding",
              "priority": "critical",
              "automationLevel": "assisted"
            },
            {
              "name": "medication-management",
              "description": "Drug interaction checking and prescription management",
              "priority": "critical",
              "automationLevel": "assisted"
            }
          ]
        }
      },
      "required": ["compliance", "workflows"],
      "additionalProperties": false
    },
    "LegalConfig": {
      "type": "object",
      "description": "Legal practice automation and case management",
      "properties": {
        "practiceAreas": {
          "type": "array",
          "items": { "type": "string" },
          "default": ["corporate", "litigation", "family", "real-estate", "intellectual-property"]
        },
        "compliance": {
          "type": "object",
          "properties": {
            "clientPrivilege": { "type": "boolean", "default": true },
            "conflictChecking": { "type": "boolean", "default": true },
            "documentRetention": { "type": "number", "default": 7 },
            "ethicsCompliance": { "type": "boolean", "default": true }
          }
        },
        "workflows": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": { "type": "string" },
              "description": { "type": "string" },
              "practiceArea": { "type": "string" },
              "automationLevel": { "type": "string", "enum": ["manual", "assisted", "automated"] }
            },
            "required": ["name", "automationLevel"]
          },
          "default": [
            {
              "name": "document-review",
              "description": "Automated document analysis and clause extraction",
              "practiceArea": "corporate",
              "automationLevel": "assisted"
            },
            {
              "name": "case-research",
              "description": "Legal research and precedent analysis",
              "practiceArea": "litigation",
              "automationLevel": "assisted"
            },
            {
              "name": "billing-timetracking",
              "description": "Automated time tracking and billing generation",
              "practiceArea": "all",
              "automationLevel": "automated"
            }
          ]
        }
      },
      "required": ["practiceAreas", "compliance", "workflows"],
      "additionalProperties": false
    },
    "EducationConfig": {
      "type": "object",
      "description": "Educational institution automation and management",
      "properties": {
        "institutionType": {
          "type": "string",
          "enum": ["k12", "community-college", "university", "online", "vocational"],
          "description": "Type of educational institution"
        },
        "compliance": {
          "type": "object",
          "properties": {
            "ferpa": { "type": "boolean", "default": true },
            "accessibility": { "type": "boolean", "default": true },
            "dataRetention": { "type": "number", "default": 5 }
          }
        },
        "workflows": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": { "type": "string" },
              "description": { "type": "string" },
              "stakeholder": { "type": "string", "enum": ["student", "faculty", "admin", "parent"] },
              "automationLevel": { "type": "string", "enum": ["manual", "assisted", "automated"] }
            },
            "required": ["name", "stakeholder", "automationLevel"]
          },
          "default": [
            {
              "name": "enrollment-management",
              "description": "Automated enrollment and course registration",
              "stakeholder": "student",
              "automationLevel": "automated"
            },
            {
              "name": "grading-assistance",
              "description": "Automated grading and feedback generation",
              "stakeholder": "faculty",
              "automationLevel": "assisted"
            },
            {
              "name": "attendance-tracking",
              "description": "Automated attendance monitoring and alerts",
              "stakeholder": "admin",
              "automationLevel": "automated"
            }
          ]
        }
      },
      "required": ["institutionType", "compliance", "workflows"],
      "additionalProperties": false
    },
    "ManufacturingConfig": {
      "type": "object",
      "description": "Manufacturing operations automation and optimization",
      "properties": {
        "manufacturingType": {
          "type": "string",
          "enum": ["discrete", "process", "mixed-mode", "job-shop", "assembly"],
          "description": "Type of manufacturing operation"
        },
        "workflows": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": { "type": "string" },
              "description": { "type": "string" },
              "department": { "type": "string", "enum": ["production", "quality", "maintenance", "planning", "logistics"] },
              "automationLevel": { "type": "string", "enum": ["manual", "assisted", "automated"] }
            },
            "required": ["name", "department", "automationLevel"]
          },
          "default": [
            {
              "name": "production-scheduling",
              "description": "Optimize production schedules and resource allocation",
              "department": "planning",
              "automationLevel": "automated"
            },
            {
              "name": "quality-monitoring",
              "description": "Real-time quality control and defect detection",
              "department": "quality",
              "automationLevel": "automated"
            },
            {
              "name": "predictive-maintenance",
              "description": "Equipment maintenance scheduling and failure prediction",
              "department": "maintenance",
              "automationLevel": "assisted"
            }
          ]
        },
        "kpis": {
          "type": "object",
          "properties": {
            "oee": { "type": "number", "minimum": 60, "default": 85, "description": "Overall Equipment Effectiveness" },
            "qualityRate": { "type": "number", "minimum": 95, "default": 99.5 },
            "onTimeDelivery": { "type": "number", "minimum": 90, "default": 95 },
            "inventoryTurnover": { "type": "number", "minimum": 6, "default": 12 }
          }
        }
      },
      "required": ["manufacturingType", "workflows"],
      "additionalProperties": false
    }
  }
}