{
  "type": "function",
  "category": "git",
  "function": {
    "name": "git_push",
    "description": "Push commits to remote repository",
    "parameters": {
      "type": "object",
      "properties": {
        "remote": {
          "type": "string",
          "description": "Remote name",
          "default": "origin"
        },
        "branch": {
          "type": "string",
          "description": "Branch to push (defaults to current branch)"
        },
        "force": {
          "type": "boolean",
          "description": "Force push (DANGEROUS - use with extreme caution)",
          "default": false
        },
        "set_upstream": {
          "type": "boolean",
          "description": "Set upstream tracking for the branch",
          "default": false
        },
        "path": {
          "type": "string",
          "description": "Repository path",
          "default": "."
        }
      }
    }
  }
}