{
  "name": "test-data-generator-api",
  "version": "1.0.0",
  "description": "Test Data Generator API - Node.js service for generating mock test data",
  "main": "server.js",
  "scripts": {
    "start": "bash -c '${VROOLI_ROOT:-$(git rev-parse --show-toplevel)}/scripts/lib/ui-guard.sh node server.js'",
    "dev": "bash -c '${VROOLI_ROOT:-$(git rev-parse --show-toplevel)}/scripts/lib/ui-guard.sh node --watch server.js'",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:verbose": "NODE_ENV=test jest --verbose"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "@faker-js/faker": "^8.0.0",
    "uuid": "^9.0.0",
    "joi": "^17.9.2",
    "csv-writer": "^1.6.0",
    "js2xmlparser": "^5.0.0"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "nodemon": "^2.0.22"
  },
  "keywords": [
    "test-data",
    "mock-data",
    "generator",
    "api",
    "development",
    "testing"
  ],
  "author": "Vrooli",
  "license": "MIT"
}