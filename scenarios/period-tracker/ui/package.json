{
  "name": "period-tracker-ui",
  "version": "1.0.0",
  "description": "Privacy-first menstrual health tracking UI",
  "main": "server.js",
  "scripts": {
    "start": "bash -c '${VROOLI_ROOT:-$(git rev-parse --show-toplevel)}/scripts/lib/ui-guard.sh node server.js'",
    "dev": "bash -c '${VROOLI_ROOT:-$(git rev-parse --show-toplevel)}/scripts/lib/ui-guard.sh node server.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "bash -c 'set -euo pipefail; rm -rf dist && mkdir -p dist && if [ -f index.html ]; then cp index.html dist/index.html; fi && if [ -f styles.css ]; then cp styles.css dist/styles.css; fi && for asset in app.js script.js main.js; do if [ -f \"$asset\" ]; then cp \"$asset\" \"dist/$asset\"; fi; done'"
  },
  "keywords": [
    "menstrual-health",
    "period-tracking",
    "privacy",
    "health",
    "women"
  ],
  "author": "Vrooli AI Agent",
  "license": "MIT",
  "dependencies": {
    "@vrooli/iframe-bridge": "file:../../../packages/iframe-bridge",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  }
}
