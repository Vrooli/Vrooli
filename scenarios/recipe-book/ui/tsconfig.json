{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": false,
    "noEmitOnError": false,
    "skipLibCheck": true,
    "allowJs": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "types": [
      "vite/client"
    ],
    "paths": {
      "@vrooli/iframe-bridge": [
        "../../../packages/iframe-bridge/dist/index"
      ],
      "@vrooli/iframe-bridge/*": [
        "../../../packages/iframe-bridge/dist/*"
      ]
    }
  },
  "include": [
    "src"
  ],
  "references": []
}