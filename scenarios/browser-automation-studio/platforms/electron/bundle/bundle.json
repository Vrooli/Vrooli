{
  "app": {
    "description": "Visual browser automation studio with AI-powered workflow builder and real-time debugging",
    "name": "Vrooli Ascension",
    "version": "1.0.0"
  },
  "ipc": {
    "auth_token_path": "runtime/auth_token",
    "host": "127.0.0.1",
    "mode": "loopback-http",
    "port": 39200
  },
  "ports": {
    "default_range": {
      "max": 24000,
      "min": 20000
    },
    "reserved": []
  },
  "schema_version": "v0.1",
  "services": [
    {
      "binaries": {
        "darwin-x64": {
          "path": "api/browser-automation-studio-api"
        },
        "linux-x64": {
          "path": "api/browser-automation-studio-api"
        },
        "win-x64": {
          "path": "api/browser-automation-studio-api.exe"
        }
      },
      "data_dirs": [
        "data/api"
      ],
      "description": "Bundled API process for browser-automation-studio",
      "health": {
        "interval_ms": 2000,
        "path": "/health",
        "port_name": "api",
        "retries": 5,
        "timeout_ms": 15000,
        "type": "http"
      },
      "id": "browser-automation-studio-api",
      "log_dir": "logs/api",
      "ports": {
        "requested": [
          {
            "name": "api",
            "range": {
              "max": 23200,
              "min": 23100
            },
            "requires_socket": false
          }
        ]
      },
      "readiness": {
        "pattern": "",
        "port_name": "api",
        "timeout_ms": 30000,
        "type": "port_open"
      },
      "type": "api-binary"
    },
    {
      "assets": [
        {
          "path": "ui/dist",
          "sha256": "pending",
          "size_bytes": 0
        },
        {
          "path": "ui/dist/index.html",
          "sha256": "b13969b54fca202e3a13b008b98c01dba195d89318a1789a2f1c7db8cdbcc540",
          "size_bytes": 2623
        }
      ],
      "binaries": {
        "darwin-x64": {
          "path": "ui/dist/index.html"
        },
        "linux-x64": {
          "path": "ui/dist/index.html"
        },
        "win-x64": {
          "path": "ui/dist/index.html"
        }
      },
      "critical": true,
      "description": "Production UI bundle served locally",
      "health": {
        "interval_ms": 2000,
        "path": "/",
        "port_name": "ui",
        "retries": 3,
        "timeout_ms": 10000,
        "type": "http"
      },
      "id": "ui",
      "log_dir": "logs/ui",
      "ports": {
        "requested": [
          {
            "name": "ui",
            "range": {
              "max": 24200,
              "min": 24100
            },
            "requires_socket": false
          }
        ]
      },
      "readiness": {
        "pattern": "",
        "port_name": "ui",
        "timeout_ms": 20000,
        "type": "health_success"
      },
      "type": "ui-bundle"
    }
  ],
  "swaps": [
    {
      "limitations": "",
      "original": "minio",
      "reason": "Recommended bundle-safe alternative from dependency metadata",
      "replacement": "filesystem-store"
    },
    {
      "limitations": "",
      "original": "ollama",
      "reason": "Recommended bundle-safe alternative from dependency metadata",
      "replacement": "openrouter"
    },
    {
      "limitations": "",
      "original": "postgres",
      "reason": "Recommended bundle-safe alternative from dependency metadata",
      "replacement": "sqlite"
    },
    {
      "limitations": "",
      "original": "redis",
      "reason": "Recommended bundle-safe alternative from dependency metadata",
      "replacement": "event-emitter"
    }
  ],
  "target": "desktop",
  "telemetry": {
    "file": "telemetry/deployment-telemetry.jsonl",
    "upload_url": ""
  }
}