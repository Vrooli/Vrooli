{
  "edges": [
    {
      "id": "e0",
      "source": "navigate-dashboard",
      "target": "dismiss-tutorial",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e1",
      "source": "dismiss-tutorial",
      "target": "click-projects-tab",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e2",
      "source": "click-projects-tab",
      "target": "click-new-project",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e3",
      "source": "click-new-project",
      "target": "assert-modal-visible",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e4",
      "source": "assert-modal-visible",
      "target": "generate-project-name",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e5",
      "source": "generate-project-name",
      "target": "fill-project-name",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e6",
      "source": "fill-project-name",
      "target": "expand-advanced-options",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e6b",
      "source": "expand-advanced-options",
      "target": "fill-project-description",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e7",
      "source": "fill-project-description",
      "target": "click-create",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e8",
      "source": "click-create",
      "target": "wait-for-navigation",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e8b",
      "source": "wait-for-navigation",
      "target": "assert-workflows-tab-visible",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e10",
      "source": "assert-workflows-tab-visible",
      "target": "assert-new-workflow-button-visible",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    }
  ],
  "metadata": {
    "description": "Creates a new project and verifies navigation to project detail page",
    "labels": {
      "reset": "full"
    },
    "version": "1"
  },
  "nodes": [
    {
      "action": {
        "metadata": {
          "label": "Open BAS dashboard"
        },
        "navigate": {
          "destination_type": "NAVIGATE_DESTINATION_TYPE_SCENARIO",
          "scenario": "browser-automation-studio",
          "scenario_path": "/",
          "timeout_ms": 45000,
          "wait_until": "NAVIGATE_WAIT_EVENT_NETWORKIDLE"
        },
        "type": "ACTION_TYPE_NAVIGATE"
      },
      "execution_settings": {
        "wait_after_ms": 1200
      },
      "id": "navigate-dashboard"
    },
    {
      "action": {
        "metadata": {
          "label": "Dismiss onboarding tutorial if present"
        },
        "subflow": {
          "workflow_path": "actions/dismiss-tutorial.json"
        },
        "type": "ACTION_TYPE_SUBFLOW"
      },
      "id": "dismiss-tutorial"
    },
    {
      "action": {
        "click": {
          "selector": "@selector/dashboard.tabs.projects"
        },
        "metadata": {
          "label": "Navigate to Projects tab"
        },
        "type": "ACTION_TYPE_CLICK"
      },
      "execution_settings": {
        "wait_after_ms": 500
      },
      "id": "click-projects-tab"
    },
    {
      "action": {
        "click": {
          "selector": "@selector/dashboard.newProjectButton"
        },
        "metadata": {
          "label": "Open new project modal"
        },
        "type": "ACTION_TYPE_CLICK"
      },
      "execution_settings": {
        "resilience": {
          "success_selector": "@selector/dialogs.project.root",
          "success_timeout_ms": 10000
        }
      },
      "id": "click-new-project"
    },
    {
      "action": {
        "assert": {
          "mode": "ASSERTION_MODE_EXISTS",
          "selector": "@selector/dialogs.project.root",
          "timeout_ms": 10000
        },
        "metadata": {
          "label": "Project modal visible"
        },
        "type": "ACTION_TYPE_ASSERT"
      },
      "id": "assert-modal-visible"
    },
    {
      "action": {
        "evaluate": {
          "expression": "`Test Project ${Date.now()}`",
          "store_result": "projectName"
        },
        "metadata": {
          "label": "Generate unique project name"
        },
        "type": "ACTION_TYPE_EVALUATE"
      },
      "id": "generate-project-name"
    },
    {
      "action": {
        "input": {
          "clear_first": true,
          "selector": "@selector/dialogs.project.nameInput",
          "value": "${@store/projectName}"
        },
        "metadata": {
          "label": "Enter project name"
        },
        "type": "ACTION_TYPE_INPUT"
      },
      "id": "fill-project-name"
    },
    {
      "action": {
        "click": {
          "selector": "@selector/dialogs.project.advancedOptionsToggle"
        },
        "metadata": {
          "label": "Expand advanced options"
        },
        "type": "ACTION_TYPE_CLICK"
      },
      "execution_settings": {
        "wait_after_ms": 300
      },
      "id": "expand-advanced-options"
    },
    {
      "action": {
        "input": {
          "clear_first": true,
          "selector": "@selector/dialogs.project.descriptionInput",
          "value": "Automated test project created by workflow validation"
        },
        "metadata": {
          "label": "Enter project description"
        },
        "type": "ACTION_TYPE_INPUT"
      },
      "id": "fill-project-description"
    },
    {
      "action": {
        "click": {
          "selector": "@selector/dialogs.project.submitButton"
        },
        "metadata": {
          "label": "Click Create button"
        },
        "type": "ACTION_TYPE_CLICK"
      },
      "execution_settings": {
        "wait_after_ms": 500
      },
      "id": "click-create"
    },
    {
      "action": {
        "metadata": {
          "label": "Wait for navigation and page render"
        },
        "type": "ACTION_TYPE_WAIT",
        "wait": {
          "duration_ms": 2000
        }
      },
      "id": "wait-for-navigation"
    },
    {
      "action": {
        "assert": {
          "failure_message": "Workflows tab should be visible on project detail page",
          "mode": "ASSERTION_MODE_EXISTS",
          "selector": "@selector/workflows.tab",
          "timeout_ms": 10000
        },
        "metadata": {
          "label": "Workflows tab is visible"
        },
        "type": "ACTION_TYPE_ASSERT"
      },
      "id": "assert-workflows-tab-visible"
    },
    {
      "action": {
        "assert": {
          "failure_message": "New workflow button should be visible on workflows tab",
          "mode": "ASSERTION_MODE_EXISTS",
          "selector": "@selector/workflows.newButton",
          "timeout_ms": 5000
        },
        "metadata": {
          "label": "New workflow button is visible"
        },
        "type": "ACTION_TYPE_ASSERT"
      },
      "id": "assert-new-workflow-button-visible"
    }
  ],
  "settings": {
    "viewport_height": 900,
    "viewport_width": 1440
  }
}
