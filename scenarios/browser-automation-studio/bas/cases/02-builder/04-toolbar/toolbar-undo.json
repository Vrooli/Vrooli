{
  "edges": [
    {
      "id": "call-open-demo-workflow-to-count1",
      "source": "call-open-demo-workflow",
      "target": "count1",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e5",
      "source": "count1",
      "target": "click-palette",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e6",
      "source": "click-palette",
      "target": "click-canvas",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e7",
      "source": "click-canvas",
      "target": "wait2",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e8",
      "source": "wait2",
      "target": "count2",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e9",
      "source": "count2",
      "target": "undo",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e10",
      "source": "undo",
      "target": "count3",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    }
  ],
  "metadata": {
    "description": "Validates undo button reverts the last canvas change",
    "labels": {
      "reset": "full"
    },
    "version": "1"
  },
  "nodes": [
    {
      "action": {
        "metadata": {
          "label": "Open seeded workflow via fixture"
        },
        "subflow": {
          "args": {
            "projectFolder": "${@params/projectFolder}",
            "projectName": "${@params/projectName}",
            "workflowName": "${@params/workflowName}"
          },
          "workflow_path": "actions/open-demo-workflow.json"
        },
        "type": "ACTION_TYPE_SUBFLOW"
      },
      "id": "call-open-demo-workflow"
    },
    {
      "action": {
        "evaluate": {
          "expression": "document.querySelectorAll('.react-flow__node').length",
          "store_result": "initialCount"
        },
        "metadata": {
          "label": "Count initial nodes"
        },
        "type": "ACTION_TYPE_EVALUATE"
      },
      "id": "count1"
    },
    {
      "action": {
        "click": {
          "selector": "@selector/nodePalette.cards.navigate"
        },
        "metadata": {
          "label": "Click navigate node"
        },
        "type": "ACTION_TYPE_CLICK"
      },
      "execution_settings": {
        "wait_after_ms": 500
      },
      "id": "click-palette"
    },
    {
      "action": {
        "click": {
          "selector": ".react-flow__pane"
        },
        "metadata": {
          "label": "Click canvas to add"
        },
        "type": "ACTION_TYPE_CLICK"
      },
      "id": "click-canvas"
    },
    {
      "action": {
        "metadata": {
          "label": "Wait for node added"
        },
        "type": "ACTION_TYPE_WAIT",
        "wait": {
          "duration_ms": 1000
        }
      },
      "id": "wait2"
    },
    {
      "action": {
        "evaluate": {
          "expression": "document.querySelectorAll('.react-flow__node').length",
          "store_result": "afterAddCount"
        },
        "metadata": {
          "label": "Count after add"
        },
        "type": "ACTION_TYPE_EVALUATE"
      },
      "id": "count2"
    },
    {
      "action": {
        "click": {
          "selector": "@selector/toolbar.undoButton"
        },
        "metadata": {
          "label": "Click undo"
        },
        "type": "ACTION_TYPE_CLICK"
      },
      "execution_settings": {
        "wait_after_ms": 1500
      },
      "id": "undo"
    },
    {
      "action": {
        "evaluate": {
          "expression": "document.querySelectorAll('.react-flow__node').length",
          "store_result": "afterUndoCount"
        },
        "metadata": {
          "label": "Count after undo"
        },
        "type": "ACTION_TYPE_EVALUATE"
      },
      "id": "count3"
    }
  ],
  "settings": {
    "viewport_height": 1080,
    "viewport_width": 1920
  }
}
