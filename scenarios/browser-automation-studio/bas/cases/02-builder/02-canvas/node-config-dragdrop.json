{
  "edges": [
    {
      "id": "call-open-builder-to-wait-builder",
      "source": "call-open-builder",
      "target": "wait-builder",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e6",
      "source": "wait-builder",
      "target": "drag-dragdrop-node",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e7",
      "source": "drag-dragdrop-node",
      "target": "wait-node-render",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e8",
      "source": "wait-node-render",
      "target": "verify-dragdrop-node",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e9",
      "source": "verify-dragdrop-node",
      "target": "select-dragdrop",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e12",
      "source": "find-source-selector-input",
      "target": "enter-source-selector",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e13",
      "source": "enter-source-selector",
      "target": "find-target-selector-input",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e14",
      "source": "find-target-selector-input",
      "target": "enter-target-selector",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e15",
      "source": "enter-target-selector",
      "target": "save-configuration",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e18",
      "source": "re-select-node",
      "target": "verify-source-persisted",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e19",
      "source": "verify-source-persisted",
      "target": "verify-target-persisted",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e16-bridge-e17",
      "source": "save-configuration",
      "target": "re-select-node",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    },
    {
      "id": "e10-bridge-e11",
      "source": "select-dragdrop",
      "target": "find-source-selector-input",
      "type": "WORKFLOW_EDGE_TYPE_SMOOTHSTEP"
    }
  ],
  "metadata": {
    "description": "Validates DragDrop node configuration (regression test for camelCase compiler bug)",
    "labels": {
      "reset": "full"
    },
    "version": "1"
  },
  "nodes": [
    {
      "action": {
        "metadata": {
          "label": "Open builder via fixture"
        },
        "subflow": {
          "args": {
            "projectFolder": "${@params/projectFolder}",
            "projectName": "${@params/projectName}"
          },
          "workflow_path": "actions/open-builder-from-demo.json"
        },
        "type": "ACTION_TYPE_SUBFLOW"
      },
      "id": "call-open-builder"
    },
    {
      "action": {
        "metadata": {
          "label": "Wait for builder"
        },
        "type": "ACTION_TYPE_WAIT",
        "wait": {
          "selector": "@selector/workflowBuilder.code.modeButton",
          "timeout_ms": 15000
        }
      },
      "execution_settings": {
        "wait_after_ms": 600
      },
      "id": "wait-builder"
    },
    {
      "action": {
        "drag_drop": {
          "source_selector": "@selector/nodePalette.cards.dragDrop",
          "target_selector": ".react-flow__pane",
          "timeout_ms": 10000
        },
        "metadata": {
          "label": "Drag DragDrop node (camelCase compiler bug regression test)"
        },
        "type": "ACTION_TYPE_DRAG_DROP"
      },
      "id": "drag-dragdrop-node"
    },
    {
      "action": {
        "metadata": {
          "label": "Wait for DragDrop node"
        },
        "type": "ACTION_TYPE_WAIT",
        "wait": {
          "duration_ms": 1500
        }
      },
      "id": "wait-node-render"
    },
    {
      "action": {
        "assert": {
          "failure_message": "DragDrop node should appear - previously failed with 'unsupported node type: dragDrop'",
          "mode": "ASSERTION_MODE_EXISTS",
          "selector": ".react-flow__node",
          "timeout_ms": 5000
        },
        "metadata": {
          "label": "Verify DragDrop node exists (CRITICAL: this failed before compiler fix)"
        },
        "type": "ACTION_TYPE_ASSERT"
      },
      "id": "verify-dragdrop-node"
    },
    {
      "action": {
        "click": {
          "selector": ".react-flow__node:first-of-type"
        },
        "metadata": {
          "label": "Select DragDrop node"
        },
        "type": "ACTION_TYPE_CLICK"
      },
      "id": "select-dragdrop"
    },
    {
      "action": {
        "assert": {
          "failure_message": "Source selector input should be visible",
          "mode": "ASSERTION_MODE_EXISTS",
          "selector": ".react-flow__node.selected input[placeholder*=\"source\" i], .react-flow__node.selected input:first-of-type",
          "timeout_ms": 5000
        },
        "metadata": {
          "label": "Source selector input exists"
        },
        "type": "ACTION_TYPE_ASSERT"
      },
      "id": "find-source-selector-input"
    },
    {
      "action": {
        "input": {
          "selector": ".react-flow__node.selected input:first-of-type",
          "value": ".draggable-item"
        },
        "metadata": {
          "label": "Enter source selector"
        },
        "type": "ACTION_TYPE_INPUT"
      },
      "id": "enter-source-selector"
    },
    {
      "action": {
        "assert": {
          "failure_message": "Target selector input should be visible",
          "mode": "ASSERTION_MODE_EXISTS",
          "selector": ".react-flow__node.selected input[placeholder*=\"target\" i], .react-flow__node.selected input:nth-of-type(2)",
          "timeout_ms": 5000
        },
        "metadata": {
          "label": "Target selector input exists"
        },
        "type": "ACTION_TYPE_ASSERT"
      },
      "id": "find-target-selector-input"
    },
    {
      "action": {
        "input": {
          "selector": ".react-flow__node.selected input[placeholder*=\"target\" i]",
          "value": ".drop-zone"
        },
        "metadata": {
          "label": "Enter target selector"
        },
        "type": "ACTION_TYPE_INPUT"
      },
      "execution_settings": {
        "wait_after_ms": 500
      },
      "id": "enter-target-selector"
    },
    {
      "action": {
        "click": {
          "selector": ".react-flow__pane"
        },
        "metadata": {
          "label": "Click canvas to save"
        },
        "type": "ACTION_TYPE_CLICK"
      },
      "execution_settings": {
        "wait_after_ms": 1000
      },
      "id": "save-configuration"
    },
    {
      "action": {
        "click": {
          "selector": ".react-flow__node:first-of-type /*dup-1*/"
        },
        "metadata": {
          "label": "Re-select to verify persistence"
        },
        "type": "ACTION_TYPE_CLICK"
      },
      "id": "re-select-node"
    },
    {
      "action": {
        "assert": {
          "failure_message": "Source selector should persist",
          "mode": "ASSERTION_MODE_EXISTS",
          "selector": ".react-flow__node.selected input[value=\".draggable-item\"]",
          "timeout_ms": 3000
        },
        "metadata": {
          "label": "Source selector persisted"
        },
        "type": "ACTION_TYPE_ASSERT"
      },
      "id": "verify-source-persisted"
    },
    {
      "action": {
        "assert": {
          "failure_message": "Target selector should persist",
          "mode": "ASSERTION_MODE_EXISTS",
          "selector": ".react-flow__node.selected input[value=\".drop-zone\"]",
          "timeout_ms": 3000
        },
        "metadata": {
          "label": "Target selector persisted"
        },
        "type": "ACTION_TYPE_ASSERT"
      },
      "id": "verify-target-persisted"
    }
  ],
  "settings": {
    "viewport_height": 1080,
    "viewport_width": 1920
  }
}
