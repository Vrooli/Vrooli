      "position": {
        "x": 3000,
        "y": 0
      },
      "data": {
        "label": "Capture empty workflow canvas",
        "fullPage": true,
        "waitForMs": 500
      }
    },
    {
      "id": "drag-navigate-node",
      "type": "dragDrop",
      "position": {
        "x": 3200,
        "y": 0
