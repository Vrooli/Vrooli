      "position": {
        "x": 2000,
        "y": 0
      },
      "data": {
        "label": "Capture project detail page",
        "fullPage": true,
        "waitForMs": 500
      }
    },
    {
      "id": "click-new-workflow",
      "type": "click",
      "position": {
        "x": 2200,
        "y": 0
      },
      "data": {
        "label": "Click New Workflow button",
        "selector": "@selector/workflows.newButton",
        "timeoutMs": 10000,
        "waitForMs": 1500
      }
    },
    {
      "id": "wait-for-workflow-modal",
      "type": "wait",
      "position": {
        "x": 2400,
        "y": 0
