{
  "metadata": {
    "description": "Validates redo button re-applies undone changes",
    "requirement": "BAS-TOOLBAR-REDO",
    "version": 1
  },
  "settings": {
    "executionViewport": {
      "width": 1920,
      "height": 1080,
      "preset": "desktop"
    }
  },
  "nodes": [
    {
      "id": "call-open-demo-workflow",
      "type": "workflowCall",
      "data": {
        "label": "Open seeded workflow via fixture",
        "workflowId": "@fixture/open-demo-workflow"
      }
    },
    {
      "id": "click-palette",
      "type": "click",
      "data": {
        "label": "Click navigate node",
        "selector": "@selector/nodePalette.cards.navigate",
        "timeoutMs": 5000,
        "waitForMs": 500
      }
    },
    {
      "id": "click-canvas",
      "type": "click",
      "data": {
        "label": "Click canvas to add",
        "selector": ".react-flow__pane",
        "timeoutMs": 5000,
        "waitForMs": 2000
      }
    },
    {
      "id": "wait2",
      "type": "wait",
      "data": {
        "label": "Wait",
        "durationMs": 1000,
        "waitType": "duration"
      }
    },
    {
      "id": "count-after-add",
      "type": "evaluate",
      "data": {
        "label": "Count after add",
        "expression": "document.querySelectorAll('.react-flow__node').length",
        "storeAs": "afterAddCount"
      }
    },
    {
      "id": "undo",
      "type": "click",
      "data": {
        "label": "Undo",
        "selector": "@selector/toolbar.undoButton",
        "timeoutMs": 5000,
        "waitForMs": 1000
      }
    },
    {
      "id": "count-after-undo",
      "type": "evaluate",
      "data": {
        "label": "Count after undo",
        "expression": "document.querySelectorAll('.react-flow__node').length",
        "storeAs": "afterUndoCount"
      }
    },
    {
      "id": "redo",
      "type": "click",
      "data": {
        "label": "Redo",
        "selector": "@selector/toolbar.redoButton",
        "timeoutMs": 5000,
        "waitForMs": 1500
      }
    },
    {
      "id": "count-after-redo",
      "type": "evaluate",
      "data": {
        "label": "Count after redo",
        "expression": "document.querySelectorAll('.react-flow__node').length",
        "storeAs": "afterRedoCount"
      }
    },
    {
      "id": "cap",
      "type": "screenshot",
      "data": {
        "label": "Capture",
        "fullPage": true,
        "waitForMs": 500
      }
    }
  ],
  "edges": [
    {
      "id": "call-open-demo-workflow-to-click-palette",
      "source": "call-open-demo-workflow",
      "target": "click-palette",
      "type": "smoothstep"
    },
    {
      "id": "e5",
      "source": "click-palette",
      "target": "click-canvas",
      "type": "smoothstep"
    },
    {
      "id": "e6",
      "source": "click-canvas",
      "target": "wait2",
      "type": "smoothstep"
    },
    {
      "id": "e7",
      "source": "wait2",
      "target": "count-after-add",
      "type": "smoothstep"
    },
    {
      "id": "e8",
      "source": "count-after-add",
      "target": "undo",
      "type": "smoothstep"
    },
    {
      "id": "e9",
      "source": "undo",
      "target": "count-after-undo",
      "type": "smoothstep"
    },
    {
      "id": "e10",
      "source": "count-after-undo",
      "target": "redo",
      "type": "smoothstep"
    },
    {
      "id": "e11",
      "source": "redo",
      "target": "count-after-redo",
      "type": "smoothstep"
    },
    {
      "id": "e12",
      "source": "count-after-redo",
      "target": "cap",
      "type": "smoothstep"
    }
  ]
}
