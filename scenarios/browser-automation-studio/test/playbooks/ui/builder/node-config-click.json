{
  "metadata": {
    "description": "Validates Click node property configuration including selector editing",
    "requirement": "BAS-BUILDER-NODE-CONFIG",
    "version": 1
  },
  "settings": {
    "executionViewport": {
      "width": 1920,
      "height": 1080,
      "preset": "desktop"
    }
  },
  "nodes": [
    {
      "id": "navigate-dashboard",
      "type": "navigate",
      "position": {
        "x": 0,
        "y": 0
      },
      "data": {
        "label": "Open BAS dashboard",
        "destinationType": "scenario",
        "scenario": "browser-automation-studio",
        "scenarioPath": "/",
        "waitUntil": "networkidle0",
        "timeoutMs": 45000,
        "waitForMs": 2000
      }
    },
    {
      "id": "wait-dashboard",
      "type": "wait",
      "position": {
        "x": 200,
        "y": 0
      },
      "data": {
        "label": "Wait for dashboard",
        "durationMs": 2000,
        "waitType": "duration"
      }
    },
    {
      "id": "click-demo-project",
      "type": "click",
      "position": {
        "x": 400,
        "y": 0
      },
      "data": {
        "label": "Open Demo Browser Automations project",
        "selector": "[data-testid=\"project-card\"][data-project-name=\"Demo Browser Automations\"]",
        "timeoutMs": 5000,
        "waitForMs": 2000
      }
    },
    {
      "id": "wait-project",
      "type": "wait",
      "position": {
        "x": 600,
        "y": 0
      },
      "data": {
        "label": "Wait for project view",
        "durationMs": 2000,
        "waitType": "duration"
      }
    },
    {
      "id": "create-workflow",
      "type": "click",
      "position": {
        "x": 800,
        "y": 0
      },
      "data": {
        "label": "Create new workflow",
        "selector": "[data-testid=\"new-workflow-button\"]",
        "timeoutMs": 5000,
        "waitForMs": 3000
      }
    },
    {
      "id": "wait-for-ai-modal",
      "type": "wait",
      "position": {
        "x": 900,
        "y": 0
      },
      "data": {
        "label": "Wait for AI modal",
        "selector": "[data-testid=\"ai-prompt-modal\"]",
        "timeoutMs": 8000,
        "waitType": "element",
        "continueOnError": true
      }
    },
    {
      "id": "switch-to-manual",
      "type": "click",
      "position": {
        "x": 950,
        "y": 0
      },
      "data": {
        "label": "Switch to manual builder",
        "selector": "[data-testid=\"switch-to-manual-button\"]",
        "timeoutMs": 5000,
        "waitForMs": 1000,
        "continueOnError": true
      }
    },
    {
      "id": "wait-builder",
      "type": "wait",
      "position": {
        "x": 1000,
        "y": 0
      },
      "data": {
        "label": "Wait for builder",
        "selector": "[data-testid=\"workflow-builder-code-mode-button\"]",
        "timeoutMs": 15000,
        "waitForMs": 1000,
        "waitType": "element"
      }
    },
    {
      "id": "drag-click-node",
      "type": "dragDrop",
      "position": {
        "x": 1200,
        "y": 0
      },
      "data": {
        "label": "Drag Click node onto canvas",
        "sourceSelector": "[data-testid=\"node-palette-click-card\"]",
        "targetSelector": ".react-flow__pane",
        "timeoutMs": 10000,
        "waitForMs": 2000
      }
    },
    {
      "id": "wait-node-render",
      "type": "wait",
      "position": {
        "x": 1400,
        "y": 0
      },
      "data": {
        "label": "Wait for node to render",
        "durationMs": 1500,
        "waitType": "duration"
      }
    },
    {
      "id": "verify-click-node",
      "type": "assert",
      "position": {
        "x": 1600,
        "y": 0
      },
      "data": {
        "label": "Verify Click node exists",
        "selector": ".react-flow__node",
        "assertMode": "exists",
        "timeoutMs": 5000,
        "failureMessage": "Click node should appear"
      }
    },
    {
      "id": "click-node-to-select",
      "type": "click",
      "position": {
        "x": 1800,
        "y": 0
      },
      "data": {
        "label": "Click node to select",
        "selector": ".react-flow__node:first-of-type",
        "timeoutMs": 5000,
        "waitForMs": 1000
      }
    },
    {
      "id": "verify-selected",
      "type": "assert",
      "position": {
        "x": 2000,
        "y": 0
      },
      "data": {
        "label": "Node is selected",
        "selector": ".react-flow__node.selected",
        "assertMode": "exists",
        "timeoutMs": 3000,
        "failureMessage": "Node should be selected"
      }
    },
    {
      "id": "capture-before-edit",
      "type": "screenshot",
      "position": {
        "x": 2200,
        "y": 0
      },
      "data": {
        "label": "Capture before editing",
        "fullPage": true,
        "waitForMs": 500
      }
    },
    {
      "id": "find-selector-input",
      "type": "assert",
      "position": {
        "x": 2400,
        "y": 0
      },
      "data": {
        "label": "Selector input exists",
        "selector": ".react-flow__node.selected input[placeholder*=\"Selector\" i]",
        "assertMode": "exists",
        "timeoutMs": 5000,
        "failureMessage": "Selector input should be visible"
      }
    },
    {
      "id": "click-selector-input",
      "type": "click",
      "position": {
        "x": 2600,
        "y": 0
      },
      "data": {
        "label": "Click selector input",
        "selector": ".react-flow__node.selected input[placeholder*=\"Selector\" i] /*dup-1*/",
        "timeoutMs": 5000,
        "waitForMs": 300
      }
    },
    {
      "id": "clear-selector",
      "type": "keyboard",
      "position": {
        "x": 2800,
        "y": 0
      },
      "data": {
        "label": "Clear existing selector",
        "keys": ["Control", "a"],
        "waitForMs": 200
      }
    },
    {
      "id": "type-selector",
      "type": "type",
      "position": {
        "x": 3000,
        "y": 0
      },
      "data": {
        "label": "Type test selector",
        "selector": ".react-flow__node.selected input[placeholder*=\"Selector\" i] /*dup-2*/",
        "text": "#test-button",
        "timeoutMs": 5000,
        "waitForMs": 500
      }
    },
    {
      "id": "trigger-blur",
      "type": "click",
      "position": {
        "x": 3200,
        "y": 0
      },
      "data": {
        "label": "Click canvas to blur input",
        "selector": ".react-flow__pane",
        "timeoutMs": 5000,
        "waitForMs": 1000
      }
    },
    {
      "id": "capture-after-edit",
      "type": "screenshot",
      "position": {
        "x": 3400,
        "y": 0
      },
      "data": {
        "label": "Capture after editing selector",
        "fullPage": true,
        "waitForMs": 500
      }
    },
    {
      "id": "re-select-node",
      "type": "click",
      "position": {
        "x": 3600,
        "y": 0
      },
      "data": {
        "label": "Re-select node to verify persistence",
        "selector": ".react-flow__node:first-of-type /*dup-1*/",
        "timeoutMs": 5000,
        "waitForMs": 800
      }
    },
    {
      "id": "verify-selector-persisted",
      "type": "evaluate",
      "position": {
        "x": 3800,
        "y": 0
      },
      "data": {
        "label": "Verify selector persisted",
        "expression": "document.querySelector('.react-flow__node.selected input[placeholder*=\"Selector\" i]')?.value === '#test-button'",
        "storeAs": "selectorPersisted"
      }
    },
    {
      "id": "assert-selector-saved",
      "type": "assert",
      "position": {
        "x": 4000,
        "y": 0
      },
      "data": {
        "label": "Assert selector was saved",
        "selector": ".react-flow__node.selected input[value=\"#test-button\"]",
        "assertMode": "exists",
        "timeoutMs": 3000,
        "failureMessage": "Selector should persist after blur"
      }
    }
  ],
  "edges": [
    {
      "id": "e1",
      "source": "navigate-dashboard",
      "target": "wait-dashboard",
      "type": "smoothstep"
    },
    {
      "id": "e2",
      "source": "wait-dashboard",
      "target": "click-demo-project",
      "type": "smoothstep"
    },
    {
      "id": "e3",
      "source": "click-demo-project",
      "target": "wait-project",
      "type": "smoothstep"
    },
    {
      "id": "e4",
      "source": "wait-project",
      "target": "create-workflow",
      "type": "smoothstep"
    },
    {
      "id": "e5",
      "source": "create-workflow",
      "target": "wait-for-ai-modal",
      "type": "smoothstep"
    },
    {
      "id": "e5a",
      "source": "wait-for-ai-modal",
      "target": "switch-to-manual",
      "type": "smoothstep"
    },
    {
      "id": "e5b",
      "source": "switch-to-manual",
      "target": "wait-builder",
      "type": "smoothstep"
    },
    {
      "id": "e6",
      "source": "wait-builder",
      "target": "drag-click-node",
      "type": "smoothstep"
    },
    {
      "id": "e7",
      "source": "drag-click-node",
      "target": "wait-node-render",
      "type": "smoothstep"
    },
    {
      "id": "e8",
      "source": "wait-node-render",
      "target": "verify-click-node",
      "type": "smoothstep"
    },
    {
      "id": "e9",
      "source": "verify-click-node",
      "target": "click-node-to-select",
      "type": "smoothstep"
    },
    {
      "id": "e10",
      "source": "click-node-to-select",
      "target": "verify-selected",
      "type": "smoothstep"
    },
    {
      "id": "e11",
      "source": "verify-selected",
      "target": "capture-before-edit",
      "type": "smoothstep"
    },
    {
      "id": "e12",
      "source": "capture-before-edit",
      "target": "find-selector-input",
      "type": "smoothstep"
    },
    {
      "id": "e13",
      "source": "find-selector-input",
      "target": "click-selector-input",
      "type": "smoothstep"
    },
    {
      "id": "e14",
      "source": "click-selector-input",
      "target": "clear-selector",
      "type": "smoothstep"
    },
    {
      "id": "e15",
      "source": "clear-selector",
      "target": "type-selector",
      "type": "smoothstep"
    },
    {
      "id": "e16",
      "source": "type-selector",
      "target": "trigger-blur",
      "type": "smoothstep"
    },
    {
      "id": "e17",
      "source": "trigger-blur",
      "target": "capture-after-edit",
      "type": "smoothstep"
    },
    {
      "id": "e18",
      "source": "capture-after-edit",
      "target": "re-select-node",
      "type": "smoothstep"
    },
    {
      "id": "e19",
      "source": "re-select-node",
      "target": "verify-selector-persisted",
      "type": "smoothstep"
    },
    {
      "id": "e20",
      "source": "verify-selector-persisted",
      "target": "assert-selector-saved",
      "type": "smoothstep"
    }
  ]
}
