{
  "metadata": {
    "description": "Loads the Demo Browser Automations project and opens the seeded workflow for builder interactions",
    "fixture_id": "open-demo-workflow"
  },
  "nodes": [
    {
      "id": "fixture-call-open-project",
      "type": "workflowCall",
      "data": {
        "label": "Open Demo project",
        "workflowId": "@fixture/open-demo-project"
      }
    },
    {
      "id": "fixture-click-demo-workflow",
      "type": "click",
      "data": {
        "label": "Open seeded workflow",
        "selector": "@selector/workflows.card[data-workflow-name=\"Demo: Capture Example.com Hero\"]",
        "timeoutMs": 10000,
        "resilience": {
          "successSelector": "@selector/workflowBuilder.code.modeButton",
          "successTimeoutMs": 15000
        }
      }
    },
    {
      "id": "fixture-assert-nodes",
      "type": "assert",
      "data": {
        "label": "Seeded nodes are visible",
        "selector": ".react-flow__node",
        "assertMode": "exists",
        "timeoutMs": 5000,
        "failureMessage": "Seeded workflow should render nodes"
      }
    }
  ],
  "edges": [
    {
      "id": "fixture-open-project",
      "source": "fixture-call-open-project",
      "target": "fixture-click-demo-workflow",
      "type": "smoothstep"
    },
    {
      "id": "fixture-builder-ready",
      "source": "fixture-click-demo-workflow",
      "target": "fixture-assert-nodes",
      "type": "smoothstep"
    }
  ]
}
