{
  "metadata": {
    "description": "Slow-executing test workflow for telemetry validation (5-10 second duration)",
    "fixture_id": "slow-execution-demo",
    "version": 1,
    "reset": "none"
  },
  "settings": {
    "executionViewport": {
      "width": 1440,
      "height": 900,
      "preset": "desktop"
    }
  },
  "nodes": [
    {
      "id": "nav-1",
      "type": "navigate",
      "data": {
        "label": "Navigate to example.com",
        "url": "https://example.com",
        "timeoutMs": 10000
      }
    },
    {
      "id": "wait-1",
      "type": "wait",
      "data": {
        "label": "Wait 2 seconds",
        "waitForMs": 2000
      }
    },
    {
