      "type": "screenshot",
      "data": {
        "label": "Capture first screenshot",
        "fullPage": false
      }
    },
    {
      "id": "assert-1",
      "type": "assert",
      "data": {
        "label": "Assert page title exists",
        "selector": "h1",
        "assertMode": "exists",
        "timeoutMs": 5000
      }
    },
    {
      "id": "nav-2",
      "type": "navigate",
      "data": {
        "label": "Navigate to example.org",
