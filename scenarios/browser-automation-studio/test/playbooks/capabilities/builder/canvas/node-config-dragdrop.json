{
  "metadata": {
    "description": "Validates DragDrop node configuration (regression test for camelCase compiler bug)",
    "requirement": "BAS-BUILDER-NODE-CONFIG",
    "version": 1
  },
  "settings": {
    "executionViewport": {
      "width": 1920,
      "height": 1080,
      "preset": "desktop"
    }
  },
  "nodes": [
    {
      "id": "call-open-builder",
      "type": "workflowCall",
      "data": {
        "label": "Open builder via fixture",
        "workflowId": "@fixture/open-builder-from-demo(projectName=@seed/projectName, projectFolder=@seed/projectFolder)"
      }
    },
    {
      "id": "wait-builder",
      "type": "wait",
      "data": {
        "label": "Wait for builder",
        "selector": "@selector/workflowBuilder.code.modeButton",
        "timeoutMs": 15000,
        "waitForMs": 1000,
        "waitType": "element"
      }
    },
    {
      "id": "drag-dragdrop-node",
      "type": "dragDrop",
      "data": {
        "label": "Drag DragDrop node (camelCase compiler bug regression test)",
        "sourceSelector": "@selector/nodePalette.cards.dragDrop",
        "targetSelector": ".react-flow__pane",
        "timeoutMs": 10000,
        "waitForMs": 2000
      }
    },
    {
      "id": "wait-node-render",
      "type": "wait",
      "data": {
        "label": "Wait for DragDrop node",
        "durationMs": 1500,
        "waitType": "duration"
      }
    },
    {
      "id": "verify-dragdrop-node",
      "type": "assert",
      "data": {
        "label": "Verify DragDrop node exists (CRITICAL: this failed before compiler fix)",
        "selector": ".react-flow__node",
        "assertMode": "exists",
        "timeoutMs": 5000,
        "failureMessage": "DragDrop node should appear - previously failed with 'unsupported node type: dragDrop'"
      }
    },
    {
      "id": "select-dragdrop",
      "type": "click",
      "data": {
        "label": "Select DragDrop node",
        "selector": ".react-flow__node:first-of-type",
        "timeoutMs": 5000,
        "waitForMs": 1000
      }
    },
    {
      "id": "capture-initial",
      "type": "screenshot",
      "data": {
        "label": "Capture DragDrop node initial state",
        "fullPage": true,
        "waitForMs": 500
      }
    },
    {
      "id": "find-source-selector-input",
      "type": "assert",
      "data": {
        "label": "Source selector input exists",
        "selector": ".react-flow__node.selected input[placeholder*=\"source\" i], .react-flow__node.selected input:first-of-type",
        "assertMode": "exists",
        "timeoutMs": 5000,
        "failureMessage": "Source selector input should be visible"
      }
    },
    {
      "id": "enter-source-selector",
      "type": "type",
      "data": {
        "label": "Enter source selector",
        "selector": ".react-flow__node.selected input:first-of-type",
        "text": ".draggable-item",
        "timeoutMs": 5000,
        "waitForMs": 500
      }
    },
    {
      "id": "find-target-selector-input",
      "type": "assert",
      "data": {
        "label": "Target selector input exists",
        "selector": ".react-flow__node.selected input[placeholder*=\"target\" i], .react-flow__node.selected input:nth-of-type(2)",
        "assertMode": "exists",
        "timeoutMs": 5000,
        "failureMessage": "Target selector input should be visible"
      }
    },
    {
      "id": "enter-target-selector",
      "type": "type",
      "data": {
        "label": "Enter target selector",
        "selector": ".react-flow__node.selected input:nth-of-type(2)",
        "text": ".drop-zone",
        "timeoutMs": 5000,
        "waitForMs": 500
      }
    },
    {
      "id": "save-configuration",
      "type": "click",
      "data": {
        "label": "Click canvas to save",
        "selector": ".react-flow__pane",
        "timeoutMs": 5000,
        "waitForMs": 1000
      }
    },
    {
      "id": "capture-configured",
      "type": "screenshot",
      "data": {
        "label": "Capture configured DragDrop node",
        "fullPage": true,
        "waitForMs": 500
      }
    },
    {
      "id": "re-select-node",
      "type": "click",
      "data": {
        "label": "Re-select to verify persistence",
        "selector": ".react-flow__node:first-of-type /*dup-1*/",
        "timeoutMs": 5000,
        "waitForMs": 800
      }
    },
    {
      "id": "verify-source-persisted",
      "type": "assert",
      "data": {
        "label": "Source selector persisted",
        "selector": ".react-flow__node.selected input[value=\".draggable-item\"]",
        "assertMode": "exists",
        "timeoutMs": 3000,
        "failureMessage": "Source selector should persist"
      }
    },
    {
      "id": "verify-target-persisted",
      "type": "assert",
      "data": {
        "label": "Target selector persisted",
        "selector": ".react-flow__node.selected input[value=\".drop-zone\"]",
        "assertMode": "exists",
        "timeoutMs": 3000,
        "failureMessage": "Target selector should persist"
      }
    },
    {
      "id": "verify-final-state",
      "type": "screenshot",
      "data": {
        "label": "Final DragDrop configuration confirmed",
        "fullPage": true,
        "waitForMs": 500
      }
    }
  ],
  "edges": [
    {
      "id": "call-open-builder-to-wait-builder",
      "source": "call-open-builder",
      "target": "wait-builder",
      "type": "smoothstep"
    },
    {
      "id": "e6",
      "source": "wait-builder",
      "target": "drag-dragdrop-node",
      "type": "smoothstep"
    },
    {
      "id": "e7",
      "source": "drag-dragdrop-node",
      "target": "wait-node-render",
      "type": "smoothstep"
    },
    {
      "id": "e8",
      "source": "wait-node-render",
      "target": "verify-dragdrop-node",
      "type": "smoothstep"
    },
    {
      "id": "e9",
      "source": "verify-dragdrop-node",
      "target": "select-dragdrop",
      "type": "smoothstep"
    },
    {
      "id": "e10",
      "source": "select-dragdrop",
      "target": "capture-initial",
      "type": "smoothstep"
    },
    {
      "id": "e11",
      "source": "capture-initial",
      "target": "find-source-selector-input",
      "type": "smoothstep"
    },
    {
      "id": "e12",
      "source": "find-source-selector-input",
      "target": "enter-source-selector",
      "type": "smoothstep"
    },
    {
      "id": "e13",
      "source": "enter-source-selector",
      "target": "find-target-selector-input",
      "type": "smoothstep"
    },
    {
      "id": "e14",
      "source": "find-target-selector-input",
      "target": "enter-target-selector",
      "type": "smoothstep"
    },
    {
      "id": "e15",
      "source": "enter-target-selector",
      "target": "save-configuration",
      "type": "smoothstep"
    },
    {
      "id": "e16",
      "source": "save-configuration",
      "target": "capture-configured",
      "type": "smoothstep"
    },
    {
      "id": "e17",
      "source": "capture-configured",
      "target": "re-select-node",
      "type": "smoothstep"
    },
    {
      "id": "e18",
      "source": "re-select-node",
      "target": "verify-source-persisted",
      "type": "smoothstep"
    },
    {
      "id": "e19",
      "source": "verify-source-persisted",
      "target": "verify-target-persisted",
      "type": "smoothstep"
    },
    {
      "id": "e20",
      "source": "verify-target-persisted",
      "target": "verify-final-state",
      "type": "smoothstep"
    }
  ]
}
