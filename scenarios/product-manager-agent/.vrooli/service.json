{
  "service": {
    "name": "product-manager-agent",
    "displayName": "Product Manager Agent",
    "description": "Strategic AI assistant for product management",
    "version": "1.0.0",
    "resources": {
      "n8n": {
        "enabled": true,
        "workflows": [
          "initialization/n8n/feature-prioritizer.json",
          "initialization/n8n/market-researcher.json",
          "initialization/n8n/roadmap-generator.json",
          "initialization/n8n/roi-calculator.json",
          "initialization/n8n/sprint-planner.json",
          "initialization/n8n/user-feedback-processor.json",
          "initialization/n8n/competitor-analyzer.json",
          "initialization/n8n/decision-analyzer.json"
        ]
      },
      "ollama": {
        "enabled": true,
        "models": [
          "llama3.2:3b",
          "nomic-embed-text"
        ]
      },
      "postgres": {
        "enabled": true,
        "schemas": [
          "initialization/postgres/schema.sql"
        ]
      },
      "qdrant": {
        "enabled": true,
        "collections": [
          "initialization/qdrant/collections.json"
        ]
      }
    },
    "services": {
      "api": {
        "port": 8800
      },
      "ui": {
        "port": 3800
      }
    },
    "tags": [
      "app",
      "go-api"
    ]
  },
  "lifecycle": {
    "setup": {
      "description": null,
      "condition": {
        "checks": [
          {
            "type": "binaries",
            "targets": [
              "api/product-manager-agent-api"
            ]
          },
          {
            "type": "cli",
            "command": "product-manager-agent"
          },
          {
            "type": "dependencies",
            "paths": [
              "ui/package.json"
            ]
          },
          {
            "type": "resources",
            "populated": true
          }
        ]
      },
      "steps": null
    }
  }
}
