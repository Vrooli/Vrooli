          }
        ]
      ]
    },
    "set-defaults": {
      "main": [
        [
          {
            "node": "postgres-query",
            "type": "main",
            "index": 0
          },
          {
            "node": "postgres-stats",
            "type": "main",
            "index": 0
          },
          {
            "node": "postgres-achievements",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "postgres-query": {
      "main": [
        [
          {
