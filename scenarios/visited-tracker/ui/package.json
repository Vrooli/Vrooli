{
  "name": "visited-tracker-ui",
  "version": "1.0.0",
  "description": "Campaign management dashboard for visited tracker",
  "main": "server.js",
  "scripts": {
    "start": "bash -c '${VROOLI_ROOT:-$(git rev-parse --show-toplevel)}/scripts/lib/ui-guard.sh node server.js'",
    "dev": "bash -c '${VROOLI_ROOT:-$(git rev-parse --show-toplevel)}/scripts/lib/ui-guard.sh node server.js'",
    "test": "jest --runInBand --coverage --testTimeout=30000"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "keywords": [
    "visited-tracker",
    "campaign-tracking",
    "file-operations",
    "developer-tools"
  ],
  "devDependencies": {
    "jest": "^30.1.3",
    "supertest": "^7.1.4"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "server.js",
      "**/*.js",
      "!node_modules/**",
      "!coverage/**",
      "!**/coverage/**"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 60,
        "branches": 35,
        "functions": 60,
        "lines": 60
      }
    }
  }
}
