{
  "name": "visited-tracker-ui",
  "version": "1.0.0",
  "description": "Campaign management dashboard for visited tracker",
  "main": "server.js",
  "scripts": {
    "start": "bash -c '${VROOLI_ROOT:-$(git rev-parse --show-toplevel)}/scripts/lib/ui-guard.sh node server.js'",
    "dev": "bash -c '${VROOLI_ROOT:-$(git rev-parse --show-toplevel)}/scripts/lib/ui-guard.sh node server.js'",
    "test": "jest --runInBand --coverage --testTimeout=30000",
    "build": "node build.js"
  },
  "dependencies": {
    "@vrooli/iframe-bridge": "file:../../../packages/iframe-bridge",
    "express": "^4.18.2"
  },
  "keywords": [
    "visited-tracker",
    "campaign-tracking",
    "file-operations",
    "developer-tools"
  ],
  "devDependencies": {
    "bats": "^1.12.0",
    "jest": "^30.1.3",
    "supertest": "^7.1.4"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "server.js",
      "**/*.js",
      "!node_modules/**",
      "!coverage/**",
      "!**/coverage/**"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "json-summary"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 60,
        "branches": 35,
        "functions": 60,
        "lines": 60
      }
    }
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
