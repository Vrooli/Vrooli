{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "types": [
      "vite/client"
    ],
    "allowJs": false,
    "strict": true,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "paths": {
      "@/components/*": [
        "src/components/*"
      ],
      "@/lib/*": [
        "src/lib/*"
      ],
      "@/hooks/*": [
        "src/hooks/*"
      ],
      "@/styles/*": [
        "src/styles/*"
      ],
      "@/*": [
        "src/*"
      ],
      "@vrooli/api-base": [
        "../../../packages/api-base/dist/index.js"
      ],
      "@vrooli/iframe-bridge": [
        "../../../packages/iframe-bridge/dist/index.js"
      ],
      "@vrooli/iframe-bridge/*": [
        "../../../packages/iframe-bridge/dist/*"
      ]
    },
    "allowImportingTsExtensions": true,
    "noEmit": true
  },
  "include": [
    "src"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
