  "queries": {
    "get_analytics_overview": {
      "type": "postgresql",
      "query": "SELECT COUNT(*) as total_data_points, AVG(CASE WHEN data IS NOT NULL AND jsonb_typeof(data) = 'object' THEN 100 ELSE 0 END) as avg_data_quality FROM scraping_results WHERE started_at > NOW() - INTERVAL '30 days'",
      "runOnLoad": true,
      "refetchInterval": 300000
    },
    "get_daily_volume_trend": {
      "type": "postgresql",
      "query": "SELECT DATE(sr.started_at) as date, sa.platform, SUM(sr.extracted_count) as data_points FROM scraping_results sr JOIN scraping_agents sa ON sr.agent_id = sa.id WHERE sr.started_at > NOW() - INTERVAL '{{ trend_timeframe || '30d' }}' GROUP BY DATE(sr.started_at), sa.platform ORDER BY date",
      "runOnLoad": true,
      "refetchInterval": 300000
    },
    "get_platform_performance": {
      "type": "postgresql",
      "query": "SELECT sa.platform, COUNT(*) as total_runs, COUNT(CASE WHEN sr.status = 'success' THEN 1 END) as success_count, AVG(CASE WHEN sr.status = 'success' THEN 1 ELSE 0 END) * 100 as success_rate, AVG(sr.execution_time_ms::float / 1000) as avg_time_seconds FROM scraping_agents sa JOIN scraping_results sr ON sa.id = sr.agent_id GROUP BY sa.platform",
      "runOnLoad": true,
      "refetchInterval": 300000
    },
    "get_agent_performance": {
      "type": "postgresql",
      "query": "SELECT sa.name as agent_name, sa.platform, COUNT(sr.id) as total_runs, AVG(CASE WHEN sr.status = 'success' THEN 1 ELSE 0 END) * 100 as success_rate, AVG(sr.execution_time_ms::float / 1000) as avg_execution_time, AVG(sr.extracted_count::float / (sr.execution_time_ms::float / 60000)) as data_points_per_minute, AVG(CASE WHEN sr.status != 'success' THEN 1 ELSE 0 END) * 100 as error_rate FROM scraping_agents sa LEFT JOIN scraping_results sr ON sa.id = sr.agent_id WHERE sr.started_at > NOW() - INTERVAL '{{ perf_timeframe || '24h' }}' GROUP BY sa.id, sa.name, sa.platform ORDER BY total_runs DESC",
      "runOnLoad": true,
      "refetchInterval": 300000
    },
    "get_quality_issues": {
      "type": "postgresql",
      "query": "SELECT NOW() as detected_at, sa.name as agent_name, 'missing_data' as issue_type, 'medium' as severity, 'Some required fields are frequently empty' as description, 15 as affected_records, 'open' as status FROM scraping_agents sa LIMIT 5",
      "runOnLoad": true,
      "refetchInterval": 600000
    },
    "get_export_configurations": {
      "type": "postgresql",
      "query": "SELECT * FROM export_configs ORDER BY created_at DESC",
      "runOnLoad": true,
      "refetchInterval": 300000
    },
    "create_export_config": {
      "type": "postgresql",
      "query": "INSERT INTO export_configs (name, format, agent_ids, destination_config, schedule_cron, enabled) VALUES ('{{ export_name }}', '{{ export_format }}', '{{ selected_agents }}'::uuid[], '{{ destination_config }}'::jsonb, {{ export_schedule ? \"'\" + export_schedule + \"'\" : 'NULL' }}, true)",
      "runOnLoad": false
    },
    "execute_export": {
      "type": "http",
      "url": "http://localhost:5678/webhook/execute-export",
      "method": "POST",
      "body": {"export_id": "{{ export_id }}"},
      "runOnLoad": false
    },
    "generate_analytics_report": {
      "type": "http",
      "url": "http://localhost:5678/webhook/generate-report",
      "method": "POST",
      "body": {"report_type": "comprehensive", "format": "pdf"},
      "runOnLoad": false
    }
  },
  "variables": {
