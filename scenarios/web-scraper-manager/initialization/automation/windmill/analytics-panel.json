{
  "value": {
    "type": "tabscomponent",
    "tabs": [
      {
        "id": "overview",
        "value": "Analytics Overview"
      },
      {
        "id": "performance",
        "value": "Performance Metrics"
      },
      {
        "id": "data_quality",
        "value": "Data Quality"
      },
      {
        "id": "exports",
        "value": "Data Exports"
      },
      {
        "id": "insights",
        "value": "AI Insights"
      }
    ],
    "configuration": {
      "overview": {
        "type": "containercomponent",
        "components": [
          {
            "id": "analytics_header",
            "type": "textcomponent",
            "configuration": {
              "text": "# ðŸ“Š Scraping Analytics & Business Intelligence\n## Comprehensive insights into your web scraping operations\n**Last updated:** {{now.format('MMMM Do, YYYY [at] h:mm A')}}",
              "style": {
                "fontSize": "16px",
                "fontWeight": "600",
                "color": "#1e3a8a",
                "backgroundColor": "#eff6ff",
                "padding": "20px",
                "borderRadius": "8px",
                "marginBottom": "20px"
              }
            }
          },
          {
            "id": "kpi_grid",
            "type": "containercomponent",
            "configuration": {
              "style": {
                "display": "grid",
                "gridTemplateColumns": "repeat(auto-fit, minmax(200px, 1fr))",
                "gap": "15px",
                "marginBottom": "30px"
              }
            },
            "components": [
              {
                "id": "total_data_points",
                "type": "containercomponent",
                "configuration": {
                  "style": {
                    "backgroundColor": "#ecfdf5",
                    "border": "2px solid #10b981",
                    "borderRadius": "12px",
                    "padding": "15px",
                    "textAlign": "center"
                  }
                },
                "components": [
                  {
                    "id": "data_points_text",
                    "type": "textcomponent",
                    "configuration": {
                      "text": "## {{total_data_points || 0}}\n**Data Points Collected**\n*Last 30 days*",
                      "style": {"color": "#047857", "marginBottom": "0"}
                    }
                  }
                ]
              },
              {
                "id": "average_quality",
                "type": "containercomponent",
                "configuration": {
                  "style": {
                    "backgroundColor": "#f0f9ff",
                    "border": "2px solid #0ea5e9",
                    "borderRadius": "12px",
                    "padding": "15px",
                    "textAlign": "center"
                  }
                },
                "components": [
                  {
                    "id": "quality_text",
                    "type": "textcomponent",
                    "configuration": {
                      "text": "## {{(avg_data_quality || 0).toFixed(1)}}%\n**Average Data Quality**\n*Completeness Score*",
                      "style": {"color": "#0c4a6e", "marginBottom": "0"}
                    }
                  }
                ]
              },
              {
                "id": "cost_efficiency",
                "type": "containercomponent",
                "configuration": {
                  "style": {
                    "backgroundColor": "#fef3c7",
                    "border": "2px solid #f59e0b",
                    "borderRadius": "12px",
                    "padding": "15px",
                    "textAlign": "center"
                  }
                },
                "components": [
                  {
                    "id": "efficiency_text",
                    "type": "textcomponent",
                    "configuration": {
                      "text": "## ${{(cost_per_point || 0).toFixed(4)}}\n**Cost per Data Point**\n*Operational Efficiency*",
                      "style": {"color": "#92400e", "marginBottom": "0"}
                    }
                  }
                ]
              },
              {
                "id": "storage_utilization",
                "type": "containercomponent",
                "configuration": {
                  "style": {
                    "backgroundColor": "#f3e8ff",
                    "border": "2px solid #8b5cf6",
                    "borderRadius": "12px",
                    "padding": "15px",
                    "textAlign": "center"
                  }
                },
                "components": [
                  {
                    "id": "storage_text",
                    "type": "textcomponent",
                    "configuration": {
                      "text": "## {{(storage_used_gb || 0).toFixed(1)}}GB\n**Storage Utilized**\n*Vector + Object Storage*",
                      "style": {"color": "#7c3aed", "marginBottom": "0"}
                    }
                  }
                ]
              }
            ]
          },
          {
            "id": "trends_section",
            "type": "containercomponent",
            "configuration": {
              "style": {
                "backgroundColor": "white",
                "border": "1px solid #e5e7eb",
                "borderRadius": "8px",
                "padding": "20px",
                "marginBottom": "20px"
              }
            },
            "components": [
              {
                "id": "trends_header",
                "type": "containercomponent",
                "configuration": {
                  "style": {
                    "display": "flex",
                    "justifyContent": "space-between",
                    "alignItems": "center",
                    "marginBottom": "15px"
                  }
                },
                "components": [
                  {
                    "id": "trends_title",
                    "type": "textcomponent",
                    "configuration": {
                      "text": "### ðŸ“ˆ Performance Trends",
                      "style": {"margin": "0"}
                    }
                  },
                  {
                    "id": "time_selector",
                    "type": "selectcomponent",
                    "configuration": {
                      "options": [
                        {"value": "7d", "label": "Last 7 Days"},
                        {"value": "30d", "label": "Last 30 Days"},
                        {"value": "90d", "label": "Last 90 Days"}
                      ],
                      "value": "30d",
                      "onChange": {
                        "type": "set_variable",
                        "variable": "trend_timeframe",
                        "value": "{{ value }}"
                      }
                    }
                  }
                ]
              },
              {
                "id": "trends_chart",
                "type": "chartcomponent",
                "configuration": {
                  "title": "Daily Scraping Volume",
                  "type": "line",
                  "data": "={{ daily_volume_trend || [] }}",
                  "xField": "date",
                  "yField": "data_points",
                  "seriesField": "platform",
                  "height": "300px",
                  "smooth": true
                }
              }
            ]
          },
          {
            "id": "platform_breakdown",
            "type": "containercomponent",
            "configuration": {
              "style": {
                "display": "grid",
                "gridTemplateColumns": "1fr 1fr",
                "gap": "20px"
              }
            },
            "components": [
              {
                "id": "success_rate_chart",
                "type": "containercomponent",
                "configuration": {
                  "style": {
                    "backgroundColor": "white",
                    "border": "1px solid #e5e7eb",
                    "borderRadius": "8px",
                    "padding": "20px"
                  }
                },
                "components": [
                  {
                    "id": "success_chart",
                    "type": "chartcomponent",
                    "configuration": {
                      "title": "Success Rate by Platform",
                      "type": "doughnut",
                      "data": "={{ platform_success_rates || [] }}",
                      "labelField": "platform",
                      "valueField": "success_rate",
                      "height": "250px"
                    }
                  }
                ]
              },
              {
                "id": "execution_time_chart",
                "type": "containercomponent",
                "configuration": {
                  "style": {
                    "backgroundColor": "white",
                    "border": "1px solid #e5e7eb",
                    "borderRadius": "8px",
                    "padding": "20px"
                  }
                },
                "components": [
                  {
                    "id": "time_chart",
                    "type": "chartcomponent",
                    "configuration": {
                      "title": "Average Execution Time",
                      "type": "bar",
                      "data": "={{ platform_execution_times || [] }}",
                      "xField": "platform",
                      "yField": "avg_time_seconds",
                      "height": "250px"
                    }
                  }
                ]
              }
            ]
          }
        ]
      },
      "performance": {
        "type": "containercomponent",
        "components": [
          {
            "id": "performance_header",
            "type": "textcomponent",
            "configuration": {
              "text": "### âš¡ Performance Analytics & Optimization",
              "style": {"marginBottom": "20px"}
            }
          },
          {
            "id": "performance_filters",
            "type": "containercomponent",
            "configuration": {
              "style": {
                "backgroundColor": "#f9fafb",
                "border": "1px solid #e5e7eb",
                "borderRadius": "8px",
                "padding": "15px",
                "marginBottom": "20px",
                "display": "flex",
                "gap": "15px",
                "alignItems": "center",
                "flexWrap": "wrap"
              }
            },
            "components": [
              {
                "id": "perf_platform_filter",
                "type": "selectcomponent",
                "configuration": {
                  "label": "Platform",
                  "options": [
                    {"value": "all", "label": "All Platforms"},
                    {"value": "huginn", "label": "Huginn"},
                    {"value": "browserless", "label": "Browserless"},
                    {"value": "agent-s2", "label": "Agent-S2"}
                  ],
                  "value": "all"
                }
              },
              {
                "id": "perf_metric_select",
                "type": "selectcomponent",
                "configuration": {
                  "label": "Primary Metric",
                  "options": [
                    {"value": "execution_time", "label": "Execution Time"},
                    {"value": "success_rate", "label": "Success Rate"},
                    {"value": "data_quality", "label": "Data Quality"},
                    {"value": "throughput", "label": "Throughput"}
                  ],
                  "value": "execution_time"
                }
              },
              {
                "id": "perf_timeframe",
                "type": "selectcomponent",
                "configuration": {
                  "label": "Time Range",
                  "options": [
                    {"value": "1h", "label": "Last Hour"},
                    {"value": "24h", "label": "Last 24 Hours"},
                    {"value": "7d", "label": "Last 7 Days"},
                    {"value": "30d", "label": "Last 30 Days"}
                  ],
                  "value": "24h"
                }
              }
            ]
          },
          {
            "id": "performance_metrics",
            "type": "containercomponent",
            "configuration": {
              "style": {
                "backgroundColor": "white",
                "border": "1px solid #e5e7eb",
                "borderRadius": "8px",
                "padding": "20px",
                "marginBottom": "20px"
              }
            },
            "components": [
              {
                "id": "metrics_table",
                "type": "tablecomponent",
                "configuration": {
                  "title": "Agent Performance Summary",
                  "data": "={{ agent_performance || [] }}",
                  "columns": [
                    {
                      "field": "agent_name",
                      "header": "Agent"
                    },
                    {
                      "field": "platform",
                      "header": "Platform",
                      "type": "badge",
                      "width": "100px",
                      "colorMap": {
                        "huginn": "warning",
                        "browserless": "info",
                        "agent-s2": "purple"
                      }
                    },
                    {
                      "field": "total_runs",
                      "header": "Total Runs",
                      "type": "number",
                      "width": "100px"
                    },
                    {
                      "field": "success_rate",
                      "header": "Success Rate",
                      "width": "100px",
                      "render": "{{ (row.success_rate || 0).toFixed(1) }}%",
                      "colorize": true
                    },
                    {
                      "field": "avg_execution_time",
                      "header": "Avg Time (s)",
                      "width": "110px",
                      "render": "{{ (row.avg_execution_time || 0).toFixed(1) }}"
                    },
                    {
                      "field": "data_points_per_minute",
                      "header": "Throughput",
                      "width": "100px",
                      "render": "{{ (row.data_points_per_minute || 0).toFixed(1) }}/min"
                    },
                    {
                      "field": "error_rate",
                      "header": "Error Rate",
                      "width": "100px",
                      "render": "{{ (row.error_rate || 0).toFixed(1) }}%",
                      "colorize": true,
                      "colorInverse": true
                    },
                    {
                      "field": "last_24h_trend",
                      "header": "24h Trend",
                      "width": "100px",
                      "render": "{{ row.last_24h_trend > 0 ? 'ðŸ“ˆ +' + row.last_24h_trend.toFixed(1) + '%' : row.last_24h_trend < 0 ? 'ðŸ“‰ ' + row.last_24h_trend.toFixed(1) + '%' : 'âž¡ï¸ 0%' }}"
                    }
                  ],
                  "pagination": true,
                  "pageSize": 15,
                  "sortable": true,
                  "defaultSort": {
                    "field": "total_runs",
                    "direction": "desc"
                  }
                }
              }
            ]
          },
          {
            "id": "performance_charts",
            "type": "containercomponent",
            "configuration": {
              "style": {
                "display": "grid",
                "gridTemplateColumns": "1fr 1fr",
                "gap": "20px"
              }
            },
            "components": [
              {
                "id": "response_time_distribution",
                "type": "containercomponent",
                "configuration": {
                  "style": {
                    "backgroundColor": "white",
                    "border": "1px solid #e5e7eb",
                    "borderRadius": "8px",
                    "padding": "20px"
                  }
                },
                "components": [
                  {
                    "id": "response_chart",
                    "type": "chartcomponent",
                    "configuration": {
                      "title": "Response Time Distribution",
                      "type": "histogram",
                      "data": "={{ response_time_distribution || [] }}",
                      "xField": "time_bucket",
                      "yField": "count",
                      "height": "300px"
                    }
                  }
                ]
              },
              {
                "id": "throughput_over_time",
                "type": "containercomponent",
                "configuration": {
                  "style": {
                    "backgroundColor": "white",
                    "border": "1px solid #e5e7eb",
                    "borderRadius": "8px",
                    "padding": "20px"
                  }
                },
                "components": [
                  {
                    "id": "throughput_chart",
                    "type": "chartcomponent",
                    "configuration": {
                      "title": "Throughput Over Time",
                      "type": "line",
                      "data": "={{ throughput_timeline || [] }}",
                      "xField": "timestamp",
                      "yField": "data_points_per_hour",
                      "seriesField": "platform",
                      "height": "300px"
                    }
                  }
                ]
              }
            ]
          }
        ]
      },
      "data_quality": {
        "type": "containercomponent",
        "components": [
          {
            "id": "quality_header",
            "type": "textcomponent",
            "configuration": {
              "text": "### ðŸŽ¯ Data Quality Monitoring & Analysis",
              "style": {"marginBottom": "20px"}
            }
          },
          {
            "id": "quality_summary",
            "type": "containercomponent",
            "configuration": {
              "style": {
                "backgroundColor": "white",
                "border": "1px solid #e5e7eb",
                "borderRadius": "8px",
                "padding": "20px",
                "marginBottom": "20px"
              }
            },
            "components": [
              {
                "id": "quality_stats",
                "type": "containercomponent",
                "configuration": {
                  "style": {
                    "display": "grid",
                    "gridTemplateColumns": "repeat(auto-fit, minmax(150px, 1fr))",
                    "gap": "15px",
                    "marginBottom": "20px"
                  }
                },
                "components": [
                  {
                    "id": "completeness_score",
                    "type": "textcomponent",
                    "configuration": {
                      "text": "**{{(completeness_score || 0).toFixed(1)}}%**\nCompleteness",
                      "style": {"textAlign": "center", "padding": "15px", "backgroundColor": "#ecfdf5", "borderRadius": "6px"}
                    }
                  },
                  {
                    "id": "accuracy_score",
                    "type": "textcomponent",
                    "configuration": {
                      "text": "**{{(accuracy_score || 0).toFixed(1)}}%**\nAccuracy",
                      "style": {"textAlign": "center", "padding": "15px", "backgroundColor": "#f0f9ff", "borderRadius": "6px"}
                    }
                  },
                  {
                    "id": "consistency_score",
                    "type": "textcomponent",
                    "configuration": {
                      "text": "**{{(consistency_score || 0).toFixed(1)}}%**\nConsistency",
                      "style": {"textAlign": "center", "padding": "15px", "backgroundColor": "#fef3c7", "borderRadius": "6px"}
                    }
                  },
                  {
                    "id": "duplicate_rate",
                    "type": "textcomponent",
                    "configuration": {
                      "text": "**{{(duplicate_rate || 0).toFixed(1)}}%**\nDuplicate Rate",
                      "style": {"textAlign": "center", "padding": "15px", "backgroundColor": "#fee2e2", "borderRadius": "6px"}
                    }
                  }
                ]
              },
              {
                "id": "quality_trend_chart",
                "type": "chartcomponent",
                "configuration": {
                  "title": "Data Quality Trends (Last 30 Days)",
                  "type": "line",
                  "data": "={{ quality_trends || [] }}",
                  "xField": "date",
                  "yField": "quality_score",
                  "seriesField": "metric_type",
                  "height": "300px",
                  "smooth": true
                }
              }
            ]
          },
          {
            "id": "quality_issues",
            "type": "containercomponent",
            "configuration": {
              "style": {
                "backgroundColor": "white",
                "border": "1px solid #e5e7eb",
                "borderRadius": "8px",
                "padding": "20px"
              }
            },
            "components": [
              {
                "id": "issues_title",
                "type": "textcomponent",
                "configuration": {
                  "text": "**ðŸš¨ Recent Data Quality Issues**",
                  "style": {"marginBottom": "15px"}
                }
              },
              {
                "id": "quality_issues_table",
                "type": "tablecomponent",
                "configuration": {
                  "data": "={{ quality_issues || [] }}",
                  "columns": [
                    {
                      "field": "detected_at",
                      "header": "Detected",
                      "type": "datetime",
                      "width": "140px"
                    },
                    {
                      "field": "agent_name",
                      "header": "Agent"
                    },
                    {
                      "field": "issue_type",
                      "header": "Issue Type",
                      "type": "badge",
                      "width": "120px",
                      "colorMap": {
                        "missing_data": "warning",
                        "duplicate_data": "danger",
                        "format_error": "info",
                        "validation_failed": "purple"
                      }
                    },
                    {
                      "field": "severity",
                      "header": "Severity",
                      "type": "badge",
                      "width": "80px",
                      "colorMap": {
                        "high": "danger",
                        "medium": "warning",
                        "low": "info"
                      }
                    },
                    {
                      "field": "description",
                      "header": "Description"
                    },
                    {
                      "field": "affected_records",
                      "header": "Records",
                      "type": "number",
                      "width": "80px"
                    },
                    {
                      "field": "status",
                      "header": "Status",
                      "type": "badge",
                      "width": "100px",
                      "colorMap": {
                        "open": "warning",
                        "investigating": "info",
                        "resolved": "success",
                        "ignored": "secondary"
                      }
                    }
                  ],
                  "pagination": true,
                  "pageSize": 10,
                  "sortable": true,
                  "defaultSort": {
                    "field": "detected_at",
                    "direction": "desc"
                  }
                }
              }
            ]
          }
        ]
      },
      "exports": {
        "type": "containercomponent",
        "components": [
          {
            "id": "exports_header",
            "type": "textcomponent",
            "configuration": {
              "text": "### ðŸ“¤ Data Export & Integration Management",
              "style": {"marginBottom": "20px"}
            }
          },
          {
            "id": "create_export",
            "type": "containercomponent",
            "configuration": {
              "style": {
                "backgroundColor": "#f8fafc",
                "border": "1px solid #e2e8f0",
                "borderRadius": "8px",
                "padding": "20px",
                "marginBottom": "20px"
              }
            },
            "components": [
              {
                "id": "export_form_title",
                "type": "textcomponent",
                "configuration": {
                  "text": "**Create New Export Configuration**",
                  "style": {"marginBottom": "15px"}
                }
              },
              {
                "id": "export_form",
                "type": "formcomponent",
                "configuration": {
                  "layout": "horizontal",
                  "fields": [
                    {
                      "id": "export_name",
                      "type": "text",
                      "label": "Export Name",
                      "placeholder": "e.g., Weekly Product Data Export",
                      "required": true
                    },
                    {
                      "id": "export_format",
                      "type": "select",
                      "label": "Format",
                      "options": [
                        {"value": "json", "label": "JSON"},
                        {"value": "csv", "label": "CSV"},
                        {"value": "xml", "label": "XML"},
                        {"value": "parquet", "label": "Parquet"},
                        {"value": "api", "label": "API Webhook"},
                        {"value": "webhook", "label": "Custom Webhook"}
                      ],
                      "required": true
                    },
                    {
                      "id": "selected_agents",
                      "type": "multiselect",
                      "label": "Select Agents",
                      "options": "{{ available_agents_list || [] }}",
                      "required": true
                    },
                    {
                      "id": "export_schedule",
                      "type": "text",
                      "label": "Schedule (Cron)",
                      "placeholder": "0 0 * * 1 (weekly on Monday)"
                    },
                    {
                      "id": "destination_config",
                      "type": "textarea",
                      "label": "Destination Config (JSON)",
                      "placeholder": "{\"url\": \"https://api.example.com/webhooks/data\", \"headers\": {}}",
                      "rows": 3
                    }
                  ],
                  "onSubmit": {
                    "type": "run_query",
                    "query": "create_export_config"
                  }
                }
              }
            ]
          },
          {
            "id": "export_configs_table",
            "type": "containercomponent",
            "configuration": {
              "style": {
                "backgroundColor": "white",
                "border": "1px solid #e5e7eb",
                "borderRadius": "8px",
                "padding": "20px"
              }
            },
            "components": [
              {
                "id": "exports_table_header",
                "type": "containercomponent",
                "configuration": {
                  "style": {
                    "display": "flex",
                    "justifyContent": "space-between",
                    "alignItems": "center",
                    "marginBottom": "15px"
                  }
                },
                "components": [
                  {
                    "id": "exports_table_title",
                    "type": "textcomponent",
                    "configuration": {
                      "text": "**Export Configurations**",
                      "style": {"margin": "0"}
                    }
                  },
                  {
                    "id": "bulk_export",
                    "type": "buttoncomponent",
                    "configuration": {
                      "text": "ðŸ“Š Generate Report",
                      "color": "primary",
                      "onClick": {
                        "type": "run_query",
                        "query": "generate_analytics_report"
                      }
                    }
                  }
                ]
              },
              {
                "id": "exports_table",
                "type": "tablecomponent",
                "configuration": {
                  "data": "={{ export_configurations || [] }}",
                  "columns": [
                    {
                      "field": "name",
                      "header": "Export Name"
                    },
                    {
                      "field": "format",
                      "header": "Format",
                      "type": "badge",
                      "width": "80px",
                      "colorMap": {
                        "json": "info",
                        "csv": "success",
                        "xml": "warning",
                        "parquet": "purple"
                      }
                    },
                    {
                      "field": "agent_count",
                      "header": "Agents",
                      "width": "80px",
                      "render": "{{ row.agent_ids ? row.agent_ids.length : 0 }}"
                    },
                    {
                      "field": "schedule_cron",
                      "header": "Schedule",
                      "width": "120px",
                      "render": "{{ row.schedule_cron || 'Manual' }}"
                    },
                    {
                      "field": "enabled",
                      "header": "Status",
                      "type": "badge",
                      "width": "80px",
                      "render": "{{ row.enabled ? 'Active' : 'Disabled' }}",
                      "colorMap": {
                        "Active": "success",
                        "Disabled": "secondary"
                      }
                    },
                    {
                      "field": "last_export",
                      "header": "Last Export",
                      "type": "datetime",
                      "width": "140px"
                    },
                    {
                      "field": "actions",
                      "header": "Actions",
                      "width": "150px",
                      "render": {
                        "type": "containercomponent",
                        "components": [
                          {
                            "type": "containercomponent",
                            "configuration": {
                              "style": {
                                "display": "flex",
                                "gap": "5px"
                              }
                            },
                            "components": [
                              {
                                "id": "run_export",
                                "type": "buttoncomponent",
                                "configuration": {
                                  "text": "â–¶ï¸",
