{
  "schedule_presets": {
    "daily_ai_progress": {
      "name": "Daily AI Progress Report",
      "description": "Daily briefing on the latest developments in artificial intelligence",
      "category": "technology",
      "frequency": "daily",
      "cron_expression": "0 9 * * *",
      "timezone": "America/New_York",
      "topic_template": "Latest artificial intelligence and machine learning developments - {{DATE}}",
      "depth": "standard",
      "target_length": 3,
      "tags": ["ai", "technology", "daily", "briefing"],
      "search_filters": {
        "time_range": "last_24_hours",
        "exclude_domains": ["blogspot.com", "medium.com"],
        "prefer_domains": ["arxiv.org", "openai.com", "anthropic.com", "deepmind.com", "nature.com"],
        "required_keywords": ["artificial intelligence", "machine learning", "AI research"]
      },
      "custom_prompts": {
        "focus_areas": ["research breakthroughs", "industry applications", "regulatory updates"],
        "tone": "professional but accessible",
        "emphasis": "practical implications and business impact"
      },
      "notification_settings": {
        "email_enabled": true,
        "webhook_enabled": false,
        "summary_format": "executive"
      },
      "template": "technology_trend_analysis",
      "estimated_processing_time": "8-12 minutes"
    },
    "weekly_competitive_analysis": {
      "name": "Weekly Competitive Intelligence",
      "description": "Weekly analysis of competitive landscape and market developments",
      "category": "business",
      "frequency": "weekly",
      "cron_expression": "0 10 * * 1",
      "timezone": "America/New_York",
      "topic_template": "Competitive intelligence analysis for {{INDUSTRY}} sector - Week of {{WEEK_START}}",
      "depth": "deep",
      "target_length": 8,
      "tags": ["competitive", "intelligence", "weekly", "business"],
      "search_filters": {
        "time_range": "last_7_days",
        "focus_companies": ["configurable"],
        "include_financial": true,
        "geographic_focus": ["North America", "Europe"]
      },
      "custom_prompts": {
        "emphasis": ["product launches", "strategic partnerships", "funding rounds", "executive changes"],
        "comparison_framework": "SWOT analysis",
        "competitive_metrics": ["market share", "revenue", "product features", "customer satisfaction"]
      },
      "notification_settings": {
        "email_enabled": true,
        "webhook_enabled": true,
        "summary_format": "strategic"
      },
      "template": "competitive_intelligence",
      "estimated_processing_time": "15-25 minutes"
    },
