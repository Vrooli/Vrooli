{
  "ai_features": {
    "text_generation": {
      "enabled": true,
      "models": ["llama3.1:8b", "qwen2.5-coder:7b"],
      "max_tokens": 2048,
      "temperature_range": [0.1, 1.0]
    },
    
    "speech_to_text": {
      "enabled": true,
      "supported_formats": ["wav", "mp3", "ogg", "m4a", "flac"],
      "max_audio_duration_seconds": 10800
    },
    
    "image_generation": {
      "enabled": false,
      "supported_formats": [],
      "max_resolution": null
    },
    
    "document_processing": {
      "enabled": false,
      "supported_formats": [],
      "max_file_size_mb": 25
    },
    
    "vision_analysis": {
      "enabled": false,
      "supported_formats": ["jpg", "jpeg", "png", "gif", "webp"],
      "max_image_size_mb": 10
    }
  },
  
  "automation_features": {
    "workflow_automation": {
      "enabled": true,
      "max_concurrent_workflows": 5,
      "webhook_support": true,
      "scheduling_support": true
    },
    
    "screen_automation": {
      "enabled": false,
      "screenshot_support": true,
      "mouse_automation": true,
      "keyboard_automation": true,
      "requires_display": false
    },
    
    "web_automation": {
      "enabled": false,
      "pdf_generation": true,
      "screenshot_capture": true,
      "scraping_support": true
    },
    
    "professional_ui": {
      "enabled": true,
      "custom_branding": true,
      "dashboard_creation": true,
      "script_execution": true
    },
    
    "real_time_dashboard": {
      "enabled": false,
      "live_monitoring": true,
      "event_processing": true,
      "websocket_support": true
    }
  },
  
  "storage_features": {
    "file_storage": {
      "enabled": true,
      "public_access": false,
      "versioning": true,
      "encryption": false
    },
    
    "vector_search": {
      "enabled": true,
      "embedding_dimensions": 384,
      "similarity_threshold": 0.7,
      "max_results": 100
    },
    
    "time_series_analytics": {
      "enabled": false,
      "real_time_ingestion": true,
      "historical_analysis": true,
      "performance_monitoring": true
    },
    
    "caching": {
      "enabled": false,
      "session_storage": true,
      "response_caching": true,
      "pub_sub_messaging": true
    },
    
    "secret_management": {
      "enabled": false,
      "dynamic_secrets": true,
      "rotation_support": true,
      "audit_logging": true
    }
  },
  
  "business_features": {
    "analytics_dashboard": {
      "enabled": true,
      "usage_metrics": true,
      "performance_metrics": true,
      "cost_tracking": true,
      "export_reports": true
    },
    
    "user_management": {
      "enabled": false,
      "multi_tenancy": false,
      "role_based_access": false,
      "session_management": true
    },
    
    "api_access": {
      "enabled": true,
      "rate_limiting": true,
      "api_key_auth": false,
      "webhook_support": true
    },
    
    "monitoring_alerts": {
      "enabled": true,
      "error_notifications": true,
      "performance_alerts": true,
      "resource_health_alerts": true
    }
  },
  
  "search_features": {
    "web_search": {
      "enabled": false,
      "privacy_focused": true,
      "multiple_engines": true,
      "result_filtering": true
    },
    
    "semantic_search": {
      "enabled": true,
      "auto_embedding": true,
      "similarity_ranking": true,
      "context_aware": true
    }
  },
  
  "integration_features": {
    "external_apis": {
      "enabled": true,
      "timeout_handling": true,
      "retry_logic": true,
      "response_caching": true
    },
    
    "webhooks": {
      "enabled": true,
      "authentication": false,
      "payload_validation": true,
      "delivery_confirmation": true
