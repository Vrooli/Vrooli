    "import_export": {
      "enabled": true,
      "data_formats": ["json", "csv", "yaml", "txt", "srt"],
      "bulk_operations": true,
      "backup_restore": true
    }
  },
  
  "development_features": {
    "debug_mode": {
      "enabled": true,
      "verbose_logging": true,
      "performance_profiling": true,
      "mock_services": false
    },
    
    "testing_utilities": {
      "enabled": true,
      "sample_data": true,
      "load_testing": false,
      "integration_tests": true
    },
    
    "hot_reload": {
      "enabled": true,
      "auto_restart": true,
      "config_watching": true,
      "asset_reloading": true
    }
