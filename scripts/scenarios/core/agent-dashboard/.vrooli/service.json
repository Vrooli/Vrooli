{
  "name": "agent-dashboard",
  "version": "1.0.0",
  "description": "Dashboard for managing and monitoring all active agents",
  "category": "vrooli-helpers",
  "resources": {
    "n8n": {
      "enabled": true,
      "workflows": [
        "agent-status-monitor",
        "agent-health-checker",
        "agent-orchestrator",
        "multi-agent-coordinator",
        "agent-realtime-monitor"
      ]
    },
    "postgres": {
      "enabled": true,
      "schema": "schema.sql"
    },
    "redis": {
      "enabled": true
    },
    "ollama": {
      "enabled": true,
      "models": ["llama3.2:3b"]
    }
  },
  "ports": {
    "ui": {
      "range": [9000, 9099]
    },
    "api": {
      "range": [4000, 4099]
    }
  },
  "lifecycle": {
    "start": ["ui", "api"],
    "health_check": {
      "enabled": true,
      "interval": 15
    }
  },
  "dependencies": {
    "shared_workflows": [
      "ollama",
      "rate-limiter",
      "chain-of-thought-orchestrator"
    ]
  }
}
