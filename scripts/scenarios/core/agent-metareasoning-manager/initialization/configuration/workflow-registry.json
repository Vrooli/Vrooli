{
  "workflow_registry": {
    "n8n_workflows": [
      {
        "id": "pros-cons-analyzer",
        "name": "Pros and Cons Analyzer",
        "pattern": "pros_cons_analysis",
        "description": "Weighted analysis of advantages vs disadvantages with scoring",
        "webhook_path": "/webhook/analyze-pros-cons",
        "input_schema": {
          "required": ["input"],
          "optional": ["model", "context"],
          "properties": {
            "input": {
              "type": "string",
              "description": "The decision, proposal, or scenario to analyze"
            },
            "model": {
              "type": "string", 
              "default": "llama3.2",
              "enum": ["llama3.2", "mistral", "codellama"],
              "description": "AI model to use for analysis"
            },
            "context": {
              "type": "string",
              "description": "Additional context or constraints"
            }
          }
        },
        "output_schema": {
          "properties": {
            "analysis": {
              "type": "object",
              "properties": {
                "pros": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "item": {"type": "string"},
                      "weight": {"type": "integer", "minimum": 1, "maximum": 10},
                      "explanation": {"type": "string"}
                    }
                  }
                },
                "cons": {
                  "type": "array", 
                  "items": {
                    "type": "object",
                    "properties": {
                      "item": {"type": "string"},
                      "weight": {"type": "integer", "minimum": 1, "maximum": 10},
                      "explanation": {"type": "string"}
                    }
                  }
                },
                "recommendation": {"type": "string"},
                "confidence": {"type": "number", "minimum": 0, "maximum": 100}
              }
            },
            "scores": {
              "type": "object",
              "properties": {
                "pros_total": {"type": "number"},
                "cons_total": {"type": "number"},
                "net_score": {"type": "number"},
                "recommendation_strength": {"type": "number"}
              }
            }
          }
        },
        "execution_metrics": {
          "average_duration_ms": 8000,
          "success_rate": 0.95,
          "usage_count": 0,
          "last_updated": "2025-01-01T00:00:00Z"
        },
        "integration_points": ["database_logging", "windmill_dashboard"]
      },
      {
        "id": "swot-analysis",
