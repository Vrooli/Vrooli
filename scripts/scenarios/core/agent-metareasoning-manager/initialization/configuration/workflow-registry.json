            },
            "context": {
              "type": "string",
              "default": "General business analysis",
              "description": "Business or strategic context"
            },
            "model": {
              "type": "string",
              "default": "llama3.2", 
              "enum": ["llama3.2", "mistral", "codellama"]
            }
          }
        },
        "output_schema": {
          "properties": {
            "swot_analysis": {
              "type": "object",
              "properties": {
                "strengths": {"type": "array"},
                "weaknesses": {"type": "array"},
                "opportunities": {"type": "array"},
                "threats": {"type": "array"}
              }
            },
            "strategic_position": {"type": "string"},
            "metrics": {
              "type": "object",
              "properties": {
                "total_strengths": {"type": "integer"},
                "total_weaknesses": {"type": "integer"},
                "total_opportunities": {"type": "integer"},
                "total_threats": {"type": "integer"},
                "internal_balance": {"type": "integer"},
                "external_balance": {"type": "integer"}
              }
            }
          }
        },
        "execution_metrics": {
          "average_duration_ms": 15000,
          "success_rate": 0.92,
          "usage_count": 0,
          "last_updated": "2025-01-01T00:00:00Z"
        },
        "integration_points": ["database_logging", "windmill_dashboard", "strategic_recommendations"]
      },
      {
        "id": "risk-assessment", 
        "name": "Risk Assessment Workflow",
        "pattern": "risk_assessment",
        "description": "Comprehensive risk analysis with probability√óimpact scoring and mitigation strategies",
        "webhook_path": "/webhook/risk-assessment",
        "input_schema": {
          "required": ["action"],
          "optional": ["constraints", "model"],
          "properties": {
            "action": {
              "type": "string",
              "description": "The action, decision, or project to assess for risks"
            },
            "constraints": {
              "type": "string", 
              "default": "Standard business environment",
              "description": "Operational constraints and context"
            },
            "model": {
              "type": "string",
              "default": "llama3.2",
              "enum": ["llama3.2", "mistral", "codellama"]
            }
          }
        },
        "output_schema": {
          "properties": {
            "risk_assessment": {
              "type": "object",
              "properties": {
                "risks": {
                  "type": "array",
                  "items": {
                    "type": "object", 
                    "properties": {
                      "description": {"type": "string"},
                      "probability": {"type": "string", "enum": ["Very Low", "Low", "Medium", "High", "Very High"]},
                      "impact": {"type": "string", "enum": ["Minimal", "Minor", "Moderate", "Major", "Critical"]},
                      "category": {"type": "string"},
                      "risk_score": {"type": "integer"},
                      "risk_level": {"type": "string"},
                      "mitigation_strategies": {"type": "array"},
                      "warning_indicators": {"type": "array"}
                    }
                  }
                }
              }
            },
            "risk_metrics": {
              "type": "object",
              "properties": {
                "total_risks": {"type": "integer"},
                "critical_risks": {"type": "integer"},
                "high_risks": {"type": "integer"},
                "average_risk_score": {"type": "number"},
                "overall_risk_posture": {"type": "string", "enum": ["Low", "Medium", "High", "Critical"]}
              }
            }
          }
        },
        "execution_metrics": {
          "average_duration_ms": 18000,
          "success_rate": 0.88,
          "usage_count": 0,
          "last_updated": "2025-01-01T00:00:00Z"
        },
        "integration_points": ["database_logging", "windmill_dashboard", "mitigation_planning"]
      },
      {
        "id": "self-review",
        "name": "Self-Review Loop Workflow", 
        "pattern": "introspection",
        "description": "Recursive self-analysis and reasoning validation with confidence iteration",
        "webhook_path": "/webhook/self-review",
        "input_schema": {
          "required": ["decision"],
          "optional": ["model"],
          "properties": {
            "decision": {
              "type": "string",
              "description": "The decision, conclusion, or reasoning to self-review"
            },
            "model": {
              "type": "string",
              "default": "codellama",
              "enum": ["llama3.2", "mistral", "codellama"]
            }
          }
        },
        "output_schema": {
          "properties": {
            "review_process": {
              "type": "object",
              "properties": {
                "initial_review": {"type": "object"},
                "initial_confidence": {"type": "number"},
                "deeper_review_needed": {"type": "boolean"},
                "deeper_review": {"type": "object"},
                "final_assessment": {"type": "object"}
              }
            },
            "learning_outcomes": {
              "type": "object",
              "properties": {
                "confidence_evolution": {
                  "type": "object",
                  "properties": {
                    "initial": {"type": "number"},
                    "final": {"type": "number"}
                  }
                },
                "key_insights": {"type": "array"},
                "areas_improved": {"type": "array"},
                "iteration_count": {"type": "integer"}
              }
            }
          }
        },
        "execution_metrics": {
          "average_duration_ms": 25000,
          "success_rate": 0.90,
          "usage_count": 0,
          "last_updated": "2025-01-01T00:00:00Z"
        },
        "integration_points": ["database_logging", "learning_analytics", "confidence_tracking"]
      },
      {
        "id": "reasoning-chain",
        "name": "Reasoning Chain Orchestrator",
        "pattern": "multi_step_reasoning",
        "description": "Orchestrates multiple reasoning patterns in sequence with synthesis",
        "webhook_path": "/webhook/reasoning-chain",
        "input_schema": {
          "required": ["input"],
          "optional": ["chain_type", "model", "custom_chain"],
          "properties": {
            "input": {
              "type": "string",
              "description": "The problem or decision to analyze through the reasoning chain"
            },
            "chain_type": {
              "type": "string",
              "default": "comprehensive",
              "enum": ["comprehensive", "strategic", "critical", "creative"],
              "description": "Type of predefined reasoning chain to execute"
            },
            "model": {
              "type": "string",
              "default": "llama3.2",
              "enum": ["llama3.2", "mistral", "codellama"]
            },
            "custom_chain": {
              "type": "array",
              "description": "Custom sequence of reasoning steps (overrides chain_type)",
              "items": {
                "type": "object",
                "properties": {
                  "step": {"type": "string"},
                  "webhook": {"type": "string"},
                  "description": {"type": "string"}
                }
              }
            }
          }
        },
        "output_schema": {
          "properties": {
            "chain_execution": {
              "type": "object",
              "properties": {
                "input": {"type": "string"},
                "chain_type": {"type": "string"},
                "steps": {"type": "array"},
                "results": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "step": {"type": "string"},
                      "webhook": {"type": "string"},
                      "description": {"type": "string"},
                      "summary": {"type": "object"},
                      "full_result": {"type": "object"},
                      "executed_at": {"type": "string"}
                    }
                  }
                },
                "status": {"type": "string"},
                "started_at": {"type": "string"},
                "completed_at": {"type": "string"}
              }
            },
            "synthesis": {
              "type": "object",
              "description": "Final synthesis of all reasoning steps"
            }
          }
        },
        "execution_metrics": {
          "average_duration_ms": 45000,
          "success_rate": 0.85,
          "usage_count": 0,
          "last_updated": "2025-01-01T00:00:00Z"
        },
        "integration_points": ["database_logging", "windmill_dashboard", "comprehensive_analysis", "workflow_orchestration"]
      }
    ],
