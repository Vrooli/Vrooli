{
  "name": "agent-metareasoning-manager-api",
  "version": "1.0.0",
  "description": "API server for Agent Metareasoning Manager",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "npm run build && nodemon dist/server.js",
    "dev:watch": "tsc --watch & nodemon dist/server.js",
    "test": "jest --detectOpenHandles",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "pg": "^8.11.3",
    "redis": "^4.6.8",
    "reflect-metadata": "^0.1.14",
    "tsyringe": "^4.10.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.3",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "metareasoning",
    "api",
    "decision-support",
    "ai"
  ],
  "license": "GPL-3.0"
}
