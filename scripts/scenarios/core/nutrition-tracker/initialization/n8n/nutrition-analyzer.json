{
  "name": "nutrition-analyzer",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "nutrition-analyzer",
        "responseMode": "responseNode",
        "options": {}
      },
      "id": "webhook-trigger",
      "name": "Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [250, 300]
    },
    {
      "parameters": {},
      "id": "manual-trigger",
      "name": "Manual",
      "type": "n8n-nodes-base.manualTrigger",
      "typeVersion": 1,
      "position": [250, 100]
    },
    {
      "parameters": {
        "values": {
          "string": [
            {
              "name": "food_description",
              "value": "1 medium apple with 2 tablespoons peanut butter"
            },
            {
              "name": "meal_type",
              "value": "snack"
            },
            {
              "name": "user_id",
              "value": "demo-user-123"
            }
          ]
        },
        "options": {}
      },
      "id": "default-values",
      "name": "Default Values",
      "type": "n8n-nodes-base.set",
      "typeVersion": 3,
      "position": [450, 100]
    },
    {
      "parameters": {
        "mode": "combine",
        "mergeByFields": {
          "values": [
            {
              "field1": "food_description",
              "field2": "food_description"
            }
          ]
        },
        "joinMode": "keepEverything",
        "options": {}
      },
      "id": "merge-inputs",
      "name": "Merge",
      "type": "n8n-nodes-base.merge",
      "typeVersion": 2.1,
      "position": [650, 200]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "http://n8n:5678/webhook/ollama",
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "prompt",
              "value": "={{ `Analyze the nutritional content of this food: ${$json.food_description}\n\nProvide the following nutritional information per serving:\n- Total calories\n- Protein (grams)\n- Carbohydrates (grams)\n- Fat (grams)\n- Fiber (grams)\n- Sugar (grams)\n- Sodium (mg)\n- Serving size description\n\nAlso identify:\n- Food category (Fruits, Vegetables, Protein, Grains, Dairy, Fats, etc.)\n- Is it vegan/vegetarian/keto-friendly?\n- Any notable vitamins or minerals\n\nReturn as JSON with keys: calories, protein, carbs, fat, fiber, sugar, sodium, serving_size, food_category, dietary_tags, nutrients` }}"
            },
            {
              "name": "model",
              "value": "llama3.2:3b"
            },
            {
              "name": "format",
              "value": "json"
            },
            {
              "name": "temperature",
              "value": "0.3"
            }
          ]
        },
        "options": {}
      },
      "id": "analyze-nutrition",
      "name": "Analyze Nutrition",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4,
      "position": [850, 200]
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "=INSERT INTO meal_items (\n  meal_id,\n  custom_food_name,\n  quantity,\n  unit,\n  calories,\n  protein,\n  carbs,\n  fat,\n  fiber,\n  sugar,\n  sodium\n) VALUES (\n  gen_random_uuid(),\n  '{{ $json.food_description }}',\n  1,\n  '{{ $json.serving_size }}',\n  {{ $json.calories }},\n  {{ $json.protein }},\n  {{ $json.carbs }},\n  {{ $json.fat }},\n  {{ $json.fiber }},\n  {{ $json.sugar }},\n  {{ $json.sodium }}\n) RETURNING *;",
        "options": {}
      },
      "id": "save-to-database",
      "name": "Save Analysis",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2,
      "position": [1050, 200],
      "credentials": {
        "postgres": {
          "id": "nutrition-tracker-postgres",
          "name": "Nutrition Tracker DB"
        }
      }
    },
    {
      "parameters": {
        "values": {
          "string": [
            {
              "name": "status",
              "value": "success"
            },
            {
              "name": "message",
              "value": "={{ `Nutritional analysis complete for: ${$json.food_description}` }}"
            }
          ],
          "number": [
            {
              "name": "total_calories",
              "value": "={{ $json.calories }}"
            },
            {
              "name": "protein_grams",
              "value": "={{ $json.protein }}"
            },
            {
              "name": "carbs_grams",
              "value": "={{ $json.carbs }}"
            },
            {
              "name": "fat_grams",
              "value": "={{ $json.fat }}"
            }
          ]
        },
        "options": {}
      },
      "id": "format-response",
      "name": "Format Response",
      "type": "n8n-nodes-base.set",
      "typeVersion": 3,
      "position": [1250, 200]
    },
    {
      "parameters": {
        "respondWith": "json",
        "responseBody": "={{ $json }}",
        "options": {}
      },
      "id": "respond-to-webhook",
      "name": "Respond",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1,
      "position": [1450, 300]
    }
  ],
  "connections": {
    "webhook-trigger": {
      "main": [
        [
          {
            "node": "merge-inputs",
            "type": "main",
            "index": 1
          }
        ]
      ]
    },
    "manual-trigger": {
      "main": [
        [
          {
            "node": "default-values",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "default-values": {
      "main": [
        [
          {
            "node": "merge-inputs",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "merge-inputs": {
      "main": [
        [
          {
            "node": "analyze-nutrition",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "analyze-nutrition": {
      "main": [
        [
          {
            "node": "save-to-database",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "save-to-database": {
      "main": [
        [
          {
            "node": "format-response",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "format-response": {
      "main": [
        [
          {
            "node": "respond-to-webhook",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "active": true,
  "settings": {
    "executionOrder": "v1"
  },
  "tags": [],
  "id": "nutrition-analyzer"
}