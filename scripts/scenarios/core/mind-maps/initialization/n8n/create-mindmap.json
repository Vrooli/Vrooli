{
  "name": "mind-maps-create",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "mind-maps/create",
        "responseMode": "onReceived",
        "options": {}
      },
      "id": "webhook-trigger",
      "name": "Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1.1,
      "position": [250, 300]
    },
    {
      "parameters": {},
      "id": "manual-trigger",
      "name": "Manual Trigger",
      "type": "n8n-nodes-base.manualTrigger",
      "typeVersion": 1,
      "position": [250, 500]
    },
    {
      "parameters": {
        "mode": "raw",
        "jsonOutput": "={{ $json }}",
        "options": {}
      },
      "id": "provide-defaults",
      "name": "Provide Defaults",
      "type": "n8n-nodes-base.set",
      "typeVersion": 3.3,
      "position": [450, 500]
    },
    {
      "parameters": {
        "jsCode": "// Merge webhook or manual input\nconst input = $input.all()[0].json;\n\n// Extract and validate parameters\nconst title = input.title || 'Untitled Mind Map';\nconst description = input.description || '';\nconst userId = input.userId || 'anonymous';\nconst initialNode = input.initialNode || {\n  text: title,\n  type: 'root',\n  x: 0,\n  y: 0\n};\nconst tags = input.tags || [];\nconst isPublic = input.isPublic !== undefined ? input.isPublic : false;\n\n// Generate unique ID\nconst mapId = `map_${Date.now()}_${Math.random().toString(36).substr(2, 9)}`;\n\nreturn {\n  mapId,\n  title,\n  description,\n  userId,\n  initialNode,\n  tags,\n  isPublic,\n  createdAt: new Date().toISOString()\n};"
      },
      "id": "merge-inputs",
      "name": "Merge Inputs",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [650, 400]
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "INSERT INTO mind_maps (id, title, description, user_id, data, tags, is_public, created_at, updated_at)\nVALUES ($1, $2, $3, $4, $5, $6, $7, $8, $8)\nRETURNING *;",
        "options": {
          "queryParams": "={{ $json.mapId }}, {{ $json.title }}, {{ $json.description }}, {{ $json.userId }}, {{ JSON.stringify({ nodes: [$json.initialNode], edges: [] }) }}, {{ $json.tags }}, {{ $json.isPublic }}, {{ $json.createdAt }}"
        }
      },
      "id": "save-to-postgres",
      "name": "Save to PostgreSQL",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2.4,
      "position": [850, 400],
      "credentials": {
        "postgres": {
          "id": "postgres-default",
          "name": "PostgreSQL"
        }
      }
    },
    {
      "parameters": {
        "url": "http://localhost:5678/webhook/embed",
        "authentication": "none",
        "method": "POST",
        "body": {
          "text": "={{ $json.title + ' ' + $json.description + ' ' + $json.tags.join(' ') }}",
          "model": "nomic-embed-text"
        },
        "options": {}
      },
      "id": "generate-embedding",
      "name": "Generate Embedding",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [1050, 400]
    },
    {
      "parameters": {
        "operation": "upsert",
        "collection": "mind_maps",
        "points": [
          {
            "id": "={{ $('save-to-postgres').item.json.id }}",
            "payload": {
              "mapId": "={{ $('save-to-postgres').item.json.id }}",
              "title": "={{ $('save-to-postgres').item.json.title }}",
              "userId": "={{ $('save-to-postgres').item.json.user_id }}",
              "tags": "={{ $('save-to-postgres').item.json.tags }}",
              "createdAt": "={{ $('save-to-postgres').item.json.created_at }}"
            },
            "vector": "={{ $json.embedding }}"
          }
        ]
      },
      "id": "store-in-qdrant",
      "name": "Store in Qdrant",
      "type": "n8n-nodes-base.qdrant",
      "typeVersion": 1,
      "position": [1250, 400],
      "credentials": {
        "qdrantApi": {
          "id": "qdrant-default",
          "name": "Qdrant"
        }
      }
    },
    {
      "parameters": {
        "mode": "raw",
        "jsonOutput": "={\n  \"success\": true,\n  \"mapId\": \"{{ $('save-to-postgres').item.json.id }}\",\n  \"title\": \"{{ $('save-to-postgres').item.json.title }}\",\n  \"message\": \"Mind map created successfully\",\n  \"data\": {{ JSON.stringify($('save-to-postgres').item.json) }}\n}",
        "options": {}
      },
      "id": "format-response",
      "name": "Format Response",
      "type": "n8n-nodes-base.set",
      "typeVersion": 3.3,
      "position": [1450, 400]
    },
    {
      "parameters": {
        "respondWith": "json",
        "responseBody": "={{ $json }}",
        "options": {}
      },
      "id": "respond",
      "name": "Respond to Webhook",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1,
      "position": [1650, 300]
    }
  ],
  "connections": {
    "webhook-trigger": {
      "main": [
        [
