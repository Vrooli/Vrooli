        "additionalFields": {
          "queryParams": "={{ [\n  $json.params.id,\n  $json.body.title || null,\n  $json.body.description || null,\n  $json.body.campaign_id || null,\n  $json.body.metadata ? JSON.stringify($json.body.metadata) : null\n] }}"
        }
      },
