        "operation": "upsert",
        "collection": "mind_map_nodes",
        "points": "={{ [{\n  id: $('Insert Node').item.json.id,\n  vector: $json.embedding,\n  payload: {\n    node_id: $('Insert Node').item.json.id,\n    mind_map_id: $('Insert Node').item.json.mind_map_id,\n    content: $('Insert Node').item.json.content,\n    type: $('Insert Node').item.json.type,\n    parent_id: $('Insert Node').item.json.parent_id,\n    metadata: $('Insert Node').item.json.metadata\n  }\n}] }}",
        "options": {}
