{
  "$schema": "../../../../.vrooli/schemas/service.schema.json",
  "version": "1.0.0",
  "service": {
    "name": "multi-resource-pipeline",
    "displayName": "Multi-Resource Data Pipeline",
    "description": "Enterprise data pipeline handling multiple formats with AI processing and unified storage",
    "version": "1.0.0",
    "type": "business-application",
    "tags": [
      "data-pipeline",
      "etl",
      "multi-format",
      "enterprise-ready",
      "data-engineering",
      "high-revenue-potential",
      "multi-modal"
    ],
    "maintainers": [
      {
        "name": "Vrooli Team",
        "email": "team@vrooli.com",
        "url": "https://github.com/Vrooli"
      }
    ],
    "repository": {
      "type": "git",
      "url": "https://github.com/Vrooli/Vrooli",
      "directory": "/scripts/scenarios/core/multi-resource-pipeline"
    },
    "license": "GPL-3.0",
    "homepage": "https://github.com/Vrooli/Vrooli",
    "documentation": "./README.md",
    "support": {
      "email": "support@vrooli.com",
      "issues": "https://github.com/Vrooli/Vrooli/issues",
      "chat": "https://discord.gg/vrooli"
    }
  },
  "resources": {
    "storage": {
      "postgres": {
        "type": "postgresql",
        "enabled": true,
        "version": "16",
        "required": true,
        "initialization": {
          "data": [
            {
              "type": "schema",
              "file": "initialization/storage/schema.sql"
            },
            {
              "type": "seed",
              "file": "initialization/storage/seed.sql"
            }
          ]
        }
      },
      "minio": {
        "type": "minio",
        "enabled": true,
        "required": true,
        "initialization": {
          "buckets": [
            {
              "name": "raw-data",
              "policy": "private"
            },
            {
              "name": "processed-data",
              "policy": "private"
            },
            {
              "name": "pipeline-artifacts",
              "policy": "private"
            },
            {
              "name": "backup-storage",
              "policy": "private"
            }
          ]
        }
      },
      "qdrant": {
        "type": "qdrant",
        "enabled": true,
        "required": true,
        "initialization": {
          "collections": [
            {
              "name": "document-embeddings",
              "config": {
                "vectors": {
                  "size": 384,
                  "distance": "Cosine"
                }
              }
            },
            {
              "name": "pipeline-metadata",
              "config": {
                "vectors": {
                  "size": 384,
                  "distance": "Cosine"
                }
              }
            },
            {
              "name": "content-similarity",
              "config": {
                "vectors": {
                  "size": 384,
                  "distance": "Cosine"
                }
              }
            }
          ]
        }
      }
    },
    "ai": {
      "unstructured-io": {
        "enabled": true,
        "required": true,
        "type": "unstructured-io"
      },
      "ollama": {
        "enabled": true,
        "required": true,
        "models": ["llama3.1:8b", "nomic-embed-text", "llava:13b"]
      },
      "whisper": {
        "enabled": true,
        "required": true
      }
    },
    "automation": {
      "comfyui": {
        "enabled": false,
        "required": false,
        "initialization": {
          "workflows": [
            {
              "name": "visual-data-processing",
              "file": "initialization/workflows/comfyui/visual-processing.json",
              "enabled": false,
              "tags": ["visual", "processing"]
            }
          ]
        }
      }
    }
  },
  "deployment": {
    "target": "docker",
    "strategy": "rolling-update",
    "urls": {
      "application": "http://localhost:3000",
      "api": "http://localhost:3000/api/multi-resource-pipeline",
      "health": "http://localhost:3000/api/multi-resource-pipeline/health",
      "minio": "http://localhost:9000",
      "qdrant": "http://localhost:6333",
      "unstructured": "http://localhost:8000"
    },
    "initialization": {
      "phases": [
        {
          "name": "database-setup",
          "tasks": [
            {
              "name": "apply-schema",
              "type": "sql",
              "target": "storage.postgres",
              "file": "initialization/storage/schema.sql"
            },
            {
              "name": "seed-data",
              "type": "sql",
              "target": "storage.postgres",
              "file": "initialization/storage/seed.sql"
            }
          ]
        },
        {
          "name": "storage-setup",
          "parallel": true,
          "tasks": [
            {
              "name": "setup-minio-buckets",
              "type": "resource-inject",
              "target": "storage.minio",
              "timeout": "2m"
            },
            {
              "name": "setup-vector-collections",
              "type": "resource-inject",
              "target": "storage.qdrant",
              "timeout": "2m"
            }
          ]
        },
        {
          "name": "ai-setup",
          "parallel": true,
          "tasks": [
            {
              "name": "load-ai-models",
              "type": "resource-inject",
              "target": "ai.ollama",
              "timeout": "15m"
            },
            {
              "name": "verify-unstructured-io",
              "type": "resource-inject",
              "target": "ai.unstructured-io",
              "timeout": "2m"
            },
            {
              "name": "verify-whisper",
              "type": "resource-inject",
              "target": "ai.whisper",
              "timeout": "2m"
            }
          ]
        },
        {
          "name": "pipeline-configuration",
          "tasks": [
            {
              "name": "load-pipeline-config",
              "type": "config",
              "file": "initialization/configuration/pipeline-config.json"
            },
            {
              "name": "set-processing-flags",
              "type": "config",
              "file": "initialization/configuration/processing-flags.json"
            }
          ]
        },
        {
          "name": "startup",
          "tasks": [
            {
              "name": "start-application",
              "type": "script",
              "script": "./deployment/startup.sh",
              "timeout": "3m"
            }
          ]
        },
        {
          "name": "post-validation",
          "tasks": [
            {
              "name": "health-check",
              "type": "healthcheck",
              "target": "http://localhost:3000/api/multi-resource-pipeline/health",
              "timeout": "30s",
              "retries": 3
            },
            {
              "name": "monitor-startup",
              "type": "script",
              "script": "./deployment/monitor.sh",
              "background": true
            }
          ]
        }
      ]
    },
    "monitoring": {
      "enabled": true,
      "healthChecks": {
        "application": {
          "endpoint": "http://localhost:3000/api/multi-resource-pipeline/health",
          "interval": "30s",
          "timeout": "5s",
          "threshold": 3
        },
        "minio": {
          "endpoint": "http://localhost:9000/minio/health/ready",
          "interval": "60s",
          "timeout": "10s"
        },
        "qdrant": {
          "endpoint": "http://localhost:6333/health",
          "interval": "60s",
          "timeout": "10s"
        },
        "unstructured-io": {
          "endpoint": "http://localhost:8000/general/v0/general",
          "interval": "60s",
          "timeout": "15s"
        },
        "whisper": {
          "endpoint": "http://localhost:8090/health",
          "interval": "60s",
          "timeout": "10s"
        },
        "ollama": {
          "endpoint": "http://localhost:11434/api/tags",
          "interval": "60s",
          "timeout": "15s"
        },
        "database": {
          "type": "tcp",
          "port": 5432,
          "interval": "30s"
        }
      },
      "metrics": {
        "responseTime": {
          "p50": "800ms",
          "p95": "3000ms",
          "p99": "8000ms"
        },
        "throughput": {
          "rps": 8,
          "concurrent": 12
        },
        "errorRate": {
          "threshold": "4%"
        }
      }
    },
    "cleanup": {
      "enabled": true,
      "tasks": [
        {
          "name": "drop-database",
          "type": "sql",
          "target": "storage.postgres",
          "query": "DROP DATABASE IF EXISTS multi_resource_pipeline"
        },
        {
          "name": "clear-vector-collections",
          "type": "resource-cleanup",
          "target": "storage.qdrant"
        },
        {
          "name": "clear-storage-buckets",
          "type": "resource-cleanup",
          "target": "storage.minio"
        }
      ]
    },
    "testing": {
      "enabled": true,
      "timeout": "60m",
      "script": "./test.sh",
      "ui": {
        "required": false,
        "type": "api",
        "url": "http://localhost:3000/api/multi-resource-pipeline"
      },
      "performance": {
        "loadTest": {
          "users": 5,
          "duration": "10m",
          "rampUp": "2m"
        }
      }
    },
    "resources": {
      "requests": {
        "cpu": "1500m",
        "memory": "3Gi"
      },
      "limits": {
        "cpu": "6000m",
        "memory": "12Gi"
      }
    }
  },
  "metadata": {
    "businessModel": {
      "purpose": "commercial",
      "valueProposition": "Unified enterprise data pipeline for multi-format ingestion, AI processing, and intelligent retrieval",
      "targetMarket": "Large enterprises, data engineering teams, and organizations with complex multi-format data processing needs",
      "painPoints": [
        "Complex data formats requiring specialized processing tools and expertise",
        "Lack of unified pipeline for handling documents, audio, images, and structured data",
        "Difficulty in making processed data searchable and accessible across organization"
      ],
      "solution": "Comprehensive data pipeline integrating multiple AI services for unified multi-format data processing and intelligent storage",
      "competitiveAdvantage": "Complete integration of Unstructured.io, Whisper, vision models, and vector storage in single deployable solution",
      "revenuePotential": {
        "initial": "$8,000",
        "recurring": "$20,000",
        "expansion": "$45,000",
        "totalEstimate": "$73,000"
      },
      "marketDemand": {
        "level": "very-high",
        "growth": "35% annually",
        "evidence": "Growing enterprise demand for unified data processing and AI-powered document intelligence solutions"
      },
      "roi": {
        "developmentCost": "$12,000",
        "timeToMarket": "4 weeks",
        "breakEven": "5 months"
      }
    },
    "performance": {
      "targets": {
        "initializationTime": "8m",
        "responseTime": {
          "p50": "800ms",
          "p95": "3000ms",
          "p99": "8000ms"
        },
        "throughput": {
          "minimum": 5,
          "target": 8,
          "peak": 25
        },
        "availability": "99.5%",
        "errorRate": "<4%"
      },
      "resources": {
        "cpu": {
          "minimum": "3.0",
          "target": "6.0",
          "maximum": "12.0"
        },
        "memory": {
          "minimum": "6Gi",
          "target": "12Gi",
          "maximum": "24Gi"
        },
        "storage": {
          "database": "100Gi",
          "vectors": "200Gi",
          "raw-data": "1Ti",
          "processed-data": "500Gi"
        }
      }
    },
    "customization": {
      "variables": {
        "APP_NAME": {
          "type": "string",
          "default": "Multi-Resource Data Pipeline",
          "description": "Application display name"
        },
        "COMPANY_NAME": {
          "type": "string",
          "default": "Your Company",
          "description": "Customer company name"
        },
        "PRIMARY_COLOR": {
          "type": "string",
          "default": "#059669",
          "description": "Brand primary color"
        },
        "DATABASE_NAME": {
          "type": "string",
          "default": "multi_resource_pipeline",
          "description": "Database name"
        },
        "APP_PORT": {
          "type": "number",
          "default": 3000,
          "description": "Application port"
        },
        "MINIO_PORT": {
          "type": "number",
          "default": 9000,
          "description": "MinIO storage port"
        },
        "QDRANT_PORT": {
          "type": "number",
          "default": 6333,
          "description": "Qdrant vector database port"
        }
      },
      "features": {
        "authentication": {
          "enabled": true,
          "provider": "jwt",
          "sso": true
        },
        "notifications": {
          "enabled": true,
          "channels": ["email", "webhook", "slack"]
        },
        "analytics": {
          "enabled": true,
          "provider": "internal"
        },
        "documentProcessing": {
          "enabled": true,
          "provider": "unstructured-io"
        },
        "audioProcessing": {
          "enabled": true,
          "provider": "whisper"
        },
        "vectorSearch": {
          "enabled": true,
          "provider": "qdrant"
        },
        "objectStorage": {
          "enabled": true,
          "provider": "minio"
        }
      },
      "integrations": [
        {
          "name": "Enterprise Data Sources",
          "type": "api",
          "required": false
        },
        {
          "name": "Data Lakes",
          "type": "storage",
          "required": false
        },
        {
          "name": "Business Intelligence Tools",
          "type": "webhook",
          "required": false
        }
      ]
    },
    "category": "ai-data-analysis",
    "complexity": "advanced",
    "marketDemand": "very-high"
  }
}
