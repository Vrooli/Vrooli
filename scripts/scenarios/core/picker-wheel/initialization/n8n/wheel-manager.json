    {
      "parameters": {
        "workflowId": "={{ $env.WORKFLOW_ID_ollama || 'nZwMYTAQAYUATglq' }}",
        "options": {
          "wait": true
        },
        "inputData": {
          "prompt": "Suggest 6-8 creative and diverse options for a picker wheel about '{{ $json.topic }}'. Return ONLY a JSON array of objects with 'label' and optional 'color' (hex) fields. Make labels fun with emojis where appropriate. Example format: [{\"label\": \"Pizza üçï\", \"color\": \"#FF6B6B\"}, {\"label\": \"Sushi üç±\", \"color\": \"#4ECDC4\"}]",
          "model": "llama3.2:3b",
          "type": "reasoning",
          "quiet": true,
          "timeout_seconds": 30
        }
      },
      "id": "call_ollama",
      "name": "Get AI Suggestions",
      "type": "n8n-nodes-base.executeWorkflow",
      "typeVersion": 1,
      "position": [850, 200]
    },
    {
      "parameters": {
        "mode": "runOnceForEachItem",
        "jsCode": "// Parse AI response and format suggestions\nconst aiResponse = $input.item.json;\nlet suggestions = [];\n\ntry {\n  // Extract the response text\n  const responseText = aiResponse.response || '';\n  \n  // Try to find JSON array in the response\n  const jsonMatch = responseText.match(/\\[.*\\]/s);\n  if (jsonMatch) {\n    suggestions = JSON.parse(jsonMatch[0]);\n  }\n  \n  // Ensure each suggestion has required fields\n  suggestions = suggestions.map((item, index) => {\n    // Generate colors if not provided\n    const defaultColors = ['#FF6B6B', '#4ECDC4', '#FFD93D', '#95E1D3', '#F38181', '#AA96DA', '#8785A2', '#FFC7C7'];\n    return {\n      label: item.label || `Option ${index + 1}`,\n      color: item.color || defaultColors[index % defaultColors.length],\n      weight: item.weight || 1\n    };\n  });\n} catch (error) {\n  // Fallback to default suggestions if parsing fails\n  suggestions = [\n    { label: 'Option A ‚≠ê', color: '#FF6B6B', weight: 1 },\n    { label: 'Option B üéØ', color: '#4ECDC4', weight: 1 },\n    { label: 'Option C üé®', color: '#FFD93D', weight: 1 },\n    { label: 'Option D üöÄ', color: '#95E1D3', weight: 1 }\n  ];\n}\n\nreturn {\n  success: true,\n  operation: 'suggest',\n  topic: $('Operation Router').item.json.topic,\n  suggestions: suggestions,\n  suggestion_count: suggestions.length\n};"
      },
      "id": "format_suggestions",
      "name": "Format Suggestions",
      "type": "n8n-nodes-base.code",
      "typeVersion": 1,
      "position": [1050, 200]
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "INSERT INTO wheels (name, description, options, theme, tags, is_public, session_id) VALUES ($1, $2, $3, $4, $5, $6, $7) RETURNING *",
        "options": {
          "queryParams": "={{ [$json.wheel_data.name, $json.wheel_data.description, JSON.stringify($json.wheel_data.options), $json.wheel_data.theme || 'neon', $json.wheel_data.tags || [], $json.wheel_data.is_public || false, $json.session_id] }}"
        }
      },
      "id": "create_wheel",
      "name": "Create Wheel",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2.5,
      "position": [850, 300],
      "credentials": {
        "postgres": {
          "id": "postgres_picker_wheel",
          "name": "Postgres - Picker Wheel"
        }
      }
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "SELECT * FROM wheels WHERE is_public = true OR session_id = $1 ORDER BY created_at DESC LIMIT 50",
        "options": {
          "queryParams": "={{ [$json.session_id || 'public'] }}"
        }
      },
      "id": "list_wheels",
      "name": "List Wheels",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2.5,
      "position": [850, 400],
      "credentials": {
        "postgres": {
          "id": "postgres_picker_wheel",
          "name": "Postgres - Picker Wheel"
        }
      }
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "DELETE FROM wheels WHERE id = $1 AND session_id = $2 RETURNING id",
        "options": {
          "queryParams": "={{ [$json.wheel_data.id, $json.session_id] }}"
        }
      },
      "id": "delete_wheel",
      "name": "Delete Wheel",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2.5,
      "position": [850, 500],
      "credentials": {
        "postgres": {
          "id": "postgres_picker_wheel",
          "name": "Postgres - Picker Wheel"
        }
      }
