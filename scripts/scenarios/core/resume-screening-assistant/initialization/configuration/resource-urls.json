{
  "resources": {
    "unstructured_io": {
      "baseUrl": "http://localhost:11450",
      "healthCheck": "http://localhost:11450/healthcheck",
      "endpoints": {
        "process": "/general/v0/general",
        "partition": "/partition/v0/partition"
      }
    },
    "ollama": {
      "baseUrl": "http://localhost:11434",
      "healthCheck": "http://localhost:11434/api/tags",
      "endpoints": {
        "generate": "/api/generate",
        "chat": "/api/chat",
        "embeddings": "/api/embeddings",
        "models": "/api/tags",
        "pull": "/api/pull"
      }
    },
    "qdrant": {
      "baseUrl": "http://localhost:6333",
      "healthCheck": "http://localhost:6333/collections",
      "endpoints": {
        "collections": "/collections",
        "points": "/collections/{collection}/points",
        "search": "/collections/{collection}/points/search",
        "upsert": "/collections/{collection}/points",
        "delete": "/collections/{collection}/points/delete"
      }
    },
    "browserless": {
      "baseUrl": "http://localhost:3000",
      "healthCheck": "http://localhost:3000/pressure",
      "endpoints": {
        "screenshot": "/screenshot",
        "pdf": "/pdf",
        "scrape": "/scrape",
        "function": "/function"
      },
      "optional": true
    },
    "minio": {
      "baseUrl": "http://localhost:9000",
      "healthCheck": "http://localhost:9000/minio/health/live",
      "endpoints": {
        "upload": "/upload",
        "download": "/download",
        "list": "/list"
      },
      "credentials": {
        "accessKey": "${MINIO_ACCESS_KEY:-minioadmin}",
        "secretKey": "${MINIO_SECRET_KEY:-minioadmin}"
      },
      "optional": true
    },
    "postgres": {
      "baseUrl": "postgresql://localhost:5432",
      "healthCheck": "tcp://localhost:5432",
      "endpoints": {
        "direct": "postgresql://localhost:5432/resume_screening_assistant"
      },
      "database": "resume_screening_assistant",
      "credentials": {
        "username": "${POSTGRES_USER:-postgres}",
        "password": "${POSTGRES_PASSWORD:-password}"
      }
    },
    "n8n": {
      "baseUrl": "http://localhost:5678",
      "healthCheck": "http://localhost:5678/healthz",
      "endpoints": {
        "workflows": "/api/v1/workflows",
        "executions": "/api/v1/executions",
        "webhook": "/webhook"
      },
      "required": true
    },
    "windmill": {
      "baseUrl": "http://localhost:8000",
      "healthCheck": "http://localhost:8000/api/version",
      "endpoints": {
        "scripts": "/api/w/{workspace}/scripts",
        "flows": "/api/w/{workspace}/flows",
        "apps": "/api/w/{workspace}/apps",
        "jobs": "/api/w/{workspace}/jobs"
      },
      "workspace": "resume-screening-assistant",
      "required": true
    }
  },
  "apiGateway": {
    "enabled": false,
    "baseUrl": "http://localhost:8080",
    "prefix": "/api/v1"
  }
}