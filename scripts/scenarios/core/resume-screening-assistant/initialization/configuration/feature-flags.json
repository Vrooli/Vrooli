{
  "features": {
    "resume_parsing": {
      "enabled": true,
      "description": "Enable resume document parsing and text extraction",
      "rollout": 100
    },
    "ai_scoring": {
      "enabled": true,
      "description": "Enable AI-powered candidate scoring and assessment",
      "rollout": 100
    },
    "job_matching": {
      "enabled": true,
      "description": "Enable automatic job-candidate matching",
      "rollout": 100
    },
    "bulk_upload": {
      "enabled": true,
      "description": "Allow bulk resume upload and processing",
      "rollout": 100,
      "maxBatchSize": 50
    },
    "linkedin_enrichment": {
      "enabled": false,
      "description": "Enrich candidate profiles with LinkedIn data",
      "rollout": 0,
      "requiresBrowserless": true
    },
    "automated_reports": {
      "enabled": true,
      "description": "Generate automated assessment reports",
      "rollout": 100
    },
    "api_access": {
      "enabled": true,
      "description": "Enable REST API access for integrations",
      "rollout": 100
    },
    "webhook_notifications": {
      "enabled": true,
      "description": "Send webhook notifications for events",
      "rollout": 100
    },
    "candidate_portal": {
      "enabled": false,
      "description": "Enable candidate self-service portal",
      "rollout": 0,
      "requiresWindmill": true
    },
    "advanced_analytics": {
      "enabled": true,
      "description": "Track and display recruitment analytics",
      "rollout": 100
    },
    "custom_scoring_models": {
      "enabled": false,
      "description": "Allow custom AI scoring model configuration",
      "rollout": 0,
      "experimental": true
    },
    "resume_comparison": {
      "enabled": true,
      "description": "Compare multiple candidates side-by-side",
      "rollout": 100
    },
    "skill_extraction": {
      "enabled": true,
      "description": "Extract and categorize skills from resumes",
      "rollout": 100
    },
    "experience_timeline": {
      "enabled": true,
      "description": "Build career timeline from resume data",
      "rollout": 100
    },
    "duplicate_detection": {
      "enabled": true,
      "description": "Detect duplicate candidate submissions",
      "rollout": 100
    },
    "gdpr_compliance": {
      "enabled": true,
      "description": "Enable GDPR compliance features",
      "rollout": 100,
      "settings": {
        "dataRetention": 90,
        "rightToErasure": true,
        "consentTracking": true
      }
    },
    "multi_language": {
      "enabled": false,
      "description": "Support multiple languages in resume parsing",
      "rollout": 0,
      "supportedLanguages": ["en"],
      "experimental": true
    },
    "video_screening": {
      "enabled": false,
      "description": "Support video resume submissions",
      "rollout": 0,
      "requiresWhisper": true,
      "experimental": true
    },
    "ai_interview_prep": {
      "enabled": false,
      "description": "Generate interview questions based on resume",
      "rollout": 0,
      "experimental": true
    },
    "team_collaboration": {
      "enabled": true,
      "description": "Enable team collaboration features",
      "rollout": 100,
      "features": {
        "comments": true,
        "ratings": true,
        "sharing": true
      }
    }
  },
  "experiments": {
    "new_scoring_algorithm": {
      "enabled": false,
      "percentage": 0,
      "description": "Test new ML-based scoring algorithm"
    },
    "enhanced_parsing": {
      "enabled": false,
      "percentage": 0,
      "description": "Test enhanced resume parsing with OCR"
    },
    "realtime_processing": {
      "enabled": false,
      "percentage": 0,
      "description": "Test real-time resume processing pipeline"
    }
  },
  "rolloutStrategy": {
    "type": "percentage",
    "enableGradualRollout": true,
    "rollbackOnError": true,
    "errorThreshold": 5
  },
  "overrides": {
    "development": {
      "all_features": true,
      "skip_validation": true,
      "verbose_logging": true
    },
    "testing": {
      "bulk_upload.maxBatchSize": 5,
      "gdpr_compliance.settings.dataRetention": 7
    }
  }
}