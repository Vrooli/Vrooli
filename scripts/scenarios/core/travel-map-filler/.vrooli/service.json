{
  "id": "travel-map-filler",
  "name": "Travel Map Filler",
  "description": "Interactive world map for tracking and visualizing travel history with achievements",
  "version": "1.0.0",
  "category": "lifestyle",
  "tags": ["travel", "mapping", "visualization", "achievements"],
  "enabled": true,
  "config": {
    "ports": {
      "api": 8760,
      "ui": 8761
    },
    "resources": {
      "n8n": {
        "enabled": true,
        "workflows": [
          "travel-tracker",
          "travel-search",
          "travel-list"
        ]
      },
      "postgres": {
        "enabled": true,
        "database": "travel_map"
      },
      "qdrant": {
        "enabled": true,
        "collections": ["travel_locations"]
      },
      "ollama": {
        "enabled": false,
        "models": []
      },
      "redis": {
        "enabled": false
      }
    },
    "health_check": {
      "endpoint": "/api/health",
      "interval": 30,
      "timeout": 5
    }
  },
  "ui": {
    "type": "javascript",
    "path": "ui/",
    "port": 8761
  },
  "api": {
    "type": "go",
    "path": "api/",
    "port": 8760
  },
  "initialization": {
    "n8n": {
      "workflows": [
        {
          "path": "initialization/n8n/travel-tracker.json",
          "active": true
        },
        {
          "path": "initialization/n8n/travel-search.json",
          "active": true
        },
        {
          "path": "initialization/n8n/travel-list.json",
          "active": true
        }
      ]
    },
    "postgres": {
      "schemas": [
        {
          "path": "initialization/postgres/schema.sql"
        }
      ]
    },
    "qdrant": {
      "collections": [
        {
          "path": "initialization/qdrant/collections.json"
        }
      ]
    }
  },
  "dependencies": {
    "scenarios": [],
    "resources": ["n8n", "postgres", "qdrant"]
  },
  "cli": {
    "executable": "cli/travel-map",
    "commands": [
      {
        "name": "add",
        "description": "Add a new travel location",
        "usage": "travel-map add <location> [options]"
      },
      {
        "name": "search",
        "description": "Search travel memories",
        "usage": "travel-map search <query>"
      },
      {
        "name": "stats",
        "description": "View travel statistics",
        "usage": "travel-map stats"
      },
      {
        "name": "list",
        "description": "List all travels",
        "usage": "travel-map list [options]"
      }
    ]
  },
  "test": {
    "scenarios": [
      {
        "path": "test/scenario-test.yaml"
      }
    ]
  }
}
