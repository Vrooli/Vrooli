{
  "name": "Simple Notification Workflow",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "/notify",
        "responseMode": "onReceived"
      },
      "id": "webhook-001",
      "name": "Notification Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [200, 300]
    },
    {
      "parameters": {
        "text": "Received notification: {{ $json.message }}",
        "options": {}
      },
      "id": "slack-001",
      "name": "Send to Slack",
      "type": "n8n-nodes-base.slack",
      "typeVersion": 1, 
      "position": [400, 300]
    }
  ],
  "connections": {
    "Notification Webhook": {
      "main": [
        [
          {
            "node": "Send to Slack",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "active": true,
  "id": "simple-notification-test"
}