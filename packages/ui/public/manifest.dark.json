{
    "id": "/vrooli",
    "name": "Vrooli",
    "short_name": "Vrooli",
    "description": "A collaborative and self-improving automation platform",
    "dir": "ltr",
    "lang": "en-US",
    "prefer_related_applications": false,
    "omnibox": {
        "keyword": "vrooli"
    },
    "categories": [
        "ai",
        "automation",
        "productivity"
    ],
    "icons": [
        {
            "src": "/favicon-16x16.png",
            "sizes": "16x16",
            "type": "image/png"
        },
        {
            "src": "/android-chrome-192x192.png",
            "sizes": "192x192",
            "type": "image/png"
        },
        {
            "src": "/android-chrome-512x512.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "any"
        },
        {
            "src": "/logo-mask-512x512.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "maskable"
        }
    ],
    "background_color": "#242930",
    "theme_color": "#242930",
    "display": "standalone",
    "display_override": [
        "window-controls-overlay",
        "standalone",
        "minimal-ui",
        "fullscreen",
        "browser"
    ],
    "orientation": "any",
    "scope": "/",
    "scope_extensions": [
        {
            "origin": "*.<DOMAIN>"
        }
    ],
    "start_url": "/",
    "screenshots": [
        {
            "src": "/screenshots/1-intro-page.png",
            "sizes": "720x1280",
            "platform": "narrow",
            "type": "image/png",
            "label": "About Vrooli"
        },
        {
            "src": "/screenshots/2-build-screenshot.png",
            "sizes": "720x1280",
            "platform": "narrow",
            "type": "image/png",
            "label": "Connecting subroutines together"
        },
        {
            "src": "/screenshots/3-create-routine-screenshot.png",
            "sizes": "720x1280",
            "platform": "narrow",
            "type": "image/png",
            "label": "Defining routine information"
        },
        {
            "src": "/screenshots/4-run-routine-screenshot.png",
            "sizes": "720x1280",
            "platform": "narrow",
            "type": "image/png",
            "label": "Running a routine"
        },
        {
            "src": "/screenshots/5-build-with-community-screenshot.png",
            "sizes": "720x1280",
            "platform": "narrow",
            "type": "image/png",
            "label": "Find routines page"
        }
    ],
    "shortcuts": [
        {
            "name": "Open Dashboard",
            "short_name": "Dashboard",
            "url": "/",
            "description": "View upcoming events, reminders, and notes",
            "icons": [
                {
                    "src": "/icons/shortcut-home-dark-192x192.png",
                    "sizes": "192x192"
                }
            ]
        },
        {
            "name": "Search Site",
            "short_name": "Search",
            "url": "/search",
            "description": "Find a public routine, project, bot, or other item",
            "icons": [
                {
                    "src": "/icons/shortcut-search-dark-192x192.png",
                    "sizes": "192x192"
                }
            ]
        },
        {
            "name": "Create Object",
            "short_name": "Create",
            "url": "/create",
            "description": "Create a routine, note, reminder, project, or any other item",
            "icons": [
                {
                    "src": "/icons/shortcut-create-dark-192x192.png",
                    "sizes": "192x192"
                }
            ]
        },
        {
            "name": "Notifications and Messages",
            "short_name": "Inbox",
            "url": "/inbox",
            "description": "View notifications and messages from bots and other users",
            "icons": [
                {
                    "src": "/icons/shortcut-inbox-dark-192x192.png",
                    "sizes": "192x192"
                }
            ]
        },
        {
            "name": "View My Stuff",
            "short_name": "My Stuff",
            "url": "/my",
            "description": "See all of your routines, projects, and other items",
            "icons": [
                {
                    "src": "/icons/shortcut-my-dark-192x192.png",
                    "sizes": "192x192"
                }
            ]
        }
    ],
    "related_applications": [
        {
            "platform": "webapp",
            "url": "<UI_URL>"
        }
    ],
    "launch_handler": {
        "client_mode": [
            "navigate-existing",
            "auto"
        ]
    },
    "file_handlers": [],
    "protocol_handlers": [],
    "handle_links": "not-preferred",
    "edge_side_panel": {},
    "widgets": []
}