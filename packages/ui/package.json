  "scripts": {
    "pretest": "cd ../shared && pnpm run build:test",
    "test": "vitest run",
    "test-watch": "vitest",
    "test-coverage": "vitest run --coverage",
    "test:integration": "vitest run --config vitest.config.integration.ts",
    "test:integration-watch": "vitest --config vitest.config.integration.ts",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome",
    "build": "rimraf dist && npx vite build && node workbox-build.js && pnpm run post-build",
    "post-build": "pnpm run post-build-copy",
    "post-build-copy": "copyfiles -u 1 \"src/**/*.css\" \"src/**/*.md\" dist",
    "start-development": "npx vite",
    "start-production": "serve --config serve.json",
    "analyze": "source-map-explorer --html \"source-tree-$(date '+%Y%m%d%H%M%S').html\" --no-border-checks dist/assets/*",
    "lint": "eslint src --ignore-pattern '**/__test/**' --ignore-pattern '**/*.test.ts' --ignore-pattern '**/*.test.tsx' --ignore-pattern '**/*.spec.ts' --ignore-pattern '**/*.spec.tsx' --ignore-pattern '**/*.stories.ts' --ignore-pattern '**/*.stories.tsx' --ignore-pattern '**/*.roundtrip.test.*' --ignore-pattern '**/*.scenario.test.*' --ignore-pattern '**/*.integration.test.*' --cache --cache-location .eslintcache --fix",
    "lint:test": "eslint '{src,test}/**/*.{ts,tsx}' '{src,test}/**/*test.{ts,tsx}' '{src,test}/**/*spec.{ts,tsx}' '{src,test}/**/*.stories.{ts,tsx}' 'src/**/__test/**/*' --config .eslintrc.test.cjs --cache --cache-location .eslintcache.test --fix",
    "lint:all": "pnpm run lint && pnpm run lint:test",
    "type-check": "NODE_OPTIONS='--max-old-space-size=3072 --no-deprecation' tsc --noEmit",
    "type-check:test": "NODE_OPTIONS='--max-old-space-size=3072 --no-deprecation' tsc --noEmit -p tsconfig.test-all.json",
    "type-check:all": "pnpm run type-check && pnpm run type-check:test",
    "validate": "pnpm run lint && pnpm run type-check",
    "validate:all": "pnpm run lint:all && pnpm run type-check:all",
    "storybook": "npx storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
