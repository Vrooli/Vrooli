    "scripts": {
        "test": "NODE_OPTIONS='--max-old-space-size=8192' vitest run",
        "test-watch": "vitest",
        "test-coverage": "NODE_OPTIONS='--max-old-space-size=8192' vitest run --coverage",
        "coverage-report": "vitest run --coverage --reporter=html",
        "build": "rimraf dist && vite build && node workbox-build.js && pnpm run post-build",
        "post-build": "pnpm run post-build-copy",
        "post-build-copy": "copyfiles -u 1 \"src/**/*.css\" \"src/**/*.md\" dist",
        "start-development": "vite",
        "start-production": "serve --config serve.json",
        "analyze": "source-map-explorer --html \"source-tree-$(date '+%Y%m%d%H%M%S').html\" --no-border-checks dist/assets/*",
        "lint": "eslint src",
        "type-check": "tsc --noEmit",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not op_mini all"
