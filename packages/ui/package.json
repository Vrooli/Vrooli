    "scripts": {
        "build": "react-scripts build --max-old-space-size=8192 --optimize_for_size --max_executable_size=4096 --stack_size=4096",
        "start-development": "react-scripts start",
        "start-production": "serve -s build -l 3000",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "analyze": "source-map-explorer build/static/js/*",
        "download-schema": "apollo client:download-schema --endpoint=http://localhost:5000/api/v1 src/graphql/generated/schema.graphql",
        "gen-graphql-types": "apollo client:codegen --target typescript --localSchemaFile src/graphql/generated/schema.graphql --outputFlat --includes \"src/graphql/**/*.ts\" --excludes \"src/graphql/generated\" src/graphql/generated"
