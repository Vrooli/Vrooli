{
    "extends": [
        "react-app"
    ],
    "ignorePatterns": [
        "build/",
        "dist/",
        "node_modules/"
    ],
    "rules": {
        "quotes": [
            "error",
            "double"
        ],
        "import/no-unresolved": [
            2,
            {
                "caseSensitive": true
            }
        ]
    }
}