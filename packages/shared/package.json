{
    "name": "@vrooli/shared",
    "author": "Vrooli",
    "license": "AGPL-3.0-only",
    "version": "1.9.11",
    "private": true,
    "description": "Common code shared between server and ui",
    "main": "dist/index.js",
    "typings": "src/types.d.ts",
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./src/index.ts"
        },
        "./ai": {
            "import": "./dist/ai/index.js",
            "types": "./src/ai/index.ts"
        },
        "./api": {
            "import": "./dist/api/index.js",
            "types": "./src/api/index.ts"
        },
        "./consts": {
            "import": "./dist/consts/index.js",
            "types": "./src/consts/index.ts"
        },
        "./execution": {
            "import": "./dist/execution/types/index.js",
            "types": "./src/execution/types/index.ts"
        },
        "./forms": {
            "import": "./dist/forms/index.js",
            "types": "./src/forms/index.ts"
        },
        "./id": {
            "import": "./dist/id/index.js",
            "types": "./src/id/index.ts"
        },
        "./run": {
            "import": "./dist/run/index.js",
            "types": "./src/run/index.ts"
        },
        "./shape": {
            "import": "./dist/shape/index.js",
            "types": "./src/shape/index.ts"
        },
        "./translations": {
            "import": "./dist/translations/index.js",
            "types": "./src/translations/index.ts"
        },
        "./utils": {
            "import": "./dist/utils/index.js",
            "types": "./src/utils/index.ts"
        },
        "./validation": {
            "import": "./dist/validation/index.js",
            "types": "./src/validation/index.ts"
        },
        "./validation/models": {
            "import": "./dist/validation/models/index.js",
            "types": "./src/validation/models/index.ts"
        },
        "./validation/models/fixtures": {
            "import": "./dist/validation/models/__test/fixtures/index.js",
            "types": "./src/validation/models/__test/fixtures/index.ts"
        },
        "./validation/forms": {
            "import": "./dist/validation/forms/index.js",
            "types": "./src/validation/forms/index.ts"
        },
        "./validation/utils": {
            "import": "./dist/validation/utils/index.js",
            "types": "./src/validation/utils/index.ts"
        }
    },
    "scripts": {
        "test": "NODE_OPTIONS='--max-old-space-size=8192' npx vitest run",
        "test-watch": "npx vitest",
        "test-coverage": "NODE_OPTIONS='--max-old-space-size=8192' npx vitest run --coverage",
        "lint": "eslint src",
        "build": "rimraf dist && cross-env NODE_ENV=production swc src -d dist --ignore \"**/*.test.ts,**/*.test.js,**/*.test.tsx,**/*.test.jsx,**/*.test.cjs\" && pnpm run post-build",
        "post-build": "run-s post-build-copy post-build-fix-imports",
        "post-build-copy": "copyfiles -u 1 \"src/**/*.json\" dist",
        "post-build-fix-imports": "sh ../../scripts/fixImports.sh assert dist",
        "type-check": "tsc --noEmit"
    },
    "dependencies": {
        "bpmn-moddle": "^9.0.1",
        "cron-parser": "^5.2.0",
        "i18next": "^22.4.10",
        "iso8601-duration": "^2.1.2",
        "jexl": "^2.3.0",
        "moment": "^2.29.4",
        "moment-timezone": "^0.5.43",
        "nanoid": "^5.1.5",
        "uuid": "^9.0.0",
        "xmlbuilder2": "^3.0.2",
        "yup": "^1.3.3"
    },
    "devDependencies": {
        "@swc-node/register": "^1.10.10",
        "@swc/cli": "^0.1.63",
        "@swc/core": "^1.3.104",
        "@types/bpmn-moddle": "^5.1.11",
        "@types/jexl": "^2.3.4",
        "@types/node": "^18.14.1",
        "@typescript-eslint/eslint-plugin": "^5.59.0",
        "@typescript-eslint/parser": "^5.59.0",
        "@vitest/coverage-v8": "^3.0.9",
        "c8": "^10.1.3",
        "copyfiles": "^2.4.1",
        "cross-env": "^7.0.3",
        "dotenv-cli": "^8.0.0",
        "jsdom": "^26.0.0",
        "npm-run-all": "^4.1.5",
        "rimraf": "^5.0.10",
        "ts-json-schema-generator": "^1.2.0",
        "typescript-json-schema": "^0.65.1",
        "vitest": "~3.0.7"
    },
    "packageManager": "pnpm@10.5.2"
}