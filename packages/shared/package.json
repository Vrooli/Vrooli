{
  "_comment": "AI_CHECK: TYPE_SAFETY=shared-exports-fix | LAST: 2025-06-29 - Fixed TypeScript exports by adding declaration generation",
  "name": "@vrooli/shared",
  "author": "Vrooli",
  "license": "AGPL-3.0-only",
  "version": "2.0.2",
  "private": true,
  "description": "Common code shared between server and ui",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./ai": {
      "import": "./dist/ai/index.js",
      "types": "./dist/ai/index.d.ts"
    },
    "./api": {
      "import": "./dist/api/index.js",
      "types": "./dist/api/index.d.ts"
    },
    "./api/types": {
      "import": "./dist/api/types.js",
      "types": "./dist/api/types.d.ts"
    },
    "./consts": {
      "import": "./dist/consts/index.js",
      "types": "./dist/consts/index.d.ts"
    },
    "./execution": {
      "import": "./dist/execution/types/index.js",
      "types": "./dist/execution/types/index.d.ts"
    },
    "./forms": {
      "import": "./dist/forms/index.js",
      "types": "./dist/forms/index.d.ts"
    },
    "./id": {
      "import": "./dist/id/index.js",
      "types": "./dist/id/index.d.ts"
    },
    "./run": {
      "import": "./dist/run/index.js",
      "types": "./dist/run/index.d.ts"
    },
    "./shape": {
      "import": "./dist/shape/index.js",
      "types": "./dist/shape/index.d.ts"
    },
    "./translations": {
      "import": "./dist/translations/index.js",
      "types": "./dist/translations/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./validation": {
      "import": "./dist/validation/index.js",
      "types": "./dist/validation/index.d.ts"
    },
    "./validation/models": {
      "import": "./dist/validation/models/index.js",
      "types": "./dist/validation/models/index.d.ts"
    },
    "./validation/forms": {
      "import": "./dist/validation/forms/index.js",
      "types": "./dist/validation/forms/index.d.ts"
    },
    "./validation/utils": {
      "import": "./dist/validation/utils/index.js",
      "types": "./dist/validation/utils/index.d.ts"
    },
    "./test-fixtures": {
      "import": "./test-dist/__test/fixtures/index.js",
      "types": "./src/__test/fixtures/index.ts"
    },
    "./test-fixtures/api-inputs": {
      "import": "./test-dist/__test/fixtures/api-inputs/index.js",
      "types": "./src/__test/fixtures/api-inputs/index.ts"
    },
    "./test-fixtures/config": {
      "import": "./test-dist/__test/fixtures/config/index.js",
      "types": "./src/__test/fixtures/config/index.ts"
    }
  },
  "scripts": {
    "test": "bash ../../scripts/package-test.sh shared",
    "test-watch": "npx vitest",
    "test-coverage": "NODE_OPTIONS='--max-old-space-size=8192' npx vitest run --coverage",
    "lint": "eslint src --ignore-pattern '**/__test/**' --ignore-pattern '**/*.test.ts' --ignore-pattern '**/*.test.tsx' --ignore-pattern '**/*.spec.ts' --ignore-pattern '**/*.spec.tsx' --cache --cache-location .eslintcache --fix",
    "lint:test": "eslint '{src,test}/**/*.{ts,tsx}' '{src,test}/**/*test.ts' '{src,test}/**/*test.tsx' '{src,test}/**/*spec.ts' '{src,test}/**/*spec.tsx' 'src/**/__test/**/*' --config .eslintrc.test.cjs --cache --cache-location .eslintcache.test --fix",
    "lint:all": "pnpm run lint && pnpm run lint:test",
    "build": "rimraf dist .tsbuildinfo && cross-env NODE_ENV=production swc src -d dist --ignore \"**/*.test.ts,**/*.test.js,**/*.test.tsx,**/*.test.jsx,**/*.test.cjs,**/__test/**,**/__tools/**\" && tsc --project . --emitDeclarationOnly --outDir dist && pnpm run post-build",
    "build:test": "rimraf test-dist && cross-env NODE_ENV=test swc src -d test-dist && pnpm run post-build:test",
    "post-build": "run-s post-build-copy post-build-fix-imports",
    "post-build:test": "run-s post-build-copy:test post-build-fix-imports:test",
    "post-build-copy": "copyfiles -u 1 \"src/**/*.json\" dist",
    "post-build-copy:test": "copyfiles -u 1 \"src/**/*.json\" test-dist",
    "post-build-fix-imports": "sh ../../scripts/fixImports.sh assert dist",
    "post-build-fix-imports:test": "sh ../../scripts/fixImports.sh assert test-dist",
    "type-check": "NODE_OPTIONS='--max-old-space-size=2048 --no-deprecation' tsc --noEmit",
    "type-check:test": "NODE_OPTIONS='--max-old-space-size=2048 --no-deprecation' tsc --noEmit -p tsconfig.test.json",
    "type-check:all": "pnpm run type-check && pnpm run type-check:test",
    "validate": "pnpm run lint && pnpm run type-check",
    "validate:all": "pnpm run lint:all && pnpm run type-check:all"
  },
  "dependencies": {
    "bpmn-moddle": "^9.0.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.10",
    "cron-parser": "^5.2.0",
    "i18next": "^22.4.10",
    "iso8601-duration": "^2.1.2",
    "jexl": "^2.3.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "nanoid": "^5.1.5",
    "uuid": "^9.0.0",
    "xmlbuilder2": "^3.0.2",
    "yup": "^1.3.3",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.104"
  },
  "devDependencies": {
    "@swc-node/register": "^1.10.10",
    "@types/bpmn-moddle": "^5.1.11",
    "@types/jexl": "^2.3.4",
    "@types/node": "^18.14.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitest/coverage-v8": "^3.0.9",
    "c8": "^10.1.3",
    "dotenv-cli": "^8.0.0",
    "jsdom": "^26.0.0",
    "ts-json-schema-generator": "^1.2.0",
    "typescript-json-schema": "^0.65.1",
    "vitest": "~3.0.7"
  },
  "packageManager": "pnpm@10.5.2"
}
