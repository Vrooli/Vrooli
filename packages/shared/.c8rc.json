{
    "all": true,
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "src/**/*.test.ts",
        "src/__test/**",
        "coverage/**",
        "dist/**"
    ],
    "reporter": [
        "text",
        "html",
        "lcov"
    ],
    "reportDir": "./coverage",
    "sourceMap": true,
    "skipFull": false,
    "clean": true,
    "checkCoverage": true,
    "branches": 70,
    "lines": 80,
    "functions": 80,
    "statements": 80,
    "tempDirectory": "./.nyc_output"
}