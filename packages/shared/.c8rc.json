{
    "all": true,
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "**/node_modules/**",
        "src/**/*.test.ts",
        "src/**/*.test.js",
        "src/**/__test__/**/*",
        "src/__test/**/*",
        "coverage/**",
        "dist/**",
        "**/__test__/**/*"
    ],
    "reporter": [
        "text",
        "html",
        "lcov"
    ],
    "reportDir": "./coverage",
    "sourceMap": true,
    "src": ["src"],
    "skipFull": false,
    "clean": true,
    "checkCoverage": false,
    "branches": 70,
    "lines": 80,
    "functions": 80,
    "statements": 80,
    "tempDirectory": "./.nyc_output"
}