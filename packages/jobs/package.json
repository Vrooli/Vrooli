    "scripts": {
        "build-tests": "swc src -d test-dist",
        "test": "yarn build-tests && dotenv -e ../../.env-test -- mocha \"test-dist/**/*.test.js\"",
        "test-watch": "yarn build-tests && dotenv -e ../../.env-test -- mocha --watch \"test-dist/**/*.test.js\"",
        "lint": "eslint src",
        "build": "swc src -d dist",
        "pre-prod": "echo 'no pre-prod steps required'",
        "pre-develop": "yarn pre-prod",
        "start-development": "NODE_OPTIONS=\"--max-old-space-size=4096\" nodemon dist/index.js",
        "start-production": "node --experimental-modules --experimental-specifier-resolution=node dist/index.js",
        "type-check": "NODE_OPTIONS=\"--max-old-space-size=6144\" tsc --noEmit"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.666.0",
