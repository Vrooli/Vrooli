        "nodemailer": "^6.6.0",
        "nodemon": "3.0.2",
        "openai": "4.103.0",
        "sharp": "^0.32.3",
        "socket.io": "^4.6.2",
        "stopword": "^2.0.8",
        "stripe": "^11.17.0",
        "twilio": "^3.61.0",
        "typescript": "5.4.5",
        "web-push": "^3.5.0",
        "winston": "^3.7.2"
