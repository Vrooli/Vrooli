    "version": "1.9.11",
    "private": true,
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "test": "vitest run",
        "test-watch": "vitest",
        "test-coverage": "vitest run --coverage",
        "coverage-report": "vitest run --coverage --reporter=html",
        "lint": "eslint src",
        "build": "rimraf dist && cross-env NODE_ENV=production swc src -d dist",
        "pre-prod": "echo 'no pre-prod steps required'",
        "pre-develop": "pnpm run pre-prod",
        "start-development": "cross-env NODE_OPTIONS=--max-old-space-size=4096 nodemon dist/index.js",
        "start-production": "node --experimental-modules --experimental-specifier-resolution=node dist/index.js",
        "type-check": "cross-env NODE_OPTIONS=--max-old-space-size=6144 tsc --noEmit"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.666.0",
