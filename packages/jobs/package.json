        "dotenv-cli": "^8.0.0",
        "mocha": "^11.1.0",
        "sinon": "^19.0.2",
        "npm-run-all": "^4.1.5",
        "cross-env": "^7.0.3",
        "rimraf": "^5.0.10",
        "vitest": "~3.0.9"
    },
    "packageManager": "pnpm@10.5.2"
