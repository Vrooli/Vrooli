        "@vrooli/server": "workspace:*",
        "@vrooli/shared": "workspace:*",
        "@prisma/client": "6.1.0",
        "bcrypt": "^5.0.1",
        "bullmq": "^5.52.2",
        "busboy": "^1.6.0",
        "cookie": "^0.7.2",
        "cors": "^2.8.5",
        "express": "^5.0.1",
        "file-type": "^20.0.0",
        "heic-convert": "^1.2.4",
        "i18next": "^22.4.10",
        "jsonwebtoken": "^9.0.0",
        "node-cron": "^3.0.0",
        "node-fetch": "^3.2.10",
