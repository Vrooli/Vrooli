{
  "name": "@vrooli/jobs",
  "author": "Vrooli",
  "license": "AGPL-3.0-only",
  "version": "2.0.0",
  "private": true,
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "pretest": "cd ../shared && pnpm run build:test && cd ../server && pnpm run build",
    "test": "bash ../../scripts/package-test.sh jobs",
    "test-watch": "npx vitest",
    "test-coverage": "NODE_OPTIONS='--max-old-space-size=8192' npx vitest run --coverage",
    "lint": "eslint src --cache --cache-location .eslintcache --fix",
    "build": "rimraf dist && cross-env NODE_ENV=production swc src -d dist --ignore \"**/*.test.ts,**/*.test.js,**/*.test.tsx,**/*.test.jsx,**/*.test.cjs,**/__test/**\"",
    "pre-prod": "echo 'no pre-prod steps required'",
    "pre-develop": "pnpm run pre-prod",
    "start-development": "cross-env NODE_OPTIONS=--max-old-space-size=4096 nodemon dist/index.js",
    "start-production": "node --experimental-modules --experimental-specifier-resolution=node dist/index.js",
    "type-check": "cross-env NODE_OPTIONS='--max-old-space-size=3072 --no-deprecation' tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.666.0",
    "@emurgo/cardano-serialization-lib-nodejs": "^9.1.2",
    "@prisma/client": "6.1.0",
"@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.104",
    "@vrooli/server": "workspace:*",
    "@vrooli/shared": "workspace:*",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.52.2",
    "busboy": "^1.6.0",
    "cookie": "^0.7.2",
    "cors": "^2.8.5",
"cross-env": "^7.0.3",
    "express": "^5.0.1",
    "file-type": "^20.0.0",
    "heic-convert": "^1.2.4",
    "i18next": "^22.4.10",
    "jsonwebtoken": "^9.0.0",
    "node-cron": "^3.0.0",
    "node-fetch": "^3.2.10",
    "node-ical": "^0.15.3",
    "nodemailer": "^6.6.0",
    "nodemon": "3.0.2",
    "openai": "4.103.0",
    "rimraf": "^5.0.10",
    "sharp": "^0.32.3",
    "socket.io": "^4.6.2",
    "stopword": "^2.0.8",
    "stripe": "^11.17.0",
    "twilio": "^3.61.0",
    "typescript": "5.8.3",
    "web-push": "^3.5.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "0x": "^5.5.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/heic-convert": "^1.2.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^17.0.35",
    "@types/node-cron": "^3.0.1",
    "@types/nodemailer": "^6.4.4",
    "@types/stopword": "^2.0.0",
    "@types/web-push": "^3.3.2",
    "@vitest/coverage-v8": "^3.0.9",
    "dotenv-cli": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "testcontainers": "^10.18.0",
    "vitest": "~3.0.9"
  },
  "packageManager": "pnpm@10.5.2"
}
