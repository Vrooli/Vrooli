{
    "exclude": [
        "/.*__mocks__.*/",
        "/.*\\/types\\.ts$/",
        "/.*\\/types\\.d\\.ts$/"
    ],
    "jsc": {
        "minify": {
            "compress": true,
            "mangle": true
        },
        "parser": {
            "syntax": "typescript",
            "tsx": true,
            "decorators": true,
            "dynamicImport": true
        },
        "target": "es2017"
    },
    "module": {
        "type": "es6"
    },
        "sourceMaps": false
}
